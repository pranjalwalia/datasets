    function registerGetBlogUrls() {
      gadgets.rpc.register('getBlogUrls', function() {
        var holder = {};
        
          
            
            
              holder.currentPost = "http://www.blogger.com/feeds/5043546193960902595/posts/default/6714829209354917821";
            
            
            
              holder.currentComments = "http://www.blogger.com/feeds/5043546193960902595/6714829209354917821/comments/default";
            
            holder.currentPostUrl = "";
            holder.currentPostId = 6714829209354917821
          
          
          
