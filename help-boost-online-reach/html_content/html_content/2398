<script type="text/javascript">
require (['util/googleAnalytics', 'util/model'], function(GA, model) {
  GA.setSegmentation({});
  model.onRefresh('user', function (user) {
    if (user && user.userName) {
      GA.trackCustomVariable('logged_in', 'true', 3);
    } else {
      GA.trackCustomVariable('logged_in', 'false', 3);
    }
    if (typeof(console) === 'object') {
      console.log("Not suppressing page view in GA.");
    }
    GA.trackPageview();
    GA.trackPageLoadTime();
  });
});
(function() {
  var ga = document.createElement('script');     ga.type = 'text/javascript'; ga.async = true;
  ga.src = ('https:'   == document.location.protocol ? 'https://ssl'   : 'http://www') + '.google-analytics.com/ga.js';
  var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
})();
</script>

