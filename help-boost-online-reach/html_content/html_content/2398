  <span id="updateServings"></span>
  <script type="text/javascript">
    require(['underscore', 'jquery', 'jquery.ninja.ui'], function(_u, $, _ninja) {
      require.ready(function () {
        $(function ($) {
          $b = $.ninja().button({
            html: 'Change'
          }).select(function () {
            var ns = $('#servings').val();
            var n = ns ? parseInt(ns) : null;

            if (n && _.isNumber(n) && n.toString() === ns.toString()) {
              window.location.href = window.location.protocol + "//" + window.location.host + window.location.pathname + "?servings=" + n;
            }
          });

          $('#updateServings').append($b);
          $('#servings').keypress(function (event) {
            if (event.which === 13) {
              event.preventDefault();

              $b.click();
            }
          });
        });
      });
    });
  </script>

