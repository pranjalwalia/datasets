<script type="text/javascript">
var _fbPermissions = {};

function getCurrentUserFbPermissions() {
  return _fbPermissions;
};

window.fbAsyncInit = function() {
  FB.init({appId: '54208124338', 
           status: true,
           cookie: true,
           oauth: true,
           xfbml: true,
           channelUrl: location.protocol + "//" +
           location.hostname +
           (location.port.length > 0 ? ":" + location.port : "") +
           '/html/channel.html'
          });

  // Hack to fix http://bugs.developers.facebook.net/show_bug.cgi?id=20168 for IE7/8/9
//  FB.UIServer.setLoadedNode = function (a, b) {     FB.UIServer._loadedNodes[a.id] = b; };
//  FB.UIServer.setActiveNode = function(a,b) { FB.UIServer._active[a.id]=b; };
  
  //FB.Event.subscribe('auth.sessionChange', facebookPostlogin);

  // listeners for the like button and the comments
  FB.Event.subscribe('edge.create', function(href, widget) {    
    require(['util/googleAnalytics'], function(GA) {
      //logTrackingEvent('fb-like-recipe', href);

      GA.trackEvent('FB', 'like', 'href', href);
    });
  });

  FB.Event.subscribe('comments.add', function(response) {
    require(['util/googleAnalytics'], function(GA) {
      //logTrackingEvent('fb-comment-recipe', window.location.href);

      GA.trackEvent('FB', 'comment', 'href', href);
    });     
  });

  FB.getLoginStatus(function (response) {
    if (response.authResponse) {
      var session = response.authResponse;
      var permissionsToAsk = ["email","publish_stream","publish_actions"];
      
      if (session) {
        FB.Data.query("select " + permissionsToAsk.join(', ') + " from permissions where uid = {0}", session.uid)
          .wait(function (rows) {
            if (rows) {
              for (var k in permissionsToAsk)
                _fbPermissions[permissionsToAsk[k]] = rows[0][permissionsToAsk[k]] != "0";
            }
          });
      }
    }
  });

  require(['jquery'], function($) {
    $('#fb-root').trigger('fbLoaded');
  });
  //FB.XFBML.parse();
};

function onFbLoaded(handler) {
  $('#fb-root').bind('fbLoaded', handler);
}

(function() {
  var e = document.createElement('script'); e.async = true;
  e.src = 'https:' +
    '//connect.facebook.net/en_US/all.js';
  document.getElementById('fb-root').appendChild(e);
}());
</script>

