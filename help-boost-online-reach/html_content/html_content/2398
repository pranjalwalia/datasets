  require(['jquery', 'jquery.ninja.ui', 'cells/recipePage/emailWindow',  'jquery.validationEngine-en', 'jquery.validationEngine'], function($, _ninja, _e, _ve, _v) {
    if ((!$) && jQuery) {
      $ = jQuery;
    }

    $(function() {
      var $emailPopUp;
      var $buttonEmail = $.ninja().button({
        html: ' Email'
      }).prepend($.ninja().icon({
        name: 'mail'
      })).select(function () {
        $emailPopUp = $buttonEmail.ninja().popup({
          html: $('<div/>', {
            id: 'emailWindow',
            width: 480,
            height: 530
          }),
          button: true,
          window: true
        });

        $('#emailWindow').showEmailForm('1AAC532C-09E0-FDE4-7D680929191B03FC', 'Zucchini Lasagna (No Pasta!)', 'http://i.yummly.com/Zucchini-Lasagna-_no-Pasta_-Recipezaar-2.l.jpg');

      }).deselect(function() {
        $.validationEngine.closePrompt('.formError',true);
        $emailPopUp.detach();
      });

      $('#buttonEmail').append($buttonEmail);


    });
  });
</script>

