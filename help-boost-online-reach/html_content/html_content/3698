RootCookies.SetCookie('show_sidebar', 'no', 1);
$('.close-sidebar').hide();
$('.show-sidebar').show();
$('#rsidebar').fadeOut(1000);
$('#blog').animate({width: "770px"}, 1000);
$('.box').animate({width: "760px"}, 1000);
});
$('.show-sidebar').click(function() {  
RootCookies.SetCookie('show_sidebar', 'no', -1);       
