			<div class="Column2">

				<!-- 300x250 Dart ad : start -->
				<!-- Start DoubleClick Ad Tag --> 
<div class="ContentBorder">
	<script type="text/javascript"> 

	// this is delcared public so we can call "dclk" later on for things 
	// such as var adaptvCompanionAdTag, on the video page.
	var dclk;

	(function(){

		var sitename = "dmd.cracked";
		var zone     = "content";
		var sz       = "300x600,300x250";
		var ugc      = "";
		var art      = "12977";
		var auth     = "chris bucholz";
		var dbr      = "";
		var rating   = "r";
		var cat      = "";
		var loc      = "atf";
		var vid      = "";
		var envr     = "";
		var tile     = "2"; // (for roadblocking)
		var ctype	 = "columnists";
		var ord      = "66331371314553";
		var title    = "";
		
					dclk = '<scr'+'ipt type="text/javascript" src="http://ad.doubleclick.net/adj/'+sitename+'/'+zone+';cat='+cat+';ctype='+ctype+';envr='+envr+';loc='+loc+';rating='+rating+';dbr='+dbr+';auth='+auth+';art='+art+title + ';vid='+vid+';ugc='+ugc+';%RSI%%Q%tile='+tile+';sz='+sz+';videoegg=;ord='+ord+';?"><\/scr'+'ipt>';
		
		dclk = dclk.replace('%RSI%', segQS);
		dclk = dclk.replace('%Q%', quantSegs);

