</div><style type="text/css" media="screen">#dd_ajax_float{
	background:none repeat scroll 0 0 #FFFFFF;
	border:1px solid #DDDDDD;
	float:left;
	margin-left:-90px;
	margin-right:10px;
	margin-top:-45px;
        margin-bottom: 15px;
	position:absolute;
	z-index:9999;
       overflow:hidden;
}</style><script type="text/javascript">jQuery(document).ready(function($){

    $.getDocHeight = function(){
        return Math.max(
            $(document).height(),
            $(window).height(),
            /* For opera: */
            document.documentElement.clientHeight
        );
    };

	var $postShare = $('#dd_ajax_float');
	
	if($('.dd_content_wrap').length > 0){
	
		var descripY = parseInt($('.dd_content_wrap').offset().top) - 20;
		var pullX = $postShare.css('margin-left');
	
		$(window).scroll(function () { 
		  
            var toBottom = $postShare.height() + $('footer').height() + 36;
			var scrollY = $(window).scrollTop();
			var fixedShare = $postShare.css('position') == 'fixed';
			
            
            if (scrollY > $.getDocHeight() - toBottom ){
                $postShare.css({
                    position: 'absolute',
                    top: $.getDocHeight() - toBottom ,
                    marginLeft: pullX
                });
            }
            else if($('#dd_ajax_float').length > 0){
			
				if ( scrollY > descripY && !fixedShare ) {
					$postShare.stop().css({
						position: 'fixed',
						top: 65
					});
				} else if ( scrollY < descripY && fixedShare ) {
					$postShare.css({
						position: 'absolute',
						top: descripY,
						marginLeft: pullX
					});
				}
				
			}
	
		});
	}
});</script><script type="text/javascript"> jQuery(document).ready(function($) { window.setTimeout('loadLinkedin_109135()',1000); }); </script><script type="text/javascript"> function loadLinkedin_109135(){ jQuery(document).ready(function($) { $('.dd-linkedin-109135').remove();$.getScript('http://platform.linkedin.com/in.js'); }); }</script><script type="text/javascript"> jQuery(document).ready(function($) {
	
		if($(window).width()> 790){ 
			$('#dd_ajax_float').show()
		}else{
			$('#dd_ajax_float').hide()
		}

		$(window).resize(function() { 
			
			if($(window).width()> 790){ 
				$('#dd_ajax_float').show()
			}else{
				$('#dd_ajax_float').hide()
			}
			
		});  

