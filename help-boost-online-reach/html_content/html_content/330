												title: 'Fork in the Road - Tejal Rao - 10 Best Sandwiches in New York',
												sig: 'fe0ad20503a9c0ad743c10bdaf9e29a9'
											}
										}).ready(function(){
											conv_ready = true;
											MVN.queue(function(){
												MVN.LF.login();
												LF.Dispatcher.addListener(MVN.LF.binds());
												MVN.LF.clean();
											});
										});

										MVN.queue(function(){
											/*
											** LiveFyre Component
											*/
											var LF_Component = function(controller){
												var me = this;
												me.MVN = controller;
												me.version = 'LiveFyre for MVN 1.0';
												me.autoInit = false;
											}
								
											LF_Component.prototype = {
												checked: false,
												queued: false,
												binds: function(){
													var me = this;
													return {
													handle_auth_login: function(){
														if(me.MVN.omni||false){
															me.queued = false;
															if(!me.MVN.user){
																var src_desc = '';
                                me.MVN.omni.actionSrc = 'lf';
                                me.MVN.omni.action(me.MVN.omni.buildAction('login', {sid: '8|53|'}));
															}
														}else if(!me.queued){
															me.MVN.queue(me.binds().handle_auth_login);
															me.queued = true;
														}
													},
													handle_auth_logout: function(){
														if(me.MVN.omni||false){
															me.queued = false;
															if(me.MVN.user||VVM.getCookie('MVN[anon]')){
																me.MVN.omni.action('logout');
															}
														}else if(!me.queued){
															me.MVN.queue(me.binds().handle_auth_logout);
															me.queued = true;
														}
													},
													handle_comment_shown: function(){
														me.clean();
													}
												}
											},
											login: function(){
												var me = this;
												if(me.checked==false&&window.conv&&window.conv_ready&&me.MVN.omni&&me.MVN.sso&&(me.MVN.user||VVM.getCookie('MVN[anon]'))){
													me.checked = true;
													me.MVN.sso.getLfToken(function(o){
														if(o&&o.status=='success'){
															conv.login(o.token);
															if(o.anon && o.anon == true){
																me.clean();
															}
														}
													});
												}
												return false;
											},            
											clean: function(){
												$('#livefyre .lf_comment_container').not('.lf_deleted').each(function(){
													var usr = $('.lf_comment_user', this).attr('jid');
													if(typeof usr == 'string' && usr.substr(0,5) == 'anon_'){
														$('.hastooltip', this).removeClass('hastooltip');
														$('a.lf_comment_user_nick', this).replaceWith(
															$('<span style="font-weight:bold" title="Anonymous User"/>').text($('a.lf_comment_user_nick', this).text())
														);
														$('img.lf_main_avatar', this).css('cursor', 'default');
														$('a.lf_user', this).replaceWith('<div class="lf_user" title="Anonymous User">'+$('a.lf_user', this).html()+'</div>');
													}
												});
											}
										}
										MVN.load({
											'LF':{
												component: LF_Component,
												params: {}
											}
										});
									});
								</script>
