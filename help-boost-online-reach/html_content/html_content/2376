		oPSlideshow_229885.oBlog			= {'entry_author_headshot_url':'', 'entry_title': '', 'entry_text': '', 'entry_vertical': '', 'entry_url': ''};
		
		oPSlideshow_229885.init();
		//fix to hide blog image if first slide is blog
		jQuery('.ps-slideshow-blog-image').hide();
	});
	</script></p>
									
