	    pageTracker._trackEvent("Comment", "Submit", "/article/quit_drinking_coffee_and_start_smelling_it/1359");
	});
	
	$('#commentFormButton').click(function()
		{
				  		//alert('You must Sign In to add a comment.');
	  		window.location = '/signin';
	  		}); 

	}); 
</script> <!--END_SEARCHABLE-->				</fieldset>
			</div>
