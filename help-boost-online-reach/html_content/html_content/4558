<script type="text/javascript">
/* <![CDATA[ */
function mtUpdateSignInWidget(u) {
    var separator = '&nbsp;&nbsp;|&nbsp;&nbsp;';
    var el = document.getElementById('signin-widget-content');
    var content = '';
    // alert (document.URL);
    //alert (encodeURIComponent(document.URL));
    if (!el) return;
    if (u) {
        if (u && u.is_authenticated) {
            user = u;
            mtSaveUser();
        } else {
            // user really isn't logged in; so let's do this!
            return mtSignIn();
        }
    } else {
        u = mtGetUser();
    }
    if (u && u.name) {
        var url;
		content += '<div id="login-tab" class="loggedIn"><div id="login-tab-inner">';
        if (u.is_authenticated) {
            if (u.is_author) {

				url = 'http://www.seriouseats.com/user/profile/'; 


            } else {
                url = u.url;
            }
        } else if (u.url) {
            url = u.url;
        } else {
            url = null;
        }
        if (url)
            content += '<span class="welcome">Hello, <a href="' + url + '">' + u.name + '</a>.</span>';
        else
            content += '<span class="welcome">Hello, <strong>' + u.name + '.</span>';
        if (u.is_author)
            content += '<span class="account"><a href="http://www.seriouseats.com/cgi-bin/mte/mt-cp.cgi?__mode=edit&blog_id=34&return_to=' + encodeURIComponent(document.URL) + '">Account</a></span>';
        
        content += ' | <a href="javascript:void(0)" onclick="return mtSignOutOnClick()">Log out</a>';
        
		content += '</div></div>'
    } else if (u && u.is_banned) {
        content = 'You do not have permission to sign in to this blog.';
    } else {
		content = '<div id="login-tab" class="notLoggedIn"><div id="login-tab-inner"><span class="welcome">Welcome to Serious Eats!</span><a onclick="return mtSignInOnClick(\'login-tab-inner\')" href="javascript:void(0)" id="login-link">Log in</a> or <a href="http://www.seriouseats.com/cgi-bin/mte/mt-cp.cgi?__mode=register&blog_id=34&blog_id=34&return_to=http%3A%2F%2Fwww.seriouseats.com%2Fwelcome%2F">join</a> the community.</div></div>'
    }
    //alert ("content is: " + content);
    el.innerHTML = content;
}
mtAttachEvent('usersignin', mtUpdateSignInWidget);
mtUpdateSignInWidget();
/* ]]> */
</script>
