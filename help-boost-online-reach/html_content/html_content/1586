<script>

(function($){

    $.scrollbarWidth = function() {
        if (!$._scrollbarWidth) {
            var $body = $('body');
            var w = $body.css('overflow', 'hidden').width();
            $body.css('overflow','scroll');
            w -= $body.width();
            if (!w) w=$body.width()-$body[0].clientWidth; // IE in standards mode
                $body.css('overflow','');
                $._scrollbarWidth = w;
        }
        return $._scrollbarWidth;
    };

})(jQuery);

function resizeGutters(){
    var containerOffset = document.getElementById("container").offsetLeft;
    $("#leftgutter").css("left", containerOffset - 160);
    $("#rightgutter").css("left", containerOffset + 950);
}
if (!((window.XMLHttpRequest == undefined) && (ActiveXObject != undefined))){
    $(window).resize(function () {
        resizeGutters();
    });

    $(document).ready(function () {
        resizeGutters();
        $("#leftgutter").show();
        $("#rightgutter").show();
    });
}

</script>

