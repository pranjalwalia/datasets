$(window).load(function(){
	function moveFloatMenu() {
		if (menuYloc.top < $(this).scrollTop()){
			$('#navColumnOneWrapper').css('position','fixed');
			$('#navColumnOneWrapper').css('top','0');
		}
		else {
			$('#navColumnOneWrapper').css('position','absolute');
			$('#navColumnOneWrapper').css('top','auto');
		}
	}
menuYloc = $('#navColumnOneWrapper').offset();
$(window).scroll(moveFloatMenu);
moveFloatMenu();
});

