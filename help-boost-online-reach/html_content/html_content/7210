					for(var item = 0; item < ndn_entries[col].length; item++){
						var matches = ndn_entries[col][item].entry_url.match(/[&?]vid=([^&]*)&/);
						if(matches != null && matches.length > 0) ndn_videos.push(matches[1]);
					}
				}
				
				jQuery("#hp_ndn_page_1 .snp_most_popular_entry").each(function(){
					var title=jQuery(this).attr('data-title');
					var link=jQuery(this).attr('data-link');
					
					if(jQuery(this).hasClass('large_entry')){
						var thumbUrl = jQuery(this).find('.widget_thumb').attr('longdesc');
						var _h = 190;
						var _w = 260;
						var _pattern = /^([^_]+)(_[0-9ct]+)(_[0-9]+_[0-9]+)?(\.)?(\w{3})$/;
						var _matches = _pattern.exec(_str);
						var _newStr = _matches[1] + _matches[2] + '_'+_w +'_'+ _h + _matches[4] + _matches[5];
						jQuery(this).find('.widget_thumb').attr('longdesc', _newStr).attr('src', _newStr);
					}
					
					jQuery('a', this).click(function(e){ndn_lightbox_launch({ 'title': title, 'PageURL': link, 'ReferrerURL': link },link); e.preventDefault(); e.stopPropagation(); return false;});
				});
				
				var ndn_pagination = new CommonPaginator({'moduleName': 'ndn', 'maxPage': ndn_max_page, 'is_circle': true, 'is_custom_data': true, 'custom_data': ndn_entries, 'custom_img_width': 74, 'custom_img_height': 54, 'custom_play_button': true});
				HuffPoUtil.ImageLoader.foldCheck("hp_ndn_page_1");
			</script>
		
		
			



	
