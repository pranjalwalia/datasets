function getCookie(c_name)
{
var i,x,y,ARRcookies=document.cookie.split(";");
for (i=0;i<ARRcookies.length;i++)
{
  x=ARRcookies[i].substr(0,ARRcookies[i].indexOf("="));
  y=ARRcookies[i].substr(ARRcookies[i].indexOf("=")+1);
  x=x.replace(/^\s+|\s+$/g,"");
  if (x==c_name)
    {
    return unescape(y);
    }
  }
}

var verifyCookie = getCookie("jsverify");

var date = new Date();
date.setTime( date.getTime()+(24*60*60*1000*365)*20 );
document.cookie = "jsverify=yes; expires=" + date.toGMTString() + "; path=/; domain=momaroo.com";
document.cookie = "ar=1; path=/; domain=momaroo.com";
var has_cookie = (document.cookie) ? true : false;


if( has_cookie && (verifyCookie == null || verifyCookie == '') )
{
    document.location.reload(true);
}

