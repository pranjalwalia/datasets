<script type="text/javascript">
    var commentToRecId = 0;
    function prepareCommentRec() {
        if (false) {
            alert('You cannot recommend comments on protected posts');
        }
        else if ($("#twitterloggedin").val() == "true" ||
            $("#facebookloggedin").val() == "true" ) {
            alert('You must be signed in with a Xanga Network account to recommend items');
        }
        else if (confirm('Are you sure you want to recommend this comment?')) {
            $.ajax({
                url: "/RecommendHandler.ashx",
                data: "itemType=WeblogComment&itemId=" + commentToRecId + "&blogId=744397617",
                type: "POST",
                success: function(response) {
                    if (response != null && response != '') {
                        alert(response);
                    }
                    else {
                        var countElement = $("#rec-count-" + commentToRecId);
                        var count = countElement.html();
                        if (count != null) {
                            var currentCount = parseInt(count.substring(1, count.length - 1));
                            countElement.html("(" + (currentCount + 1) + ")");
                        }
                        else {
                            $("#recommend-" + commentToRecId).append($("<span></span>").attr("id", "rec-count-" + commentToRecId).addClass("count").html(" (1)"));
                        }
                    }
                }
            });
        }
    }
</script>
