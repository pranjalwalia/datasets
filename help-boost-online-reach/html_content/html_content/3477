<div class="module module-search module-search-options" id="module--1019">

<div class="modulecontent">


   <script type="text/javascript">
        function GetSearchUrl6866358()
        {
            var searchUrl = '';
            if( document.getElementById('searchRad6866358-Tags') != null && document.getElementById('searchRad6866358-Tags').checked == true)
                searchUrl = 'http://www.momaroo.com/xangatagshome.aspx';
            else if (document.getElementById('searchRad6866358-user') != null && document.getElementById('searchRad6866358-user').checked == true)
                searchUrl = 'http://www.momaroo.com/search/'; 
            else
                searchUrl = 'http://search.momaroo.com/searchsite.aspx';

            return searchUrl;
        }

       function PrepareSearch6866358() {

           SearchBoxSearch(GetSearchUrl6866358(), escape(document.getElementById('searchbox6866358').value), null);
       }

       function PrepareOnKeyPressSearch6866358()
       {
            return SearchBoxSearchOnEnter(GetSearchUrl6866358(), escape(document.getElementById('searchbox6866358').value), null, event);
       }
   </script>
<ul class="list details-only">
    <li class="item item-1 item-odd">
        <div class="details">
            <div class="itembody">
                <input type="text" class="searchtext" value="search..." id="searchbox6866358"
                    onclick="if (this.value == 'search...') { this.value = ''; }" 
                    onmouseover="this.className='searchtext over';" onmouseout="this.className='searchtext';"
                    onfocus="this.className='searchtext over';" onblur="this.className='searchtext';"
                    onkeypress="return PrepareOnKeyPressSearch6866358();" />
                <a class="searchit" title="search" href="#" onclick="PrepareSearch6866358(); return false;">
                </a>
                
            </div>
        </div>
    </li>
</ul>
<div class="extra"></div>
</div>

