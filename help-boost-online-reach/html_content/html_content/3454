            holder.currentPostUrl = "";
            holder.currentPostId = 2075947500162167844
          
          
          
            holder.postFeed = "http://www.blogger.com/feeds/8047319909149867773/posts/default";
          
          
          
            holder.commentFeed = "http://www.blogger.com/feeds/8047319909149867773/comments/default";
          
          holder.currentBlogUrl = "http://www.plainchicken.com/";
          holder.currentBlogId = "8047319909149867773";
        
        return holder;
      });
    }
  </script>
