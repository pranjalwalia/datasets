            after:   onAfterslideshow_11});
        function onAfterslideshow_11(currslideshow_11, nextslideshow_11, optsslideshow_11) {
            // commented out by cricketts 2.11.11
            //var indexslideshow_11= optsslideshow_11.currSlide;
            //$('#prevslideshow_11')[indexslideshow_11== 0 ? 'hide' : 'show']();
