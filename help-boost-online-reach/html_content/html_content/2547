                                        <!--widget[ABBot_1_storycore_null]-->
                                        <script type="text/javascript">
var addthis_share =
{
	url: 'http://abcnews.go.com/GMA/gmas-top-10-recipes-2011/story?id=15247611', //override window url
	title: '', //override window title
	description: '' //optional
}

var lStory = new Object();
leftRailVideoOpen = false;
listStoryFlag = true;
listStoryCurrTab = 0;
lStory.isClickthrough = ("multipleitems" == "clickthrough") ? true : false;
lStory.viewOverride = readCookie('lsView');
lStory.title = document.title;
lStory.sUrl = 'javascript:lStory.handleView(lStory.altView);'
lStory.isIE7 = false;

if($.browser.msie ) {
 	if(parseInt($.browser.version, 10) == 7){
 		lStory.isIE7 = true;
 	}
}

if(lStory.viewOverride == 'all'){
	lStory.isClickthrough = false;
}else if(lStory.viewOverride == 'intro'){
	lStory.isClickthrough = true;
}

if(readCookie != null){
	eraseCookie('lsView',"/");
}

if(lStory.isClickthrough){
	lStory.altView = 'all';
	lStory.viewLabel = 'View All';
}else{
	lStory.altView = 'intro'
	lStory.viewLabel = 'View Individually';
}

lStory.handleView = function(view){
	createCookie('lsView',view,1);
	if(view == 'all'){
		window.location.hash = "all";	
	}else{
		window.location.hash = "";
	}
	window.location.reload(false);
	//window.location = window.location.href.split('#')[0];
}

lStory.restartCycle = function(){
	window.location.hash = "";
 	$('#listItems').cycle('destroy');
	$('#listItems').cycle(cyclesettings);
}

lStory.openCaption = function(itemId) {
	$('#'+itemId).find(".cap_short").hide();
	$('#'+itemId).find(".cap_full").show();
	lStory.resizePane(itemId, false);
}

lStory.closeCaption = function(itemId){
	$('#'+itemId).find(".cap_full").hide();
	$('#'+itemId).find(".cap_short").show();
	lStory.resizePane(itemId, true);
}

lStory.resizePane = function(itemId, isOpen){
	if(lStory.isClickthrough){
		var paneHt = $('#listItems').height();
		var sHt = $('#'+itemId).find(".cap_short").height();
		var fHt = $('#'+itemId).find(".cap_full").height();
		var oHt = fHt - sHt;
		
		if(isOpen){
			paneHt = paneHt - oHt;
		}else{
			paneHt = paneHt + oHt;
		}
		
		$('#listItems').animate({"height": paneHt});	
	}
}

lStory.backMarkup = "<div id='back' class='back'>\n	<div class='restart'>\n		<a href='javascript:void(0);' onclick='lStory.restartCycle()'>\n			<span id='back-arrow'></span><span id='restart-msg'>BACK TO BEGINNING</span>\n		</a>\n	<div class='clearboth'><!-- empty --></div>\n	</div>\n	<div id='relateds'><div id=\"related-header\"><p class=\"label\">More Coverage<\/p><\/div><div class=\"related-item\"><div class=\'thumbnail\'><a href=\"\/GMA\/recipe\/julia-childs-coq-au-vin-7113750\"name=\"lpos=widget[ListStory_MoreCoverage]&lid=view[RecipeLink]\"><img src=\"http:\/\/a.abcnews.com\/images\/site\/abc_default_image_wl.jpg\" alt=\"ABC News\" border=\"0\" \/><\/a><\/div><div class=\'headline\'><a href=\"\/GMA\/recipe\/julia-childs-coq-au-vin-7113750\"name=\"lpos=widget[ListStory_MoreCoverage]&lid=view[RecipeLink]\">Julia Child\'s Coq au Vin<\/a><\/div><\/div><div class=\"related-item\"><div class=\'thumbnail\'><a href=\"\/GMA\/recipe\/valerie-bertinellis-tuscan-style-soup-8818279\"name=\"lpos=widget[ListStory_MoreCoverage]&lid=view[RecipeLink]\"><img src=\"http:\/\/a.abcnews.com\/images\/GMA\/abc_soup_091015_wl.jpg\" class=\"\" alt=\"Valerie Bertinelli\'s Tuscan Style Soup\" border=\"0\"\/><\/a><\/div><div class=\'headline\'><a href=\"\/GMA\/recipe\/valerie-bertinellis-tuscan-style-soup-8818279\"name=\"lpos=widget[ListStory_MoreCoverage]&lid=view[RecipeLink]\">Valerie Bertinelli\'s Tuscan Style Soup<\/a><\/div><\/div><div class=\"related-item last\"><div class=\'thumbnail\'><a href=\"\/GMA\/recipe?id=13364622\"name=\"lpos=widget[ListStory_MoreCoverage]&lid=view[RecipeLink]\"><img src=\"http:\/\/a.abcnews.com\/images\/GMA\/abc_casserole_dm_110415_wl.jpg\" class=\"\" alt=\"PHOTO:&nbsp;Emeril\'s Broccoli, Cheese and Ham Healthy Breakfast Casserole is shown.\" border=\"0\"\/><\/a><\/div><div class=\'headline\'><a href=\"\/GMA\/recipe?id=13364622\"name=\"lpos=widget[ListStory_MoreCoverage]&lid=view[RecipeLink]\">Emeril\'s Broccoli, Cheese and Ham Healthy Breakfast Casserole<\/a><\/div><\/div></div>\n	</div>";


lStory.scrollTo = function(){
	$.scrollTo('#listanchor', 500 );
}

$(document).ready(function() {
	if(document.title != lStory.title){
		document.title = lStory.title;
	}
});

isLoadedFlag = false;
ssLocStr = window.location.href;
ssLocNew = ssLocStr;

function abcnTrack(action) {
	if(isLoadedFlag){
		if(s_omni.pageName){
			s_omni.pageName = (s_omni.pageName.indexOf('_liststory_pane')>-1)?s_omni.pageName:s_omni.pageName+'_liststory_pane';
			s_omni.eVar16 = s_omni.pageName;
			s_omni.prop19 = s_omni.pageName;
			s_omni.prop24 = null;	//most popular
			s_omni.t();
		}
	}else{
		isLoadedFlag = true;
	}
	
	var nielsenImg = new Image(1, 1); 
	nielsenImg.onerror = nielsenImg.onload = function () {
		nielsenImg.onerror = nielsenImg.onload = null; 
	};
		
	ssLocDel = (ssLocStr.indexOf('?') != -1)?'&':'?';
	ssLocNew = ssLocStr + ssLocDel + 'page=';
		
	nielsenImg.src = ["//secure-us.imrworldwide.com/cgi-bin/m?ci=us-704476h&cg=0&cc=1&si=", escape(ssLocNew), "&rp=", escape(document.referrer), "&ts=compact&rnd=", (new Date()).getTime()].join('');
}


//player functions begin

	storyPlayer = new Object();

    storyPlayer.positionPlayer = function(obj){
        objPos = $(obj).offset();
        $('#listPlayerContainer').css('position', 'absolute');
        $('#listPlayerContainer').css('z-index', '1003');
        $('#listPlayerContainer').offset({top: objPos.top, left: objPos.left});
    }
    

    storyPlayer.hide = function(){
    	$('#listPlayerContainer').css('position', 'absolute');
        $('#listPlayerContainer').css('top', '-1000px');
        $('#listPlayerContainer').css('left', '-142px');
        
        if(typeof kdp != 'undefined'){
        	kdp.sendNotification("doStop"); 
        }
        
        if(typeof window.currentVideoSlide != 'undefined'){
      		$("#" + window.currentVideoSlide).find(".credit").show();   
        }

    }
	
    playStoryVideo = function(id,videoId,isLeadItem){
    	window.currentVideoSlide = id;
    	storyPlayer.thumbSelector = isLeadItem ? ' #leadItemImage':' .listItemThumb';
        mediaContainerObj = $("#" + id + storyPlayer.thumbSelector);
    	storyPlayer.positionPlayer(mediaContainerObj);
    	//hide image credit and display video caption
      	$("#" + id).find(".credit").hide();
    	pc.playVideoByCID({id:videoId});
    }
    


//player functions end

</script>	
