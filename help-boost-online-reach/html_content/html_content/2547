				subNavMan.playlist.gotoItem("sectiongma");
				subNavDTimer=null;
				subNavActive = false;
			};
			
			
			subNavReset();
			subNavMouseOut = function(){
				if(subNavATimer != null){
					window.clearTimeout(subNavATimer);
				}
				if(subNavDTimer==null){
					subNavDTimer = window.setTimeout(subNavReset,1000);
				}
			};
			subNavMouseOver=function(){
				if(subNavDTimer!=null){
					window.clearTimeout(subNavDTimer);
					subNavDTimer=null;
				}
			};
			subNavGoto = function(obj){
				subNavMouseOver();
				var o = obj;
				var time = subNavActive ? 150:300;
				if(subNavATimer != null){
					window.clearTimeout(subNavATimer);
					subNavATimer = null;
				}
				subNavATimer = window.setTimeout(function(){
					subNavActive = true;
					subNavList.gotoItem(o.id);
					o=null;
				},time);
				subNavList.prepare(o.id);
			}
			subNavMan.pane.addMouseOut(subNavMouseOut);
			subNavMan.pane.addMouseOver(subNavMouseOver);
			// ]]>
		 </script>
	
