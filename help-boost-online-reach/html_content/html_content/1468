    <input name="ctl00$fragment_cbca5a19_742a_4255_8c85_e3e8b9b59e5d$_2aaf2909919c$SearchText" type="text" maxlength="64" size="45" id="ctl00_fragment_cbca5a19_742a_4255_8c85_e3e8b9b59e5d__2aaf2909919c_SearchText" ButtonId="SearchButton" class="text" onkeydown="return keyHandler(event);" onfocus="if(this.defaultValue==this.value) this.value='';" onblur="if(this.value=='') this.value=this.defaultValue;" />
    <span></span>
    <input type="submit" value="search" onclick="return doSearch();" />
</div>

<script type="text/javascript">
    function doSearch() {
        var searchString = $('#ctl00_fragment_cbca5a19_742a_4255_8c85_e3e8b9b59e5d__2aaf2909919c_SearchText')[0].value;

        if (searchString != '') {
            var newLocation = 'http://www.peta.org/search/SearchResults.aspx?q=' + searchString;
            $(location).attr('href', newLocation);
        }
        return false;
    }

    function keyHandler(event) {
        if (event.keyCode == 13) {

            Page_ValidationActive = false;
            event.returnValue = false;
            event.cancel = true;

            return doSearch();
                    }
        return true;
    }

    $(document).ready(function() {
        $('#ctl00_fragment_cbca5a19_742a_4255_8c85_e3e8b9b59e5d__2aaf2909919c_SearchText').coolinput({ hint: 'SEARCH PETA.ORG' });
    });
</script>






