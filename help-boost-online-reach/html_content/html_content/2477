<script>
function setCookie(name, value, expires, path, domain, secure)
{ document.cookie = name + "=" + escape(value) + (expires ? "; expires=" + expires
                                                        : "")
                                             + (path ? "; path=" + path
                                                     : "")
                                             + (domain ? "; domain=" + domain
                                                       : "")
                                             + (secure ? "secure"
                                                       : "");
}

function getCookie(name)
{ var prefix = name + '=';
  var c = document.cookie;
  var nullstring = '';
  var cookieStartIndex = c.indexOf(prefix);
  if (cookieStartIndex == -1)
    return nullstring;
  var cookieEndIndex = c.indexOf(";", cookieStartIndex + prefix.length);
  if (cookieEndIndex == -1)
    cookieEndIndex = c.length;
  return unescape(c.substring(cookieStartIndex + prefix.length, cookieEndIndex));
}

function deleteCookie(name, path, domain)
{ if (getCookie(name))
  { expires = 'Thu, 01-Jan-70 00:00:01 GMT';
    setCookie(name, '', expires, path, domain);
  }
}


// fix the bug in Navigator 2.0, Macintosh
function fixDate(date)
{ var base = new Date(0);
  var skew = base.getTime();
  if (skew > 0)
    date.setTime(date.getTime() - skew);
}

function check_tag(ctrl)
{ if (ctrl.value == '')
  { ctrl.value = 'Anonymous';
  }
}

function remember(name, value)
{ var now = new Date();
  fixDate(now);
  now.setTime(now.getTime() + 365 * 24 * 60 * 60 * 1000);
  now = now.toGMTString();
  setCookie(name, value, now, '/', '', '');
}   
  

function update_homepage(form)
{ var tag = form.community_login_tag.value;
  if (tag == 'Anonymous')
  { form.homepage.value = '';
  } else if (tag)
  { form.homepage.value = get_homepage_from_tag(tag);
  } else
  { form.homepage.value = '';
  }
  remember('homepage', form.homepage.value);
}

function get_homepage_from_tag(tag)
{ var homepage = 'http://www.orble.com/default/';
  var tag_dir = tag.toLowerCase().replace(/[^A-Z 0-9]/ig, '').replace(/ +/ig, '-');
  return homepage.replace('default', tag_dir);
}


function displayShareForm(form)
{ var val = form.community_blogs_comments_create_share.value;
  var valp = form.community_blogs_comments_create_private.value;
  var valn = form.community_blogs_comments_create_notify.value;
  var valli = form.community_blogs_comments_create_logged_in.value;
  var valcode = form.community_blogs_comments_create_private_comment_code.value;
  var div = document.getElementById('display_share_form');
  var diva = document.getElementById('display_share_form_text_a');
  var divb = document.getElementById('display_share_form_text_b');
  var divc = document.getElementById('display_share_form_text_c');
  var divd = document.getElementById('display_share_form_text_d');
  var dive = document.getElementById('display_share_form_text_e');
  var divf = document.getElementById('display_share_form_text_f');

  if (div)
  { if (val != 0 || valp != 0)
    { div.style.display = 'block';
    } else
    { div.style.display = 'none';
    }
  }

  { if (valp != 0)
    { divb.style.display = 'block';
      diva.style.display = 'none';
    } else
    { diva.style.display = 'block';
      divb.style.display = 'none';
    }
  }

  if (valcode != 0)
  {
      divc.style.display = 'none';
  }

  if (valn != 0 || valp != 0 || val != 0)
  {
      divd.style.display = 'block';
      dive.style.display = 'none';
  } else {
      divd.style.display = 'none';
      dive.style.display = 'block';
  }

  if (valli != "no")
  {
      divf.style.display = 'none';
  }
} 

</script>

