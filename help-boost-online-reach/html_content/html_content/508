<input name="main_0$leftcolumn_0$ucSaveRecipeToRecipeBox1$hdnRecipeId" type="hidden" id="main_0_leftcolumn_0_ucSaveRecipeToRecipeBox1_hdnRecipeId" />
<input name="main_0$leftcolumn_0$ucSaveRecipeToRecipeBox1$hdnRBDCategoryID" type="hidden" id="main_0_leftcolumn_0_ucSaveRecipeToRecipeBox1_hdnRBDCategoryID" />
<input name="main_0$leftcolumn_0$ucSaveRecipeToRecipeBox1$hdnMemberGenerated" type="hidden" id="main_0_leftcolumn_0_ucSaveRecipeToRecipeBox1_hdnMemberGenerated" />
<div id="SpotLightContainer">
