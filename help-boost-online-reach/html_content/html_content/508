 function SendToRegisterPage(validationGroup) {
     document.forms[0].action = 'https\x3a\x2f\x2fwww.bettycrocker.com\x2f\x7e\x2fEmailReg\x2fUserEmailProcessor.ashx\x3fReturnURL\x3dhttp\x3a\x2f\x2fwww.bettycrocker.com\x2frecipes\x2fzucchini-bars-with-browned-butter-frosting\x2f50aafca1-5ebf-4f51-b58b-4fecd16db746';
     document.forms[0].__VIEWSTATE.name = 'NOVIEWSTATE'; 
        if (typeof (Page_ClientValidate) == 'function')
            validationResult = Page_ClientValidate(validationGroup);
     
        if (validationResult)
            document.forms[0].submit();
    }
</script>
