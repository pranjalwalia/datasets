                jQuery(function() {
                    // getting the video details delete button to work
                    $('#details1 .video-remove').live('click', function(e) {
                        var toRemove = videoplayers[1].extractVideoData(this);
                        var playlist = videoplayers[1].getPlaylist();
                        for (var i = 0; i < playlist.length; i++) {
                            var playlistItem = playlist[i];
                            if(videoplayers[1].isSameVideo(playlistItem, toRemove)){
