            function do_query_add_to_playlist(query) {
                // search youtube for first match
                jQuery.ajax({
                    data: {
                        t: 'cheetah-photo-search/mvs_videosearch01.txt',
                        orderby: 'published',
                        corpus: 'current',
                        language_id: '1',
