                        video_sources: 'youtube'
                    },
                    dataType: 'jsonp',
                    url: 'http://upge.wn.com/',
                    success: function(text) {
                        video_to_add = $(text).find('.video-info').first();
                        if (video_to_add.length > 0) {
                            videoplayers[1].addVideo(video_to_add);
                        }
                        else {
     						$('#noresults').html('Could not find a suitable video for "' + query + '"!');
                        }
                    }
                });
            }
            
            jQuery(function() {
                $('.instaplay-audioplayer').live('click', function(e) {
                    e.preventDefault();
                });

                                $('#retrieve-playlist').live('click', function(e) {
                                        $('.disco-check:checked').each(function(idx) {
                                                query = $(this).next('a').attr('title');
                                                do_query_add_to_playlist(query);
                                        });
                                });
                
                                $('.disco-check').live('click', function(e) {
                                        checks_on = $('.disco-check:checked').length;
                                        if (checks_on <= 1) {
                                    	    $('#retrieve-playlist').text('Use checked songs to create your playlist');
                                        }
                                        else {
                                    	    $('#retrieve-playlist').html('Use <span class="num">'  + checks_on + '</span> checked songs to add to your playlist');
                                    	}
                                });
		jQuery('a.disco-album-all-playlist').live('click', function(e) {
			e.preventDefault();
			jQuery(this).next('a.disco-album-checkall').attr('checked', true);
			jQuery('#retrieve-playlist').click();
			$('html, body').animate({scrollTop:0}, 'slow');
		});


                jQuery('a.disco-album-checkall').live('click', function(e) {
            	    e.preventDefault();
            	    
            	    var links = $(this).parents('div.rel-head:first').next('ul.links');
            	    links.find(':checkbox').each(function() {
			$(this).click();
		    });
                });
                
                $('.toggle-tracks').live('click', function(e) {
                    tracks_list = $(this).parents('div.rel-head').next('ul.links');
                    if (tracks_list.length == 1) {
                        tracks_list.find('input.disco-check').click();
                    }
                });
                
                $('#retrieve-playlist').live('click', function(e) {
                    $('.disco-check:checked').each(function(idx) {
                        query = $(this).next('a').attr('title');
                        $(this).attr('checked', false);
                        $(this).attr('disabled', true);
                        do_query_add_to_playlist(query);
                    });
                });
                
                //$('.disco-check').live('click', function(e) {
                //    checks_on = $('.disco-check:checked').length;
                //    $('#retrieve-playlist').text('Use ' + checks_on + ' Checked Tracks to make your Playlist');
                //});
                
                $('span.insta-add-to-playlist').click(function(e) {
                    e.preventDefault();
                    query = $(this).parent().attr('title');
                    do_query_add_to_playlist(query);
                });
				 $('span.instaplay-videotrailer').click(function(e) {
                    e.preventDefault();

                    query = $(this).parent().attr('href').replace('http://wn.com/', '').replace('_', ' ');
                    play_obj_clicked = $(this);
                    
                    // search youtube for first match
                    jQuery.ajax({
                        data: {
                            t: 'cheetah-photo-search/mvs_simple_json.txt',
                            orderby: 'published',
                            corpus: 'current',
                            language_id: '1',
                            query: query
                        },
                        dataType: 'jsonp',
                        url: 'http://upge.wn.com/',
                        success: function(text) {
                            // embed the video
                            if (text.length > 0) {
                                video_obj = text[0];
                                $.fancybox({
                                            'padding'             : 10,
                                            'margin'                : 10,
                                            'autoScale'           : false,
                                            'transitionIn'        : 'none',
                                            'transitionOut'       : 'none',     
                                            'overlayColor'      : '#fff',                
                                            'hideOnOverlayClick'    : true,
                                            'hideOnContentClick'    : false,
                                            'title'               : this.title,
                                            'width'               : 560,
                                            'height'              : 340,
                                            'opacity'               : false,
                                            'href'                : 'http://www.youtube-nocookie.com/v/'+video_obj.id,
                                            'type'                : 'swf',    // <--add a comma here
                                            'swf'                 : {'allowfullscreen':'true', 'wmode':'transparent'} // <-- flashvars here
                                });
                            }
                        }
                    });
                });
                $('span.instaplay-video').click(function(e) {
                    e.preventDefault();

                    query = $(this).parent().attr('href').replace('http://wn.com/', '').replace('_', ' ');
                    play_obj_clicked = $(this);
                    
                    // search youtube for first match
                    jQuery.ajax({
                        data: {
                            t: 'cheetah-photo-search/mvs_simple_json.txt',
                            orderby: 'published',
                            corpus: 'current',
                            language_id: '1',
                            query: query
                        },
                        dataType: 'jsonp',
                        url: 'http://upge.wn.com/',
                        success: function(text) {
                            // embed the video
                            if (text.length > 0) {
                                video_obj = text[0];
                                $.fancybox({
                                'padding'             : 0,
                                'margin'              : 0,
                                'autoScale'           : false,
                                'transitionIn'        : 'none',
                                'transitionOut'       : 'none',     
                                'overlayColor'        : '#fff',                
                                'hideOnOverlayClick'  : true,
                                'hideOnContentClick'  : false,
                                'title'               : this.title,
                                'width'               : 950,
                                'height'              : 500,
                                'opacity'             : false,
                                'href'                : 'http://www.youtube-nocookie.com/v/'+video_obj.id+'&fs=1',
                                'type'                : 'swf',    // <--add a comma here
                                'swf'                 : {'allowfullscreen':'true', 'wmode':'opaque'} // <-- flashvars here
                                });
                            }
                        }
                    });
                });
                $('span.instaplay').click(function(e) {
                    e.preventDefault();
                    query = $(this).parent().attr('href').replace('http://wn.com/', '').replace('_', ' ');
                    play_obj_clicked = $(this);
                    
                    // search youtube for first match
                    jQuery.ajax({
                        data: {
                            t: 'cheetah-photo-search/mvs_simple_json.txt',
                            orderby: 'published',
                            corpus: 'current',
                            language_id: '1',
                            query: query
                        },
                        dataType: 'jsonp',
                        url: 'http://upge.wn.com/',
                        success: function(text) {
                            // embed the video
                            if (text.length > 0) {
                                video_obj = text[0];
                                video_html_obj = $('<object width="62" height="25" class="instaplay-audioplayer"><param name="movie" value="http://www.youtube-nocookie.com/v/'+video_obj.id+'?hl=en&autoplay=1"></param><param name="allowFullScreen" value="true"></param><param name="allowscriptaccess" value="always"></param><embed src="http://www.youtube-nocookie.com/v/'+video_obj.id+'?hl=en&autoplay=1" type="application/x-shockwave-flash" allowscriptaccess="always" allowfullscreen="true" width="62" height="25"><noembed><a href="http://www.youtube.com/watch?v='+video_obj.id+'?hl=en&autoplay=1">Play</a></noembed></object>');
                                play_obj_clicked.hide();
                                play_obj_clicked.after(video_html_obj);
                            }
                        }
                    });
                    
                    // embed a short variant of it
                });
            });
