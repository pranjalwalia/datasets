    <style type="text/css">
        #map_canvas { height: 400px; width: 300px; border: 1px solid 000; margin: 0px 0px 30px 15px; float: right;}
        .wikiparser h2 a, .wikiparser h3 a, .wikiparser h4 a, .wikiparser h5 a, .wikiparser h6 a {
            color: #0645AD !important;
        }
