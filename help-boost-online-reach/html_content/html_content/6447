    <script type="text/javascript"><!--
    var map=false;
    var geocoder=false;
    var marker=false;
    var infowin=false;
    var locationmap=false;
    function initialize() {
        geocoder = new google.maps.Geocoder();
        var latlng = new google.maps.LatLng(0,0);
        var myOptions = {
          zoom: 1,
          center: latlng,
          mapTypeId: google.maps.MapTypeId.ROADMAP
        };
        var el = document.getElementById("map_canvas");
        if(el){
                map = new google.maps.Map(el, myOptions);
            }
    }

    function geocode_wikipage_add_pano(addr) {
        // geocode the current location
        geocoder = new google.maps.Geocoder();
        geocoder.geocode( { 'address': addr}, function(results, status) {
          if (status == google.maps.GeocoderStatus.OK) {
            // adding the map to the location tab
            var myOpts = {
              zoom: 4,
              center: results[0].geometry.location,
              mapTypeId: google.maps.MapTypeId.ROADMAP
            };
            locationmapdiv = $('<tr><td colspan="2"><div id="location-map" style="width:280px; height: 210px;"></div></td></tr>')
            $('#wiki0 > .wikiparser > div > table').prepend(locationmapdiv);
            locationmap = new google.maps.Map(document.getElementById("location-map"), myOpts);

            // add the panoramio js and the slideshow
            $.getScript('http://www.panoramio.com/wapi/wapi.js?v=1&hl=en', function() {
                // add the html
                panodiv = $('<div id="panoslide"><a href="http://www.panoramio.com">Panoramio - Photos of the World</a></div>')

                // photos should be near results[0].geometry.location
                var myRequest = {
                  'rect': {'sw': {'lat': results[0].geometry.location.lat()-0.1, 'lng': results[0].geometry.location.lng()-0.1}, 'ne': {'lat': results[0].geometry.location.lat()+0.1, 'lng': results[0].geometry.location.lng()+0.1}}
                };
                var myOptions = {
                  'width': 600,
                  'height': 400
                };
                var widget = new panoramio.PhotoWidget('panoslide', myRequest, myOptions);
                widget.setPosition(0);
            });
          }
        });
    }

    // geocode methods
    jQuery(function() {
        $('#related_places a').mouseover(function() {
            l = $(this).html().replace(/^\s*|\s*$/g,'')
            geocoder.geocode( { 'address': l}, function(results, status) {
              if (status == google.maps.GeocoderStatus.OK) {
                if (marker) { marker.setMap(null); }
                if (infowin) { infowin.close(); }
                map.setCenter(results[0].geometry.location);
                marker = new google.maps.Marker({
                    map: map,
                    position: results[0].geometry.location,
                    animation: google.maps.Animation.DROP,
                    title:l
                });
                infowin = new google.maps.InfoWindow({
                    content: '<strong>'+results[0].formatted_address+'</strong><br /><em><a href="http://wn.com/'+l.replace('/\s+/g', '_')+'" title="'+l+'">&raquo; more WN on '+l+'</a></em>'
                });
                infowin.open(map, marker);
                map.setZoom(4);
              } else {
                if (marker) { marker.setMap(null); }
                if (infowin) { infowin.close(); }
                //alert("Could not locate on the map!");
              }
            });
        });
    });
     // -->
</script>
   
    


