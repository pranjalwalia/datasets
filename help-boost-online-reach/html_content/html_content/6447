		 // tabbed content
		    $('.people_tab').hide();			
            $('.places_tab').hide();			
            $('.suggestions_tab').hide();	
			$(".tab-menu > li").click(function(e){			                                        

				switch(e.target.id){
					case "related_tab":
						//change status & style menu
						$("#related_tab").addClass("active");
						$("#places_tab").removeClass("active");
						$("#people_tab").removeClass("active");
						$("#suggestions_tab").removeClass("active");
						//display selected division, hide others
						$("div.related_tab").fadeIn();
						$("div.places_tab").css("display", "none");
						$("div.people_tab").css("display", "none");
						$("div.suggestions_tab").css("display", "none");
					break;
					case "places_tab":
						//change status & style menu
						$("#places_tab").addClass("active");
						$("#people_tab").removeClass("active");
						$("#related_tab").removeClass("active");
						$("#suggestions_tab").removeClass("active");
						//display selected division, hide others
						$("div.places_tab").fadeIn();
						$("div.people_tab").css("display", "none");
						$("div.related_tab").css("display", "none");
						$("div.suggestions_tab").css("display", "none");
					break;
					case "people_tab":
						//change status & style menu
						$("#places_tab").removeClass("active");
						$("#people_tab").addClass("active");				
						$("#related_tab").removeClass("active");			
						$("#suggestions_tab").removeClass("active");
						//display selected division, hide others
						$("div.people_tab").fadeIn();
						$("div.places_tab").css("display", "none");
						$("div.related_tab").css("display", "none");
						$("div.suggestions_tab").css("display", "none");
					break;
					case "suggestions_tab":
						//change status & style menu
						$("#places_tab").removeClass("active");
						$("#people_tab").removeClass("active");				
						$("#related_tab").removeClass("active");			
						$("#suggestions_tab").addClass("active");
						//display selected division, hide others
						$("div.people_tab").css("display", "none");
						$("div.places_tab").css("display", "none");
						$("div.related_tab").css("display", "none");
						$("div.suggestions_tab").fadeIn();
					break;
				}
				return false;
			});
			$("#people_tab").click(function(){
				$('#tabcontainer').animate({height: $('.people_tab').css('height')},200);
			})    
			$("#places_tab").click(function(){
				$('#tabcontainer').animate({height: $('.places_tab').css('height')},200);
			})    
			$("#related_tab").click(function(){
