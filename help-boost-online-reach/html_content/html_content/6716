		r = 'C4|20130425|SPORTS01|304250161|PH'.split('|'),
		_uri = 'Avis='+r[0]+'&Dato='+r[1]+'&Kategori='+r[2]+'&Lopenr='+r[3]+'&Ref='+r[4]
	;
	if (GEL.env.url.qsv.nocache == 1){
		_uri = _uri+'&nocache=1';
	};
	_xhr.open('get', '/galleryobject?'+_uri);
	_xhr.onreadystatechange = function () {
		if (_xhr.readyState === 4) {
			GEL.thepage.pageinfo.galleryobject=JSON.parse(_xhr.responseText);
			var 
				_d = document,
				_h = _d.createElement('h6'),
				_id = GEL.ement('ody-gallerylinkimg').element,
