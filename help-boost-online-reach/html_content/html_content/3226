    <script type="text/javascript">
        $(document).ready(function() {
            document.getElementById('emailtoafriendsrc').value = 'http://contactimporter.mercola.com/EmailArticle.aspx?source=emailtoafriend&title=' + encodeURIComponent(document.title) + '&url=' + encodeURI(document.location);

            document.getElementById('facebook').src = 'http://www.facebook.com/plugins/like.php?href=' + encodeURI(document.location) + '&amp;layout=standard&amp;show_faces=false&amp;width=450&amp;action=like&amp;font=arial&amp;colorscheme=light';

            var scripts = document.getElementsByTagName('script');
            for (var i = 0; i < scripts.length; i++) {
                if (scripts[i].src == "http://www.stumbleupon.com/hostedbadge.php?s=1&r=") {
                    scripts[i].src += "href=" + encodeURI(document.location);
                    break;
                }
            }
        });       
