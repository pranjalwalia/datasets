 <script type="text/javascript">
 strurlpath= 'referenceclick.asp' 
 
if(jQuery(document).height()<=1000){
jQuery("#navigation").animate({right:"0px"},300);
}

 //store reference clicktrack start 
 var link;
 jQuery(window).ready(function () { 
 jQuery("#navigation").find("a").click(function (){
 link = $(this).attr("href");  
 jQuery.ajax({
  url: strurlpath,
  data: "newsid=89932&goto="+link,
 complete: function(XMLHttpRequest, textStatus) {
    top.location.href=link;
  }
}); 
 
 return false;
 });
 //store reference clicktrack ends 
            shownav=true; 
            setInterval("scrollingDetector()",100) 
            })
           function scrollingDetector() {
            var a=navigator.appName=="Microsoft Internet Explorer"?document.documentElement.scrollTop:window.pageYOffset;
            var b = jQuery(window).height();
            
            
            if ( jQuery.browser.msie && jQuery.browser.version <=6) {
                jQuery("#navigation").css("top",b+a-jQuery("#navigation").height() + "px");
               
            }
            if (((a>500 || jQuery(document).height()<=500)) && (shownav))
            {
                 jQuery("#navigation:hidden").show(0).animate({right:"0px"},150);
            }
            else
            {
                jQuery("#navigation:visible").animate({right:"-100px"},150).hide(0); 
            }
        } ;
        
        jQuery("#navigationClose").bind("click",function()
        {
            if(jQuery("#navigationClose").hasClass("min"))
            {
                jQuery("#navigationClose").removeClass("min").addClass("max");
                jQuery("#navigation").find("#cont").hide(0);
                //jQuery("#navigation").hide();
            }
            else
            {
                jQuery("#navigationClose").removeClass("max").addClass("min");
                jQuery("#navigation").find("#cont").show(0);

            }
        
        });
        
</script>
