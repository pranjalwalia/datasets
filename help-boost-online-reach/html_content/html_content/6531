<script type="text/javascript">
 

function showWt(parentId, parentShow, paretCount, objId){
    for(i=1;i<=paretCount;i++){
		if(i==objId){
		   	document.getElementById(parentShow+i).style.display = "block";
			document.getElementById(parentId+i).className = "widTabact";
		}else{
		    
		   	document.getElementById(parentShow+i).style.display = "none";
			document.getElementById(parentId+i).className = "";
		}
	}
}
</script>
 
 
 
