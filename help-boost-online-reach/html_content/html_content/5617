	    	//flashWrite(strMovie, divName, divLayer, intWidth, intHeight, hexBgcolor, strId, strAlign, intFlashVersion, strPlay, strLoop, strMenu, auxVars)
	    	
	    	
	    	if (typeof CN.customEvents === "undefined") {
	    	   CN.customEvents = {};
            }
		    

		    jQuery(window).bind('CN.customEvents.noflash',function(event){ 
		      jQuery.getScript('/scripts/jquery.tab.js', function() {
		         jQuery.getScript('/scripts/cn.rotator.js', function() {
		          jQuery("#flashcontent").children().not('.numberedtoggle').remove();
		          CN.rotator.init({url:'/images/home/recipesmenus/_xml/admin.xml', target:'#flashcontent'});
                  });
              });
		    })

		    		    
		    flashWrite('/images/home/recipesmenus/_swf/recipes.swf', 'flashcontent', '', 450, 266, '#ffffff', '', '', 8, '', '', '', 'base=/images/home/recipesmenus/&admin_xml=./_xml/admin.xml');
    	</script>
    </div>
</div>










