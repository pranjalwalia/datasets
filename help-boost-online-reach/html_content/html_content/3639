                    if(!("Media" in YAHOO)){YAHOO.Media = {};}
                    if(!("Ugccmt" in YAHOO.Media)){YAHOO.Media.ugccmt = {};}
                    Y.namespace("Media");
                    YAHOO.Media.ugccmt.comments = new Y.Media.UGCComments(ugccmt_data2);

                    if(YAHOO.Updates != undefined){
