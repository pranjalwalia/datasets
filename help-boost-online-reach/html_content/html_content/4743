opts = 'status=1' +
',width=' + width +
',height=' + height +
',top=' + top +
',left=' + left;
var newwindow = window.open("http://twitter.com/?status=Videos%20show%20how%20to%20turn%20an%20old%20television%20into%20a%20solar-powered%20death%20ray%20that%20will%20burn%20through...%20http://bit.ly/10DA4gk%20via%20@MailOnline", "", opts);
}
function postToPlus() {
DM.Omniture.connectCallback('plusone_vote', 'sciencetech', { "href" : "http://www.dailymail.co.uk/sciencetech/article-2291661/Videos-turn-old-television-solar-powered-death-ray-burn-path-2-000F.html" });
DM.ted.tedEvent('social-event', {'social-event' :'plusone-vote'});
var width = 575,
height = 400,
left = (window.outerWidth - width) / 2,
top = (window.outerHeight - height) / 2,
url = this.href,
opts = 'status=1' +
',width=' + width +
',height=' + height +
',top=' + top +
',left=' + left;
var newwindow = window.open("http://plus.google.com/share?url=http://www.dailymail.co.uk/sciencetech/article-2291661/Videos-turn-old-television-solar-powered-death-ray-burn-path-2-000F.html", "", opts);
}
</script>
<script type="text/javascript">
DM.has('socialLinks', 'comScore', {
mo_ev_ty: 'share',
mo_ev_pl: 'bottom',
del: 'a.js-tl'
});
</script>
</div>
