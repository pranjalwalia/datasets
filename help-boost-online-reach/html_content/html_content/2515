	jQuery('#jcarousel-next-horizontal_1355953983de07f84f95').live('click', function(){
		var slide = parseInt(jQuery('#slide_1355953983de07f84f95').html());
		if(++slide > 1) slide = 1;
		jQuery('#slide_1355953983de07f84f95').html(slide);
		if(jQuery('#carousel_1355953983de07f84f95').outerHeight(true) < jQuery('#carousel_1355953983de07f84f95 .jcarousel-item-' + slide).outerHeight(true))
			jQuery('#carousel_1355953983de07f84f95').animate({
				height: jQuery('#carousel_1355953983de07f84f95 .jcarousel-item-' + slide).outerHeight(true)
			});
		jQuery('#carousel_1355953983de07f84f95').animate({
			marginLeft: 0-300
		}, 300, function(){
			var a = jQuery(this).children('li:first').detach();
			jQuery(this).css({
				marginLeft: 0
			}).append(a);
		});
		jQuery('#carousel_1355953983de07f84f95').animate({
			height: jQuery('#carousel_1355953983de07f84f95 .jcarousel-item-' + slide).outerHeight(true)
		});
	});
	jQuery('#jcarousel-prev-horizontal_1355953983de07f84f95').live('click', function(){
		var slide = parseInt(jQuery('#slide_1355953983de07f84f95').html());
		if(--slide < 1) slide = 1;
		jQuery('#slide_1355953983de07f84f95').html(slide);
		if(jQuery('#carousel_1355953983de07f84f95').outerHeight(true) < jQuery('#carousel_1355953983de07f84f95 .jcarousel-item-' + slide).outerHeight(true))
			jQuery('#carousel_1355953983de07f84f95').animate({
				height: jQuery('#carousel_1355953983de07f84f95 .jcarousel-item-' + slide).outerHeight(true)
			});
		var a = jQuery('#carousel_1355953983de07f84f95').children('li:last').detach();
