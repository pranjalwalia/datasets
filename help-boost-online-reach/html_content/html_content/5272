					     	if (number_google_ads <= 1) { document.write('<ul class="sponsor_partner_single">'); }
							else { document.write('<ul id="sponsor_partner">'); }
							for (i = 0; i < google_ads.length; ++i) {
								document.write('<li><h4 class="sponsor_header"><a href="' + google_ads[i].url + '" onMouseOver="return GOOGLEShowStatusInfo(\'' + escape(google_ads[i].visible_url) + '\')">' + google_ads[i].line1 + '</a></h4>');
								document.write('<span class="sponsor_text"><a href="' + google_ads[i].url + '" onMouseOver="return GOOGLEShowStatusInfo(\'' + escape(google_ads[i].visible_url) + '\')">' + google_ads[i].line2 + '<span class="sponsor_text_line2"> ' + google_ads[i].line3 + '</span></a></span>');
								document.write('<span class="sponsor_footer"><a href="' + google_ads[i].url + '" onMouseOver="return GOOGLEShowStatusInfo(\'' + escape(google_ads[i].visible_url) + '\')">' + google_ads[i].visible_url + '</a></span>');
								document.write('</li>');
							}
							document.write('</ul></div>');
						}
					}
			</script>
	
