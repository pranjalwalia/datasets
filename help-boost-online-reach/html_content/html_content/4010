<script type="text/javascript">
    
    function registerGetBlogUrls() {
      gadgets.rpc.register('getBlogUrls', function() {
        var holder = {};
        
          
          
          
            holder.postFeed = "http://www.blogger.com/feeds/3481298569887767442/posts/default";
          
          
          
            holder.commentFeed = "http://www.blogger.com/feeds/3481298569887767442/comments/default";
          
          holder.currentBlogUrl = "http://elegantlywastedd.blogspot.com/";
          holder.currentBlogId = "3481298569887767442";
        
        return holder;
      });
    }
  </script>
