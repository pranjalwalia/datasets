(function(){
	var Dom = YAHOO.util.Dom,
		el = Dom.get('signinbox');
	if (YAHOO.util.Cookie.get("at") !== null) {
		YAHOO.util.Connect.asyncRequest('GET', '/sso/ajaxstatus', {
