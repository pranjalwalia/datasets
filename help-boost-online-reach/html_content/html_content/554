		if (dmJQuery.browser.msie && dmJQuery.browser.version < 8) {
			dmJQuery("#pluck_user_miniPersona_dialog_350409 > div.pluck-user-mp-wrap").addClass("pluck-user-mp-wrap-ie7");
		}
		(function($, jQuery) {
			if (typeof(pluckAppProxy.pluck_user_miniPersona_dialog_display) === 'undefined') {
				pluckAppProxy.pluck_user_miniPersona_dialog_display = function(dialog, obj) {
					if (!dialog.hasClass("pluckShown")) {
						dialog.addClass("pluckShown");
						if ($.pluckDisplayCorners()) {
							var allCorners = {tl: { radius: 5 }, tr: { radius: 5 }, bl: { radius: 5 }, br: { radius: 5 }, antiAlias: true, autoPad: false, validTags: ["div"] };
							$("#" + dialog.attr("id") + " > div.pluck-user-mp-wrap").pluckCorner(allCorners);
						} else {
							$("#" + dialog.attr("id") + " > div.pluck-user-mp-wrap").addClass("pluck-user-mp-wrap-nocorners");
						}
						if (typeof($.fn.qtip) !== 'undefined' && $.support.boxModel && !($.browser.msie && $.browser.version < 7)) {
							var qtipInfo = $("#" + dialog.attr("id") + ' > .pluck-user-mp-qtip-style');
							var qtipStyle = { background: qtipInfo.css("background-color"), tip: 'topMiddle', padding: qtipInfo.css("padding-left"), color: qtipInfo.css("color"),
