<script type="text/javascript" charset="utf-8">
window.hawkeye_ids = (function() {
    var hawkeye = {};
        hawkeye.wlOrd = new Date().getTime();
        hawkeye.publisher_id     = 1568;
        hawkeye.website_id       = 5852;
        hawkeye.content_unit_id  = 9975;
        hawkeye.content_unit_ids = '9975,9976';
        hawkeye.url              = 'req.connect.wunderloop.net/AP/' + hawkeye.publisher_id + '/' + hawkeye.website_id + '/' 
                                        + hawkeye.content_unit_id + '/js?cus=' + hawkeye.content_unit_ids 
                                        + '&ord=' + hawkeye.wlOrd;
        hawkeye.protocol = ('https:' === document.location.protocol) ? 'https://' : 'http://';
        hawkeye.final = unescape("%3Cscript src='" + hawkeye.protocol + hawkeye.url + "' type='text/javascript'%3E%3C/script%3E");
        document.write(hawkeye.final);
        
        // get the variables returned back from the script
    hawkeye.content_unit_ids = hawkeye.content_unit_ids.split(',');
    return hawkeye.content_unit_ids;
}());

(function() {
  if (window.hawkeye_ids) {
    var key = '', i = 0, content_unit_id_name = '';
    for (i=0; i < window.hawkeye_ids.length; i+=1) {
      content_unit_id_name = 'wl' + window.hawkeye_ids[i] + 'camp';
      if ('' === window[content_unit_id_name]) {
        key += window.hawkeye_ids[i] + '-no';
      } else {
        key += window.hawkeye_ids[i] + '-yes';
      }
      // don't add a + if this is the last of the key to be processed
      (i !== (window.hawkeye_ids.length - 1)) ? key += '+': '';
    }
    window.hawkeye_ids = key;
    ipc.adtech.pushParams({
        'name': 'hawkeye_ids',
        'placement': 'before',
        'order': 5, 'fn': function(val) {
          if (!val) {
            val = '';
          }
          return val;
        }});
  }
}());
