	google_skip = 0;
	function google_ad_request_done(results) {
		if (results.length == 0) {
			return;
		}
		google_skip = (google_skip + results.length);
		var output = "<div class=\"googleBorder\" style=\"border: 1px solid #cccccc; background: #ffffff; padding: 0.5em\"><p class=\"googleTitle\" style=\"margin: 0 0 0.5em 0; font-size: 12px; line-height: normal; color: #333333\"><a href=\""+google_info.feedback_url+"\" style=\"color:#000000;font-size: 12px; margin-left:5px;\">Ads by Google</a></p>";
		for(var i = 0; i < results.length; i++) {
			output += "<p style=\"margin: 5px; padding: 0; font-size:11px; line-height: normal\">";
			switch(results[i].type.toLowerCase()) {
				case "text":
					output += "<span class=\"googleLink1\"><a href=\"" + results[i].url + "\" target=\"_blank\" onmouseover=\"window.status='" + results[i].visible_url + "';return true\" onmouseout=\"window.status=''\" style=\"text-decoration: underline; line-height:normal; color: #FF2084\">" + results[i].line1 + "</a></span><br /><span class=\"googleDescription\" style=\"color: #333333\">" + results[i].line2 + " " + results[i].line3 + "</span><br /><span class=\"googleLink2\"><a href=\"" + results[i].url + "\" target=\"_blank\" onmouseover=\"window.status='" + results[i].visible_url + "';return true\" onmouseout=\"window.status=''\" style=\"text-decoration: none; font-size:11px; line-height:normal; color: #FF2084\">" + results[i].visible_url + "</a></span>";
					break;
				case "image":
					output += "<a href=\"" + results[i].url + "\" target=\"_blank\" onmouseover=\"window.status='" + results[i].visible_url + "';return true\" onmouseout=\"window.status=''\"><img src=\"" + results[i].image_url + "\" width=\"" + results[i].image_width + "\" height=\"" + results[i].image_height + "\" alt=\"\" title=\"Go to " + results[i].visible_url + "\" /></a>";
					break;
				case "flash":
					output += "<object classid=\"clsid:D27CDB6E-AE6D-11cf-96B8-444553540000\" codebase=\"http://download.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version=6,0,0,0\" width=\"" + results[i].image_width + "\" height=\"" + results[i].image_height + "\"><param name=\"movie\" value=\"" + results[i].image_url + "\"><param name=\"quality\" value=\"high\"><param name=\"AllowScriptAccess\" value=\"never\"><embed src=\"" + results[i].image_url + "\" width=\"" + results[i].image_width + "\" height=\"" + results[i].image_height + "\" type=\"application/x-shockwave-flash\" AllowScriptAccess=\"never\" pluginspace=\"http://www.macromedia.com/go/getflashplayer\"></embed></object>";
					break;
			}
			output += "</p>";
		}
		output += "</div>";
		document.write(output);
	}
