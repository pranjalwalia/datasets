<script language="Javascript">

var e2_on_the_case_contentgroup_115_two_by_oneSliderHandler = new LRSliderHandler();

e2_on_the_case_contentgroup_115_two_by_oneSliderHandler.thisSliderElementName = 'e2_on_the_case_contentgroup_115_two_by_oneSliderHandler';
e2_on_the_case_contentgroup_115_two_by_oneSliderHandler.sliderElementCount = 1;
e2_on_the_case_contentgroup_115_two_by_oneSliderHandler.sliderElementSize = 256;
e2_on_the_case_contentgroup_115_two_by_oneSliderHandler.sliderObjectViewsize = 1;
e2_on_the_case_contentgroup_115_two_by_oneSliderHandler.sliderRightLimit = e2_on_the_case_contentgroup_115_two_by_oneSliderHandler.sliderElementSize * ((e2_on_the_case_contentgroup_115_two_by_oneSliderHandler.sliderElementCount - e2_on_the_case_contentgroup_115_two_by_oneSliderHandler.sliderObjectViewsize) * -1) + 1;
e2_on_the_case_contentgroup_115_two_by_oneSliderHandler.currentSliderElement = 1;
e2_on_the_case_contentgroup_115_two_by_oneSliderHandler.sliderObjectId = 'e2_on_the_case_contentgroup_115_two_by_one_outside';
e2_on_the_case_contentgroup_115_two_by_oneSliderHandler.sliderDoSlide = false;

var e2_on_the_case_contentgroup_115_two_by_oneLeftArrowButtonMouseoutImgSrc = 'http://img.deusm.com/enterpriseefficiency/eetv_leftarrow.gif';
var e2_on_the_case_contentgroup_115_two_by_oneRightArrowButtonMouseoutImgSrc = 'http://img.deusm.com/enterpriseefficiency/eetv_rightarrow.gif';

e2_on_the_case_contentgroup_115_two_by_oneSliderHandler.LRSliderNavGraphicsHandlerSlideLeft = function() {
	// now deal with the images
	
	var currentImage = GetObject('e2_on_the_case_contentgroup_115_two_by_one_rightarrowbutton');
	// if we're at currentSliderElement < sliderElementCount make sure the right button is active
	if (this.currentSliderElement < this.sliderElementCount) { 
		currentImage.src = 'http://img.deusm.com/enterpriseefficiency/eetv_rightarrow_on.gif';
	} else {
		currentImage.src = 'http://img.deusm.com/enterpriseefficiency/eetv_rightarrow.gif';
	}
	e2_on_the_case_contentgroup_115_two_by_oneRightArrowButtonMouseoutImgSrc = currentImage.src;

	// if sliding left, change when currentSliderElement gets to 1
	currentImage = GetObject('e2_on_the_case_contentgroup_115_two_by_one_leftarrowbutton');
	if (this.currentSliderElement == 1) { 
		currentImage.src = 'http://img.deusm.com/enterpriseefficiency/eetv_leftarrow.gif';
	} else {
		currentImage.src = 'http://img.deusm.com/enterpriseefficiency/eetv_leftarrow_on.gif';
	}
	e2_on_the_case_contentgroup_115_two_by_oneLeftArrowButtonMouseoutImgSrc = currentImage.src;
	
}

e2_on_the_case_contentgroup_115_two_by_oneSliderHandler.LRSliderNavGraphicsHandlerSlideRight = function() {
	// now deal with the images
	
	var currentImage = GetObject('e2_on_the_case_contentgroup_115_two_by_one_leftarrowbutton');
	// if we're at currentSliderElement > 1 make sure the left button is active
	if (this.currentSliderElement > 1) { 
		currentImage.src = 'http://img.deusm.com/enterpriseefficiency/eetv_leftarrow_on.gif';
	} else {
		currentImage.src = 'http://img.deusm.com/enterpriseefficiency/eetv_leftarrow.gif';
	}
	e2_on_the_case_contentgroup_115_two_by_oneLeftArrowButtonMouseoutImgSrc = currentImage.src;
	
	currentImage = GetObject('e2_on_the_case_contentgroup_115_two_by_one_rightarrowbutton');
	// sliding right, change when currentSliderElement gets to this.sliderElementCount
	if (this.currentSliderElement == this.sliderElementCount) {
		currentImage.src = 'http://img.deusm.com/enterpriseefficiency/eetv_rightarrow.gif';
	} else {
		currentImage.src = 'http://img.deusm.com/enterpriseefficiency/eetv_rightarrow_on.gif';
	}
	e2_on_the_case_contentgroup_115_two_by_oneRightArrowButtonMouseoutImgSrc = currentImage.src;
}

</script>

