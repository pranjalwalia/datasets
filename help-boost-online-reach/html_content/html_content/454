/* Plugin Config */
function s_doPlugins(s)
{

	/* Plugin: getQueryParam 2.3 These are the Campaign Tracking Code values -- UTM codes are for thinkhdi.com*/
	s.campaign=s.getQueryParam('cid,kw,utm_campaign,utm_medium,utm_source,_mc,source,f_src',':');
	s.campaign=s.campaign.toLowerCase();
	
	/* Plugin: getInternalQueryParam These are the internal tracking codes*/
 	s.prop18=s.getInternalQueryParam('itc');
	s.prop18=s.prop18.toLowerCase();

	/* Plugin: getInternalQueryParam These are the source code and key code reports*/
 	s.evar22=s.getInternalQueryParam('sourceCode');
	s.evar22=s.evar22.toLowerCase();

	/* Plugin: getQueryParam 2.3 set s.prop1 to q= parameter for internal search q=google search results, queryText=verity, search_text=gama, searchcontent=crn, pro.whitepaper searches are search=xyz */
	s.prop1=s.getQueryParam('q,queryText,keyword,query,search,search_text,searchContent',':');
	s.prop1=s.prop1.toLowerCase();

	/* Plugin: getQueryParam 2.3 set s.evar25 to f= parameter for formid */
	s.eVar26=s.getQueryParam('f,formID',':');

	/* Plugin: getNewRepeat 1.0 */
	s.prop17=s.getNewRepeat();

	/*Set prop10=evar7 so can correlate custom insight traffic reports to internal campaign id*/
	if(!s.evar7)
	s.eVar7=s.prop18;

	/*Set evar17=prop17 so that Campaigns report values can be correlated to Visitor Status*/
	if(!s.evar17)
	s.eVar17=s.prop17;

	/*Set evar2=document.title*/
	s.eVar2=document.title;


	/*Set evar32=pageType to populate the Page Not Found Report in v15 3/6/2012es*/
	s.eVar32=s.pageType;


	/*Set evar1=prop5 so that Campaigns report values can be correlated to Site Section*/
	if(!s.evar1)
	s.eVar1=s.prop5;
/*
 * Set Value for Author Name | Headline report. 9sept2010
 */
	s.prop15=s.prop7 + " | " + s.prop4;

/* Set Evar value to Author|Headline for correlation to Campaign IDs 9sept2010*/
	s.eVar24=s.prop15;

/* Set Evar value to Author name for correlation to Campaign IDs 3/22/11ES*/
	s.eVar28=s.prop7;


/* Set evar value to Location.Pathname (url minus query strings) */
	
	s.eVar14=location.pathname;

/* Set evar value to ReportID for teardown.com */
	s.eVar15=s.getQueryParam('reportid,reportkey');

/*
 * Plugin: Days since last Visit 1.1 - capture time from last visit
 */
	s.prop22=s.getDaysSinceLastVisit('s_lv');

/*
 * Plugin: GetTimeParting Configuration ES20110125 to GMT-5 / East coast
 */
s.prop23=s.getTimeParting('h','-5'); // Set hour GMT -5 is East Coast
s.prop24=s.getTimeParting('d','-5'); // Set day 
s.prop25=s.getTimeParting('w','-5'); // Set weekday

/* User segmentation */
	s.getUserSegment("us_ubm");    //this is the name of the cookie.
				      //us stands for User Segmentation. ubm stands for united business media
	                              //you can change the name of the cookie to whatever you find appropriate

s.eVar27=s.prop9;

}
/* End Plugin Config */
s.doPlugins=s_doPlugins

/************************** PLUGINS SECTION *************************/
/* You may insert any plugins you wish to use here.                 */

/*
 * Plugin: Days since last Visit 1.1 - capture time from last visit
 */
s.getDaysSinceLastVisit=new Function("c",""
+"var s=this,e=new Date(),es=new Date(),cval,cval_s,cval_ss,ct=e.getT"
+"ime(),day=24*60*60*1000,f1,f2,f3,f4,f5;e.setTime(ct+3*365*day);es.s"
+"etTime(ct+30*60*1000);f0='Cookies Not Supported';f1='First Visit';f"
+"2='More than 30 days';f3='More than 7 days';f4='Less than 7 days';f"
+"5='Less than 1 day';cval=s.c_r(c);if(cval.length==0){s.c_w(c,ct,e);"
+"s.c_w(c+'_s',f1,es);}else{var d=ct-cval;if(d>30*60*1000){if(d>30*da"
+"y){s.c_w(c,ct,e);s.c_w(c+'_s',f2,es);}else if(d<30*day+1 && d>7*day"
+"){s.c_w(c,ct,e);s.c_w(c+'_s',f3,es);}else if(d<7*day+1 && d>day){s."
+"c_w(c,ct,e);s.c_w(c+'_s',f4,es);}else if(d<day+1){s.c_w(c,ct,e);s.c"
+"_w(c+'_s',f5,es);}}else{s.c_w(c,ct,e);cval_ss=s.c_r(c+'_s');s.c_w(c"
+"+'_s',cval_ss,es);}}cval_s=s.c_r(c+'_s');if(cval_s.length==0) retur"
+"n f0;else if(cval_s!=f1&&cval_s!=f2&&cval_s!=f3&&cval_s!=f4&&cval_s"
+"!=f5) return '';else return cval_s;");


/*
 * Plugin: getQueryParam 2.3 - this is to populate the (external) tracking code reports
 */
s.getQueryParam=new Function("p","d","u",""
+"var s=this,v='',i,t;d=d?d:'';u=u?u:(s.pageURL?s.pageURL:s.wd.locati"
+"on);if(u=='f')u=s.gtfs().location;while(p){i=p.indexOf(',');i=i<0?p"
+".length:i;t=s.p_gpv(p.substring(0,i),u+'');if(t){t=t.indexOf('#')>-"
+"1?t.substring(0,t.indexOf('#')):t;}if(t)v+=v?d+t:t;p=p.substring(i="
+"=p.length?i:i+1)}return v");
s.p_gpv=new Function("k","u",""
+"var s=this,v='',i=u.indexOf('?'),q;if(k&&i>-1){q=u.substring(i+1);v"
+"=s.pt(q,'&','p_gvf',k)}return v");
s.p_gvf=new Function("t","k",""
+"if(t){var s=this,i=t.indexOf('='),p=i<0?t:t.substring(0,i),v=i<0?'T"
+"rue':t.substring(i+1);if(p.toLowerCase()==k.toLowerCase())return s."
+"epa(v)}return ''");


/*
 * Plugin: getInternalQueryParam - this is to grab the itc string from the url to populate the internal tracking code report.
 */
s.getInternalQueryParam=new Function("p","d","u",""
+"var s=this,v='',i,t;d=d?d:'';u=u?u:(s.pageURL?s.pageURL:s.wd.locati"
+"on);if(u=='f')u=s.gtfs().location;while(p){i=p.indexOf(',');i=i<0?p"
+".length:i;t=s.p_gpv(p.substring(0,i),u+'');if(t){t=t.indexOf('#')>-"
+"1?t.substring(0,t.indexOf('#')):t;}if(t)v+=v?d+t:t;p=p.substring(i="
+"=p.length?i:i+1)}return v");
s.p_gpv=new Function("k","u",""
+"var s=this,v='',i=u.indexOf('?'),q;if(k&&i>-1){q=u.substring(i+1);v"
+"=s.pt(q,'&','p_gvf',k)}return v");
s.p_gvf=new Function("t","k",""
+"if(t){var s=this,i=t.indexOf('='),p=i<0?t:t.substring(0,i),v=i<0?'T"
+"rue':t.substring(i+1);if(p.toLowerCase()==k.toLowerCase())return s."
+"epa(v)}return ''");


/*
 * Plugin: getNewRepeat 1.0 - Return whether user is new or repeat
 */
s.getNewRepeat=new Function(""
+"var s=this,e=new Date(),cval,ct=e.getTime(),y=e.getYear();e.setTime"
+"(ct+30*24*60*60*1000);cval=s.c_r('s_nr');if(cval.length==0){s.c_w("
+"'s_nr',ct,e);return 'New';}if(cval.length!=0&&ct-cval<30*60*1000){s"
