<div style="float: right; width: 40px; height: 144px; margin-left: 0px;"><a href="Javascript: eetvrtcolumngeneralSliderHandler.LRSliderSlideRight(1);" onmouseout="MM_swapImage('eetvrtcolumngeneral_rightarrowbutton','',eetvrtcolumngeneralRightArrowButtonMouseoutImgSrc,1)" onmouseover="MM_swapImage('eetvrtcolumngeneral_rightarrowbutton','','http://img.deusm.com/enterpriseefficiency/eetv_rightarrow.gif',1)"><img name="eetvrtcolumngeneral_rightarrowbutton" id="eetvrtcolumngeneral_rightarrowbutton" src="http://img.deusm.com/enterpriseefficiency/eetv_rightarrow_on.gif" border="0" width="40" height="144"></a></div>
<div style="float: left; width: 40px; height: 144px; margin-left: 0px;"><a href="Javascript: eetvrtcolumngeneralSliderHandler.LRSliderSlideLeft(1);" onmouseout="MM_swapImage('eetvrtcolumngeneral_leftarrowbutton','',eetvrtcolumngeneralLeftArrowButtonMouseoutImgSrc,1)" onmouseover="MM_swapImage('eetvrtcolumngeneral_leftarrowbutton','','http://img.deusm.com/enterpriseefficiency/eetv_leftarrow_on.gif',1)"><img name="eetvrtcolumngeneral_leftarrowbutton" id="eetvrtcolumngeneral_leftarrowbutton" src="http://img.deusm.com/enterpriseefficiency/eetv_leftarrow.gif" border="0" width="40" height="144"></a></div>

<div style="float: left; display: inline; background: url(http://img.deusm.com/enterpriseefficiency/eetv_2xbg.gif) no-repeat top left; width: 256px">


<script language="Javascript">

var eetvrtcolumngeneralSliderHandler = new LRSliderHandler();

eetvrtcolumngeneralSliderHandler.thisSliderElementName = 'eetvrtcolumngeneralSliderHandler';
eetvrtcolumngeneralSliderHandler.sliderElementCount = 2;
eetvrtcolumngeneralSliderHandler.sliderElementSize = 256;
eetvrtcolumngeneralSliderHandler.sliderObjectViewsize = 1;
eetvrtcolumngeneralSliderHandler.sliderRightLimit = eetvrtcolumngeneralSliderHandler.sliderElementSize * ((eetvrtcolumngeneralSliderHandler.sliderElementCount - eetvrtcolumngeneralSliderHandler.sliderObjectViewsize) * -1) + 1;
eetvrtcolumngeneralSliderHandler.currentSliderElement = 1;
eetvrtcolumngeneralSliderHandler.sliderObjectId = 'eetvrtcolumngeneral_outside';
eetvrtcolumngeneralSliderHandler.sliderDoSlide = false;

var eetvrtcolumngeneralLeftArrowButtonMouseoutImgSrc = 'http://img.deusm.com/enterpriseefficiency/eetv_leftarrow.gif';
var eetvrtcolumngeneralRightArrowButtonMouseoutImgSrc = 'http://img.deusm.com/enterpriseefficiency/eetv_rightarrow_on.gif';

eetvrtcolumngeneralSliderHandler.LRSliderNavGraphicsHandlerSlideLeft = function() {
	// now deal with the images
	
	var currentImage = GetObject('eetvrtcolumngeneral_rightarrowbutton');
	// if we're at currentSliderElement < sliderElementCount make sure the right button is active
	if (this.currentSliderElement < this.sliderElementCount) { 
		currentImage.src = 'http://img.deusm.com/enterpriseefficiency/eetv_rightarrow_on.gif';
	} else {
		currentImage.src = 'http://img.deusm.com/enterpriseefficiency/eetv_rightarrow.gif';
	}
	eetvrtcolumngeneralRightArrowButtonMouseoutImgSrc = currentImage.src;

	// if sliding left, change when currentSliderElement gets to 1
	currentImage = GetObject('eetvrtcolumngeneral_leftarrowbutton');
	if (this.currentSliderElement == 1) { 
		currentImage.src = 'http://img.deusm.com/enterpriseefficiency/eetv_leftarrow.gif';
	} else {
		currentImage.src = 'http://img.deusm.com/enterpriseefficiency/eetv_leftarrow_on.gif';
	}
	eetvrtcolumngeneralLeftArrowButtonMouseoutImgSrc = currentImage.src;
	
}

eetvrtcolumngeneralSliderHandler.LRSliderNavGraphicsHandlerSlideRight = function() {
	// now deal with the images
	
	var currentImage = GetObject('eetvrtcolumngeneral_leftarrowbutton');
	// if we're at currentSliderElement > 1 make sure the left button is active
	if (this.currentSliderElement > 1) { 
		currentImage.src = 'http://img.deusm.com/enterpriseefficiency/eetv_leftarrow_on.gif';
	} else {
		currentImage.src = 'http://img.deusm.com/enterpriseefficiency/eetv_leftarrow.gif';
	}
	eetvrtcolumngeneralLeftArrowButtonMouseoutImgSrc = currentImage.src;
	
	currentImage = GetObject('eetvrtcolumngeneral_rightarrowbutton');
	// sliding right, change when currentSliderElement gets to this.sliderElementCount
	if (this.currentSliderElement == this.sliderElementCount) {
		currentImage.src = 'http://img.deusm.com/enterpriseefficiency/eetv_rightarrow.gif';
	} else {
		currentImage.src = 'http://img.deusm.com/enterpriseefficiency/eetv_rightarrow_on.gif';
	}
	eetvrtcolumngeneralRightArrowButtonMouseoutImgSrc = currentImage.src;
}

</script>

