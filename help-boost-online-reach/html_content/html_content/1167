//<![CDATA[
    jQuery(function($) {
        $('a[href!=#]').click(function() {
    sendEvent("countdown");
  });
  
  var callOnce = true;
  $(window).scroll(function() {
    var scrollTop = $(this).scrollTop();
    if (scrollTop > 700 && callOnce) {
      sendEvent("countdown");
      callOnce = false;
    }
  });
