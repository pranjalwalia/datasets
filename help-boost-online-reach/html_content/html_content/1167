<script charset="utf-8" type="text/javascript">
//<![CDATA[
  function load_ad_iframes() {
    jQuery(function($) {
      $(ad_array).each(function(index, elm) {
        var ad_iframe = document.createElement('iframe');
        $(ad_iframe).attr({
          noresize: true,
          scrolling: 'no',
          hspace: 0,
          vspace: 0,
          frameborder: 0,
          marginheight: 0,
          marginwidth: 0,
          allowTransparency: true,
          id: '_' + elm[0] + '_ad',
          src: elm[1],
          width: elm[2],
          height: elm[3]
        });
        var ad = $('#_' + elm[0] + '_ad');
        var container_div = ad.parent('div');
    
        
        if (index == ad_array.length - 1) {
          container_div.empty();
          container_div.append($(ad_iframe));
          setTimeout(function() { $(ad_iframe).trigger(elm[0] + '.loaded');}, 100);
          setTimeout(function() { $('#fb-root').trigger('lastAd.loaded');}, 100 + (100 * index));
        } else {
          $("body").bind(ad_array[index + 1][0] + '.loaded', function(event) {
            container_div.empty();
            container_div.append($(ad_iframe));
            setTimeout(function() { $(ad_iframe).trigger(elm[0] + '.loaded');}, 100);
          });
        }
      });
    });
  }

  function rotate_ads() {
    
    ord = Math.round(Math.random() * 100000000);
    
    jQuery(function($) {
      $("body").unbind('lastAd.loaded');
      $(ad_array).each(function(index, elm) {
        $("body").unbind(elm[0] + '.loaded');
      });
      
      $(ad_array).each(function(index, ad) {
        var src = ad[1].replace(/ord=\d+/, 'ord=' + ord);
        
        if (src.indexOf(';is_refresh=true?') == -1) {
          src = src.match(/[^\?]+/);
          src += ';is_refresh=true?';
        }
        ad[1] = src;
      });
    });
    clear_skin();
    load_ad_iframes();
  }

  if (typeof ad_array != "undefined") {
    ad_array.reverse(); 
    load_ad_iframes();
  }
    
	  if ((typeof ad_array != "undefined") && (typeof(stop_refresh) == 'undefined')) new PeriodicalExecuter(rotate_ads, 180);

//]]>
</script>
