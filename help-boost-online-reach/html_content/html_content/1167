  Event.observe(window,"load", function(event){
    $$('#funbar .funbar_viral_links a').each(function(el){
      el.observe('click', function(event){
        var action = event.element().id
        if(action.blank()) { //for yahoo buzz link which we don't control
          action = event.element().ancestors().find(function(el){return el.id && !el.id.blank();}).id;
        }
        GA.event('sharing', action, "0534b6e457-reporter-flips-out-from-that-happened");
      });
    });
  });
</script>
  
