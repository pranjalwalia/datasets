<script type="text/javascript">
  var interstitial = false;
  if(!Cookie.get("_nwsl")) {
    var pv = Cookie.get("_pv", function(val) { return parseFloat(val); }) || 0
    pv += 1;
    Cookie.set("_pv", pv, {path: '/'});
    if(pv >= 3) {
      interstitial = true;
      var noautostart = true;
    }
  }
  
  function validate_newsletter_signup() {
    if($('newsfeed_dialog_email')) {
      return Validation.validate('newsfeed_dialog_email');
    } else {
      return true;
    }
  }
  
  function temp_dismiss_dialog() {
    Cookie.set("_pv", 0, {path: '/'})
    dismiss_dialog();
  }
  
  function dismiss_dialog(action) {
    if(action != undefined) {
      var exdate=new Date();
  	  exdate.setDate(exdate.getDate()+2000);
      Cookie.set("_nwsl", action, {expires: exdate, path: '/'})
    }
    hide_dialog('newsletter_dialog');
    if($('fodplayer')) {
      sendEvent("play");
    }
  }
</script>

