//<![CDATA[

	var template_featured_1 = new Template('<!-- Start of Funny or Die Funny or Die Featured Videos Widget -->\n<noscript><div><a href=\"http://www.funnyordie.com/?utm_campaign=featured&amp;utm_medium=widget&amp;utm_source=featured\">Funny or Die Featured Videos<\/a> from <a href=\"http://www.funnyordie.com/?utm_campaign=featured&amp;utm_medium=widget&amp;utm_source=featured\">Funny or Die<\/a><\/div><\/noscript>\n<iframe id=\"odn_featured_1\" frameborder=\"0\" marginheight=\"0\" marginwidth=\"0\" src=\"http://www.funnyordie.com/widget/featured/1#{querystring}\" width=\"#{width}\" height=\"#{height}\"><\/iframe>\n<div><a href=\"http://www.funnyordie.com/?utm_campaign=featured&amp;utm_medium=widget&amp;utm_source=featured\">Funny or Die Featured Videos<\/a> from <a href=\"http://www.funnyordie.com/?utm_campaign=featured&amp;utm_medium=widget&amp;utm_source=featured\">Funny or Die<\/a><\/div>\n<!-- End of Funny or Die Funny or Die Featured Videos Widget -->');

  function querystring_featured_1() {
    var querystring = '';
    var pairs = $H();
    var defaults = $H({theme:'dark',layout:'horizontal',height:'300',width:'300',font_size:'17',header:'22'});
    defaults.each(function(pair) {
      var field_id = pair.key+'_featured_1';
      if(pair.value != $(field_id).value) {
        pairs.set(pair.key, $(field_id).value);
      }
    });
    if(pairs.size() > 0) {
      querystring = '?'+pairs.toQueryString();
    }
    field_id = 'title_featured_1';
    if($(field_id.value) != '' && $(field_id).value != 'Funny or Die Featured Videos') {
      querystring += '#'+escape($(field_id).value);
    }
    return querystring;
  }

	function update_embed_widget_code_featured_1() {
	  $('embed_widget_featured_1').value = template_featured_1.evaluate({
	    width:$('width_featured_1').value, height:$('height_featured_1').value, querystring:querystring_featured_1()
	  });
	  $('embed_widget_featured_1_dialog_box').setStyle({width:(parseInt($('width_featured_1').value)+240+'px')});
		$('embed_widget_preview_featured_1').update($('embed_widget_featured_1').value);
	}

	function get_embed_code_featured_1() {
	  GA.event('sharing','embed_widget_featured_copy','#{widget_object.id}')
	  return $F('embed_widget_featured_1');
	}


