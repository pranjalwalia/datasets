<script type="text/javascript">
	$(function(){
	
		//Need Horscope URL, is Video just Video LP or all Video types	
		gloURl = "/wd-glofeed-msn/rss/hachette_living.xml";
		
		// GLO MODULE
		$.ajax({
			type: "GET",
			url: gloURl,
			dataType: "xml",
			success: function(xml) {
				$(xml).find('channel').each(function(index){
					var dataImg = $(this).find('image_url').text();
					if (dataImg.length < 1) {
						var dataImg = "/cm/womansday/site_images/icon-glo.gif";
					}
					$('<img src="'+ dataImg +'" width="70" height="75" class="modImg" alt="Glo" title="Glo" />').appendTo('#xmlImg');
				}); 
				
				$(xml).find('item').each(function(index){
					var dataTitle = $(this).find('title').text();
					var dataLink = $(this).find('link').text();
					$('<li><a href="'+ dataLink +'" target="_blank">' + dataTitle  + '</a></li>').appendTo('#xmlList');
				}); 
			}
		});
	});

</script>
