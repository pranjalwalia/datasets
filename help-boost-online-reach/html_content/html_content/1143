                stateCache.set('ts', "1358844484");
            }
        if (!window.amznLikeStateCache) {
            window.amznLikeStateCache = {};
        }
        window.amznLikeStateCache["amznLikeStateCache_18304924623118445_dp_asin_B008ZRIY6M"] = stateCache;
        });


