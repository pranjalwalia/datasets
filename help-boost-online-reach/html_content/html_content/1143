<script>
    amznJQ.onReady('jQuery', function() {
        jQuery.ajax({
            url: "/gp/gc/widget/product-gc/183-0492462-3118445?ie=UTF8&asin=B008ZRIY6M&productGroupID=gl_drugstore",
            type: 'GET',
            async: true,
            dataType: 'html',
            tryCount: 0,
            retryLimit: 3,
            success: function (data, status, request) {
                jQuery('#product-gc').html(data);
            },
            error: function (data, status, request) {
                   // simple retry mechanism;
                   if (this.tryCount++ < this.retryLimit) {
                       jQuery.ajax(this);
                   } else { }
                }
        });
    });
</script>


  







