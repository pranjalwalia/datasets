var CSMReqs={af:{c:2,e:'amznJQ.AboveTheFold'},cf:{c:2,e:'amznJQ.criticalFeature'}};
function setCSMReq(a){
    a=a.toLowerCase();
    var b=CSMReqs[a];
    if(b&&--b.c==0){
        if(typeof uet=='function'){uet(a);};
        if(b.e){amznJQ.completedStage(b.e);};
    }
}
</script>


