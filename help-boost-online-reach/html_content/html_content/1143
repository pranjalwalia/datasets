        stateCache.ready(function(){
            if (stateCache.getTimestamp() < 1358844484) {
                stateCache.set('isLiked', jQuery("#amznLikeStateCache_18304924623118445_dp_asin_B008ZRIY6M_isLiked").text()  == "true");
                stateCache.set('customerWhitelistStatus',  parseInt(jQuery("#amznLikeStateCache_18304924623118445_dp_asin_B008ZRIY6M_customerWhitelistStatus").text()));
