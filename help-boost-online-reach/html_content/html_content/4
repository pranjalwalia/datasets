<script>
	var magnified_current_page = 1;
	var magnified_total_pages = 250;
	function shift_magnified_page_1 ( delta ) {
		var page = magnified_current_page + delta;
		if ( page < 1 ) { page = magnified_total_pages }
		if ( page > magnified_total_pages ) { page = 1 }
		show_magnified_page_1( page );
	}
	function show_magnified_page_1 ( pagenum ) {
		new Ajax.Updater ( 'magnified_page_panels_1', "/display/magnified_panels?playlist=&per_row=7&rows_per_page=1&current_page=" + pagenum + '&site_cid=' + '' + '' + '&width=960');
		magnified_current_page = pagenum;
		var counter = 0
		$$('#magnified_page_links_1 a').each( function(el) {
			( ++ counter == magnified_current_page ) ? el.addClassName('mvp_selected') : el.removeClassName('mvp_selected')
		} );
	}
</script>


