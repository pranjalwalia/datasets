<script type="text/javascript">
	var ajaxQuery = new Hash();
	ajaxQuery.set('c', currentCID);
	var currentTitle = 'Dolce & Gabbana Fall 2011 Collection';
	ajaxQuery.set('t', currentTitle);

	var currentPlaylistCIDs = new Array(['NLHVX820XXBPRZY1', 'Dolce & Gabbana Fall 2011 Collection', '/video/Dolce-Gabbana-Fall-2011-Collect'], ['W1G8R823D64BWMTT', 'Best of New York: Bespoke Cocktails', '/video/Best-of-New-York-Bespoke-Cockta'], ['R9VRC91H12ZJS7HD', 'GMA Kinda Makes it Sound Like Kate Middleton is Pregnant', '/video/GMA-Kinda-Makes-it-Sound-Like-K'], ['ZSZ8R40KT09ZMPC1', 'True Religion - Cut Blog - 12Oct11', '/video/True-Religion-Cut-Blog-12Oct11'], ['SN5NHH1NGSSSR30N', 'Watch Street Style Photographers in Action', '/video/Watch-Street-Style-Photographer'], ['NB6RYW18WXM1ZZDW', 'Best of New York: Breakfast Taco', '/video/Best-of-New-York-Breakfast-Taco'], ['H2HS111K97TP2ZP0', 'CNN_PostLaunch_2011', '/video/CNNPostLaunch2011'], ['SYPRPS0LSFB6CSSF', 'Chase Sapphire _ Sept 2011', '/video/Chase-Sapphire-Sept-2011'], ['BR3497156W7ZK901', 'Seiko_SportsBlog_Oct11', '/video/SeikoSportsBlogOct11'], ['FR7KWJ1MHT358CYK', 'AMEXFuel_Sept11', '/video/AMEXFuelSept11'], ['3K9YD638L9LYBDLL', 'Sonia_CARE_090911', '/video/SoniaCARE090911']);
	
	var cidHash = new Hash();
	currentPlaylistCIDs.each( function(cid) {
		cidHash.set(cid[0], { title: cid[1], permaLink: cid[2] });
	});
	
	readAjaxQuery = function() {
		var ajaxQueryString = location.hash.sub('#', '');
		if ( ajaxQueryString.length ) {
			ajaxQueryString.split('&').map( function(kv) {		
				var ar = kv.split('=');
				ajaxQuery.set(ar[0], ar[1]);
			});
		}
	
		currentCID = ajaxQuery.get('c');
		currentTitle = ajaxQuery.get('t');
	}
</script>

<script type="text/javascript">
Effect.Scroll = Class.create();
Object.extend(Object.extend(Effect.Scroll.prototype, Effect.Base.prototype), {
  initialize: function(element) {
    this.element = $(element);
    var options = Object.extend({
      x:    0,
      y:    0,
      mode: 'absolute'
    } , arguments[1] || {}  );
    this.start(options);
  },
  setup: function() {
    if (this.options.continuous && !this.element._ext ) {
      this.element.cleanWhitespace();
      this.element._ext=true;
      this.element.appendChild(this.element.firstChild);
    }

    this.originalLeft=this.element.scrollLeft;
    this.originalTop=this.element.scrollTop;

    if(this.options.mode == 'absolute') {
      this.options.x -= this.originalLeft;
      this.options.y -= this.originalTop;
    } else {

    }
  },
  update: function(position) {   
    this.element.scrollLeft = this.options.x * position + this.originalLeft;
    this.element.scrollTop  = this.options.y * position + this.originalTop;
  }
});
</script>

<script type="text/javascript">
var justLoggedIn = false;
logIn = function(url, form, target) {
	new Ajax.Updater(target, toolRoot + url, {
		method: 'post', 
		parameters: Form.serialize( form ), 
		evalScripts: true
	});		
}

var registrationShowing = false;
checkEmail = function(email) {
	new Ajax.Request( '/login/email?email=' + escape( email ), {
		method: 'get',
		onSuccess: function( transport ) { 
			if ( transport.responseText.indexOf('Please') > -1 ) {
				if ( !registrationShowing ) {
					$('magnify_login_button').innerHTML = 'Register';
					Effect.BlindDown('mvp_signup_section');
					$('mvp_login_mode').value = 'signup';
					$('mvp_login_title').innerHTML = "New User Registration";
					registrationShowing = true;
				}
			} else {
				if ( registrationShowing ) {
					$('magnify_login_button').innerHTML = 'Log In';
					Effect.BlindUp('mvp_signup_section');
					$('mvp_login_mode').value = 'signin';
					$('mvp_login_title').innerHTML = "Log In";	
					registrationShowing = false;
				}
			}
			$('mvp_login_indicator').hide();
		} 
	});
}

cancelLogin = function() {	
	Element.hide('magnify_login_container');	
	Element.show('magnify_tags_container');	
	$('magnify_login_button').innerHTML = 'Log In';
	Effect.BlindUp('mvp_signup_section');
	$('mvp_login_mode').value = 'signin';
	$('mvp_login_title').innerHTML = "Log In";	
	if ( $('mvp_login_errors') )
		$('mvp_login_errors').remove();
	registrationShowing = false;
	continuousPlay = true;
}

cancelPanel = function() {
	if ( justLoggedIn && $('nym_iframe_header') ) {
		$('nym_iframe_header').src = $('nym_iframe_header').src;
	}
	new Element.hide('magnify_tools_container_container');	
	if ( lastPanel ) {
		Element.hide(lastPanel);
	}
	Element.show('magnify_tags_container');
}

showPanel = function(panel, url) {
	var thisPanel = $(panel + '_created');
	var blind = $('magnify_tools_container_container');
	var container = $('magnify_tools_container');
	if ( lastPanel && (lastPanel != panel) ) {
		Element.hide(lastPanel);
	}
	if ( !thisPanel ) {
		var containerCreated = document.createElement('div');
		containerCreated.id = panel + '_created';
		containerCreated.style.position = 'relative';
		container.appendChild(containerCreated);
		new Ajax.Updater(panel + '_created', toolRoot + url + '?cid=' + currentCID + '&container=' + panel + '_created', {
			method: 'get',
			evalScripts: true,
			onComplete: function() {				
				if ( blind.style.display == 'none') {
					Element.show(blind);
				}
			}
		});
	} else {
		Element.show(panel + '_created');
		if ( blind.style.display == 'none') {
			Element.show(blind);
		}
	}
	Element.hide('magnify_login_container');
	Element.hide('magnify_tags_container');
	lastPanel = panel + '_created';
}

saveObject = function(url, form, target) {
	new Ajax.Request('/embed/player/modules/get_login.minc', {	
		onCreate: function() {
			showIndicator(true);
		},
		onSuccess: function(transport) { 
			loggedIn = transport.responseText; 
			finishSave(url, form, target); 
		}
	});
}

finishSave = function(url, form, target) {
	if ( !url || !form || !target ) {
		showIndicator(false);
		return;
	}
	if ( loggedIn > 0 ) {
		new Ajax.Updater(target, toolRoot + url, {
			method: 'post', 
			parameters: Form.serialize( form ), 
			evalScripts: true, 
			onSuccess: function() { 
				if ( justLoggedIn ) {
					sidebarCallback(currentCID);
					navbarCallback();
				}
				showIndicator(false); 
			}
		});	
	} else {	
		showIndicator(false);
		delayedObject = new Array(url, form, target);
		if ( isSSO ) {
			window.open(toolRoot + 'sso_login', 'sso_login', '');			
			showPanel('ssologin','sso_login_msg.minc');
		} else {			
			if ( lastPanel ) {
				Element.hide('magnify_tools_container_container');
			}	
			Element.hide('magnify_tags_container');	
			Element.show('magnify_login_container');	
		}
	}
}

var hostname = window.location.hostname;

playerCallback = function(videoCID, autoplay, isInclude) {
	if ( !isInclude ) {
		isInclude = false;
	}
	if ( typeof(StatsObject) != 'undefined' ) {
		StatsObject.ajaxy = true;
		StatsObject.packLog();
	}
	currentCID = videoCID;	
	var cidInfo = cidHash.get(videoCID);
	currentTitle = cidInfo.title;
	currentURL = 'http://' + location.hostname + cidInfo.permaLink;
	location.hash = "c=" + videoCID + "&t=" + currentTitle;
	document.title = 'Watch "' + currentTitle + '" at New York Magazine';
	lastPanel = '';

	var playerURL = 'http://' + hostname + componentRoot + 'continuous_player.minc?cid=' + videoCID + '&autoplay=' + (autoplay ? '1' : '') + "&player_width=612&player_height=369&preserve_aspect_ratio=0" + ( isInclude ? "&is_include=1" : "" );	

	if ( $('magnify_player_loading_message') ) {
		Element.show('magnify_player_loading_message');
	}
	
	new Ajax.Updater('magnify_player_main_area', playerURL, {
		evalScripts: true,
		method: 'get',
		asynchronous: true,
		onComplete: function() {
			if ( $('magnify_player_loading_message') ) {
				Element.hide('magnify_player_loading_message');
			}	
			continuousPlay = true;
			if ( videoCID ) {
				var playlistItem = $('magnify_widget_playlist_item_' + videoCID);
				if ( playlistItem ) {
					sidebarCallback(videoCID);
					scrollToPlaylistItem(playlistItem);
					refreshAd(videoCID);
				}
			}
			if ( !isInclude ) {
				if ( typeof(nymag_setMetadata) == 'function' ) { 
	if ( typeof(nymagHierarchy) != 'undefined' ) { 
		nymag_setMetadata({'content.hierarchy':  nymagHierarchy}); 
	} 
	if ( typeof(nymagHierarchyTitle) != 'undefined' ) { 
		nymag_setMetadata({'content.hierarchy.title':  nymagHierarchyTitle}); 
	} 
	if ( typeof(currentURL) != 'undefined' ) { 
		nymag_setMetadata({'content.pagename': currentURL}); 
	} 
} 
nymag_pageView();
			}
			try {
				pSUPERFLY.virtualPage("/video/", document.title);
			} catch(e) { }
		}
	});
}

sidebarCallback = function(videoCID) {
	var sidebarURL = 'http://' + hostname + componentRoot + 'continuous_sidebar?cid=' + videoCID;	
	
	new Ajax.Updater('magnify_player_continuous_sidebar', sidebarURL, {
		evalScripts: true,
		method: 'get',
		asynchronous: true
	});
}

navbarCallback = function(videoCID) {
	var navbarURL = 'http://' + hostname + '/services/cacheable_nav';	
	var headerContainer = $('magnify_site_header');
	if ( !headerContainer ) {
		return;
	}
	new Ajax.Request(navbarURL, {
		evalScripts: true,
		method: 'get',
		asynchronous: true,
		onSuccess: function(transport) {
			var response = transport.responseText;
			var newHeader = '<div id="mvp_inserted_header">' + response + '</div>';
			headerContainer.insert( { before: newHeader } );
			headerContainer.remove();
		}
	});
}

loadNext = function() {
	var videoCID = 'undefined';
	var maxIndex = currentPlaylistCIDs.length;
	currentPlaylistCIDs.each(function(el, index) {
		if ( el[0] == currentCID ) {
			var nextIndex = index + 1;
			if ( maxIndex > nextIndex ) {
				var videoObj = currentPlaylistCIDs[nextIndex];
				videoCID = videoObj[0];
			} else {
				videoCID = 'undefined';
			}
			$break;
		}
	});
	if ( (videoCID != 'undefined') && continuousPlay ) {
		playerCallback(videoCID, 1);
		sidebarCallback(videoCID);
		var playlistItem = $('magnify_widget_playlist_item_' + videoCID);
		scrollToPlaylistItem(playlistItem);
	}
}

scrollToPlaylistItem = function(playlistItem) {
	makeSelected(playlistItem, 'magnify-widget-playlist-item-selected', 'magnify-widget-playlist-item');
	Position.prepare();
	var listContainer = $('magnify_widget_playlist_items_container_' + currentPlaylist);
	container_y = Position.cumulativeOffset(listContainer)[1];
	element_y = Position.cumulativeOffset(playlistItem)[1];
	new Effect.Scroll(listContainer.id, { x: 0, y: ( element_y - container_y ), mode: 'absolute' });
	return false;
}

refreshAd = function(videoCID) {
	new Ajax.Request('/item/include_adunit.minc?refresh=1&dimensions=continuous_300x250&cid=' + videoCID, { 
		evalScripts: true,
		method: 'get',
		asynchronous: true,
		onSuccess: function(transport) {
			var response = transport.responseText;
			
			if ( response == '' ) {
				$('magnify_player_continuous_ad').style.height = "0px";
			}
			
			var myScripts = response.extractScripts();
			var script_src = response.match(/<script.+?src="(.+?)".+?<\/script>/i);
			//var script_src = response.match(/<\s*script [^\>]*src\s*=\s*[\""\']?([^\""\'\s>]*)/i);
			
			var framework = response.gsub(/<script.+?src="(.+?)".+?<\/script>/, '');
			$('magnify_player_continuous_ad').innerHTML = "";
			$('magnify_player_continuous_ad').innerHTML += framework;
			
			if ( script_src && script_src.length > 0 && $('mvp_ad300')  ) {
				script_src.each(function(src, iter) {
					if ( src.match( /(script|src)/ ) ) {
						return;
					}
					
					var script = new Element('script', {
						'type' : 'text/javascript',
						'src' : src
					});
					$('mvp_ad300').appendChild(script);
				});
			} 
			
			var myReturnedValues = myScripts.map(function(script) {			
				if ( script ) {
					//alert(script);
					return eval(script);
				}
			});
		}
	});
}

setUpPlayerPage = function() {	
	readAjaxQuery();
	playerCallback(currentCID, 1, true);
	sidebarCallback(currentCID);

	document.write = function(text) {
		if ( !text ) {
			return;
		}	
		
		//alert(text);
		var myScripts = text.extractScripts();
		var script_src = text.match(/<script.+?src="(.+?)".+?<\/script>/i);
		//var script_src = text.match(/<\s*script [^\>]*src\s*=\s*[\""\']?([^\""\'\s>]*)/i);
		
		if ( script_src && script_src.length > 0  && $('mvp_ad300') ) {
			script_src.each(function(src, iter) {
				if ( src.match( /(script|src)/ ) ) {
					return;
				}
				var script = new Element('script', {
					'type' : 'text/javascript',
					'src' : src
				});
				$('mvp_ad300').appendChild(script);
			});
		} else {
		
			if ( $('mvp_ad300') ) {
				$('mvp_ad300').innerHTML += text;
			}
		
		}
		var myReturnedValues = myScripts.map(function(script) {	
			if ( script ) {
				return eval(script);
			}
		});
	} 
}

// addthis fix
Object.extend(Function.prototype, {
	substr: function() {
		var __method = this;
		return function() { return ""; }
	}
});

if ( window.addEventListener ) 
	window.addEventListener("load", setUpPlayerPage, false) 
else if ( window.attachEvent ) 
	window.attachEvent("onload", setUpPlayerPage) 
</script>

