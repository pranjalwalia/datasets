<script language="javascript">

    function DoSearch()
    {
    var e = document.getElementsByName("txtSearch");
    
    if (e[e.length-1].value.length > 0 && e[e.length-1].value != 'Search')
    {
        var strSearchText, strSearchLocation;
        
        strSearchText = e[e.length-1].value;
        strSearchLocation = '/site-search/site_search.asp?zoom_query=' + strSearchText;
        window.location = strSearchLocation;
      }
    }
    // This loads the navigation JS:
    if (window.attachEvent) window.attachEvent("onload", sfHover);
  </script>

