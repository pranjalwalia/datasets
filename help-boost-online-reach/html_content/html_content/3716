  function google_ad_request_done(google_ads){
    var s = '';
    var i;
    if(google_ads.length == 0){
      return;
    }
    s += '<div class="about_adsense" id="nointelliTXT"><div class="about_adgoo"><a href="'+ google_info.feedback_url +'">Ads by Google</a></div>'    
    for(i = 0; i < google_ads.length; ++i){
      s += '<div class="about_adgap"><div class="about_adtitle"><a target="_blank" href="' +
      google_ads[i].url + '" onmouseout="window.status=\'\'" onmouseover="window.status=\'go to ' +
      google_ads[i].visible_url + '\';return true">' +
      google_ads[i].line1 + '</a></div><div>' +
      google_ads[i].line2 + ' ' +
      google_ads[i].line3 + '<div class="about_adcont"><a target="_blank" href="' +
      google_ads[i].url + '" onmouseout="window.status=\'\'" onmouseover="window.status=\'go to ' +
      google_ads[i].visible_url + '\';return true"><span>' +
      google_ads[i].visible_url + '</a></div></div></div>';
    }
    s += '</div>'
    if (google_ads[0].bidtype == "CPC") {
      google_adnum = google_adnum + google_ads.length;
    }
    document.write(s);
    return;
  }
  
  google_ad_client = 'pub-1894578950532504';
  google_ad_output = 'js';
  google_max_num_ads = '3';
  google_ad_channel = '3735313997';
  google_ad_type = 'text_html';
  google_feedback = 'on';
  google_skip = google_adnum;
</script>            
