    <div class="content clearfix">
      <time id="clientside-datetime" datetime=""></time>
<script>
  (function ($) {
    $(document).ready(function(){
     var dt = new Date();
     dtAttr = dt.getMonth() + 1 +"-" +dt.getDate() +"-" +dt.getFullYear(),
     weekday = "Sunday,Monday,Tuesday,Wednesday,Thursday,Friday,Saturday".split(","),
     month="January,February,March,April,May,June,July,August,September,October,November,December".split(","),
     dateString=weekday[dt.getDay()]+", "+month[dt.getMonth()]+" "+" "+dt.getDate()+", "+dt.getFullYear();
     $("#clientside-datetime").html(dateString);
     $("#clientside-datetime").attr("datetime", dtAttr);
  });
  })(jQuery);
</script>    </div>
  </div>
</div>
