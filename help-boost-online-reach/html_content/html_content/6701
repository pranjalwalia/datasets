//轮播函数9点矫正
if ((new Date()).getHours() < 9) {
    for (var i=0; i<rad.length; i++) {
        if (arr = rad[i][2].match(/^<startdate>(\d+)-(\d+)-(\d+)<\/startdate>$/)) {
            var d = new Date(arr[1], arr[2] - 1, arr[3]);
            d = new Date(d.getTime() - 1000 * 60 * 60 * 24);
            rad[i][2] = d.getFullYear()+"-"+(d.getMonth()+1)+"-"+d.getDate();
        }
        if (arr = rad[i][3].match(/^<enddate>(\d+)-(\d+)-(\d+)<\/enddate>$/)) {
            var d = new Date(arr[1], arr[2] - 1, arr[3]);
            d = new Date(d.getTime() - 1000 * 60 * 60 * 24);
            rad[i][3] = d.getFullYear()+"-"+(d.getMonth()+1)+"-"+d.getDate();
        }
    }
}

new RotatorAD(rad,nad,'6511');
	}
});
