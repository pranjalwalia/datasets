      <script type="text/javascript">
      var $ = function(a) {
        if (document.getElementById) {
          return eval('document.getElementById("' + a + '")')
        } else {
          if (document.layers) {
            return eval("document.layers['" + a + "']")
          } else {
            return eval("document.all." + a)
          }
        }
      }
