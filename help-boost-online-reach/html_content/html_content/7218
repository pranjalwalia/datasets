<script type="text/javascript">
YUI().use('node', function( Z ) {
	var groups = new Array();
	var nodes = Z.all("div[id|='imageGroup']");
	var j = 0;
	YUI().use('imageloader', function (Y) {

	nodes.each( function(theNode) {
		groups[j] = new Y.ImgLoadGroup({
		    foldDistance: 10
		});
		var imageNodes = theNode.all("img");
		imageNodes.each( function( imageNode ) {
			if( imageNode.getAttribute('srcUrl') ) {
				groups[j].registerImage({
				    domId: imageNode.generateID(),
				    srcUrl: imageNode.getAttribute('srcUrl')
				});
			}
		});
		var authorNodes = theNode.all("dt[class='defaultAuthor']");
		authorNodes.each( function( authorNode ) {
			groups[j].registerImage({
			    domId: authorNode.generateID(),
			    bgUrl: "http://images.askmen.com/authors/" + authorNode.getAttribute('auth') + ".jpg"
			});
		});
		var authNode = theNode.one("div[class='featuredAuthor']");
		if( authNode ) {
			groups[j].registerImage({
				domId: authNode.generateID(),
				bgUrl: "http://images.askmen.com/authors/" + authNode.getAttribute('auth') + ".jpg"
			});
		}
		j++;
	});
	});
});

</script>

