<script type="text/javascript">
    if( getMeta( "idChannel" ) == 10 ) {
        $("#fourVideos").attr( "omni_section", "Homepage VideoBox" );
    }

YUI().use('node', function( Z ) {
	var group = new Array();
	var node = Z.one("div[id='fourVideos']");
	YUI().use('imageloader', function (Y) {
		group = new Y.ImgLoadGroup({
		    foldDistance: 10
		});
		var imageNodes = node.all("img");
		imageNodes.each( function( imageNode ) {
			if( imageNode.getAttribute('srcUrl') ) {
				group.registerImage({
				    domId: imageNode.generateID(),
				    srcUrl: imageNode.getAttribute('srcUrl')
				});
			}
		});
	});
});

</script>
