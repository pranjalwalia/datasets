            $('#askmenFriends3 .menu li.tab a').click(function(){
              $link = $(this);
              $link.parents().siblings().children().removeClass('active');
              $link.addClass('active');
              block = $link.attr('rel');

              $block = $('ul.links#'+block,$link.parents('#askmenFriends3'));
              if($block.css('display') != 'block'){
                  $block.siblings().hide();
                  $block.show();
              }else{
                  // it's already active
                  }
              return false;
            });
