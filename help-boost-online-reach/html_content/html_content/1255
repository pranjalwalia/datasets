			function URLDecodeIt(encodedString) {
				var output = encodedString;
				var plus = "+";
				var f = output.indexOf(plus);
				while (f>-1) {
					output=output.substring(0,f)+"%20"+output.substring(f+1);
					f = output.indexOf(plus);
				}
				return output;
			}	

			var theTitle = "Bacon%2C+Egg+and+Cheese+Cups";
			var object = SHARETHIS.addEntry({title:URLDecodeIt(theTitle)}, {button:false,embeds:true});
