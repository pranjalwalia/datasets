 $(document).ready(function(){
    cmtLength = $("#cmtNum").size();
    if(cmtLength)
    {
    var cmtNum = $('#cmtNum').text();
    var cmtNum = cmtNum.replace("(", "");
    var cmtNum = cmtNum.replace(")", "");
    $('#commentnumberPost').empty().append(cmtNum);
    }if(cmtLength == "0"){
    	$('#commentnumberPost').empty().append("0");
    }
    $('#latestOnStyle').find('li a:empty').parent().hide();

$(".mostPopularContent img").each(function() {
     if($(this).attr("src") != '') $(this).css("display","block")    
     else {
     $(this).css("display","none")
     $(this).parent().parent().css("padding-bottom","10px")
     $(this).parent().parent().css("height","auto")
     }
})
$(".onlyOnRightImg img").each(function() {
     if($(this).attr("src") != '') $(this).css("display","block")    
     else $(this).css("display","none")
})
var commentTest = $("#commentnumberPost").html();
if(commentTest == "0"){
        $('#postHeadCmt').attr("href","#commentformtoggle");
        $('.postReadCmnt').css("display","none")
    }
$("#styleAroundWeb li:first").addClass("first");
        
    });

</script>


