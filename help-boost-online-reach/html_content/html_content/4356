<script type="text/javascript">
$(document).ready(function(){
$('.networkSlides').mouseover(function() {
  $(this).css("background","#FFE4D2");
});
$('.networkSlides').mouseout(function() {
  $(this).css("background","#FFFFFF");
});
$("#top_nav #fashion").mouseenter(function(){
    $("#navFashion").css("visibility","visible");
	$("#fashion").css("background","url('/images/v/stylelist/dropdown-top.jpg') no-repeat scroll -3px 15px transparent");
  });

  $("#top_nav #fashion").mouseleave(function(){
    $("#navFashion").css("visibility","hidden"); 
	$("#fashion").css("background","none");
  });
