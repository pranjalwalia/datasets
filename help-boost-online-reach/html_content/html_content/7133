<script type="text/javascript"> 
	function getElementHeight(El) {
		if(document.getElementById) {
			el = document.getElementById(El);
		} else if (document.all){
			el = document.all[El];
		}
		
		return el.style.pixelHeight != undefined ? parseInt(el.style.pixelHeight) : parseInt(el.style.offsetHeight);
	}

	function toggle(el) { 
		document.getElementById(el).style.display = document.getElementById(el).style.display == 'none' ? 'block' : 'none';
	}

	var scrolling	= false;
	var scroll_by	= null;
	var scroll_el	= null;
	var curr_offset	= 0;
	var new_offset	= 0;
	var interval	= null;
	var speed		= document.all ? 1 : 5; // Milliseconds between animations (ie : ff)
	var incriment	= document.all ? 13 : 8; // Number of pixels to move each animation (ie : ff)
	var scroll_limit= null;

	function animate(direction) { 
		switch(direction) { 
			case 'up' : 
				if(curr_offset + incriment >= new_offset) { 
					if(curr_offset-incriment > 0) {
						scroll_el.style.marginTop = new_offset + 'px';
					}

					window.clearInterval(interval);
					scrolling = false;
				} else { 
					curr_offset = curr_offset + incriment;
					scroll_el.style.marginTop = curr_offset + 'px';
				}
			break;
			case 'down' : 
				if(curr_offset - incriment <= new_offset) { 
					scroll_el.style.marginTop = new_offset + 'px';
					window.clearInterval(interval);
					scrolling = false;
				} else { 
					curr_offset = curr_offset - incriment
					scroll_el.style.marginTop = curr_offset + 'px';
				}
			break;
			default : 
				window.clearInterval(interval);
				scrolling = false;
			break;
		}

		return false;
	}

	function scroll(scroll_this, direction) { 
		if(scrolling) { 
			return false;
		}

		scroll_by 	= document.getElementById(scroll_this+'-container').offsetHeight;
		scroll_el	= document.getElementById(scroll_this);
		scroll_limit= -parseInt(document.getElementById(scroll_this).scrollHeight);

		if((direction == 'up' && curr_offset == 0) || (direction == 'down' && curr_offset - scroll_by < scroll_limit)) { 
			scrolling = false;
			return false;
		}

		scrolling	= true;
		new_offset	= direction == 'up' ? curr_offset + scroll_by : curr_offset - scroll_by;
		interval	= window.setInterval('animate("' + direction + '");', speed);

		return false;
	}
</script>

