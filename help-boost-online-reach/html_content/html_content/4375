                <div class="comment-avatar">
                    <a href="${CommenterURL}">
                    <img src="${CommenterAvatar}" height="50" width="50" alt="Avatar" />
                    </a>
                </div>
                <div class="comment-info">
                    <p class="comment-meta">
                        <a href="${CommenterURL}" class="commenter"><span>${Commenter}</span></a>						
						
		                
		                {{tmpl(VoteInfo) "#contentVotesTemplate"}}
		                
                    </p>
                    <div class="the-comment"><p>{{html Comment}}</p></div>
                </div>
				<span class="comment-timestamp">${CommentDate}</span>
				
            </div>
            </script>
            <script type="text/javascript">
              $(document).ready(function(){
                $("#commentRepliesNavTemplate").template("commentRepliesNavTemplate");
                $("#commentRepliesTemplate").template("commentRepliesTemplate");
              });
            </script>
    
    
                  <script id="contentVotesTemplate" type="text/x-jquery-tmpl">
                <div class="comment-votes votebox group">
                    <span id="content-votes-${ContentID}">
                        
                        <div class="votelink-up"><span id="content-votes-${ContentID}-up" class="content-votecount">${VoteUpCount}</span></div>
                        <div class="votelink-down"><span id="content-votes-${ContentID}-down" class="content-votecount">${VoteDownCount}</span></div>
                        
                       <div class="content-vote-remove-link-container" id="content-votes-${ContentID}-remove-link-container"
                           {{if MemberVote == 0}}
                           style="display:none;"
                           {{/if}}
                           >
                           <a class="content-vote-remove"
                           data-ref="${ContentNodeRef}"
                           data-vote="${MemberVote}"
                           data-content-id="${ContentID}"
                           data-read-only="0"
                           id="content-votes-${ContentID}-remove-link"
						   title="Clear Vote"
                           >Clear Vote</a>
                       </div>
                    </span>
               </div>

         </script>
         <script type="text/javascript">
           $(document).ready(function(){
             $('#contentVotesTemplate').template('contentVotesTemplate');
           });
         </script>
    
