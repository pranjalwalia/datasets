            <label for="tip-PhoneNumber">Phone Number <font style='color:#760000;font-style:italic;'>(required):</font></label><label class="error" for="phone" id="phone_error">-- Enter your phone number.</label>
            <input type='text' name='#phone' id='tip-PhoneNumber' class='form' size='50' value='' />
        </fieldset>

        <fieldset id="field-subject" style="display:none;">
            <label for='tip-Subject'>Subject <font style='color:#760000;font-style:italic;'>(required):</font></label><label class="error" for="subject" id="subject_error">-- Please write a subject.</label>
            <input type='text' name='#subject' id='tip-Subject' class='form' size='50' value='' />
        </fieldset>

        <fieldset id="field-tiptext" style="display:none;">
            <label for="tiptext">Your Tip</label><label class="error" for="message" id="tip_error">-- Please write a tip.</label>
            <textarea id="tiptext" name='#message'></textarea>
            <input type="submit" id="tip-sendbutton" src="btn-send-tip.png" value="">
        </fieldset>
	</form>
</div>
        <script>
			$(function(){

				$('#ortypehere').click(function () {
					$('#gotatip').css("z-index", "99999999999");
					$('#ortypehere').hide();
					$('#tip-AuthorName').focus();
					$('#overlay').show();
					$('#gotatip-form').addClass("form-shadow");
					$('#tip-labelname').show();
					$('#field-email').slideDown();
			        $('#field-phone').slideDown();
					$('#field-subject').slideDown();
					$('#field-tiptext').slideDown();
			    });
				$("#overlay").click(function () {
					$('#overlay').hide();
					$('.error').hide();
					$('#tip-labelname').hide();
					$('#field-email').hide();
			        $('#field-phone').hide();
					$('#field-subject').hide();
					$('#field-tiptext').hide();
					$("#signinform").fadeOut();
					$('#gotatip-form').removeClass("form-shadow");
					$('#ortypehere').show();
					$('#gotatip').css("z-index", "100000");
				});
		 	});
			$(function(){
				$('.error').hide();
				var sending = false;
				$("#tip-sendbutton").click(function () {
				    if (sending) {
				        return;
				    }
					$('.error').hide();
					var data = {};

					data['#name'] = $("#tip-AuthorName").val();
			  		if (data['#name'] == "") {
				        $("label#name_error").show();
				        $("input#tip-AuthorName").focus();
				        return false;
			      	}

					data['#email'] = $("#tip-AuthorEmail").val();
			  		if (data['#email'] == "") {
				        $("label#email_error").show();
				        $("input#AuthorEmail").focus();
				        return false;
			      	}
					data['#phone'] = $("#tip-PhoneNumber").val();
			  		if (data['#phone'] == "") {
				        $("label#phone_error").show();
				        $("input#tip-PhoneNumber").focus();
				        return false;
			      	}
					data['#subject'] = $("input#tip-Subject").val();
			  		if (data['#subject'] == "") {
				        $("label#subject_error").show();
				        $("input#tip-subject").focus();
				        return false;
			      	}
			      	data['Title'] = data['#subject'];
					data['#message'] = $("textarea#tiptext").val();
			  		if (data['#message'] == "") {
				        $("label#tip_error").show();
				        $("textarea#tiptext").focus();
				        return false;
			      	}

                    sending = true;
                    $.ajax({
                        type: 'POST',
                        url: '/tips-ajax',
                        data: data,
                        success: function(response) {
                            sending = false;
                            $('#gotatip-form').html(response);
                        },
                        dataType: 'html'
                    });
                    return false;
				});
		});
        </script>
