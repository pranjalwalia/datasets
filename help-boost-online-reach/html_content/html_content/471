<script language="Javascript">
var thisEditorField = 'comment';
var deleteCommentLink = '';
var myUsername = '';
var myPhoto = '';

// edit a user's comment
function editComment(commentId, commentMemberId) {
    document.replyform.edit_id.value = commentId;
    document.replyform.edit_member_id.value = commentMemberId; 
    document.getElementById('postReplyTitle').innerHTML = 'Edit Post';
    enableHTMLEditor(document.getElementById('commentText_'+commentId).innerHTML);
    document.location.hash = 'reply';
}

// delete a user's comment
function deleteComment(commentId) {
    if (confirm('Are you sure you want to delete this post?')) {
        document.location.href = deleteCommentLink + '&messageboard_deletepost=' + commentId; 
        return false;
    }
}

// turn the preview module off; turn the editor form back on
function editPreviewedComment() {
	document.getElementById('postCommentForm').style.display = 'block';
	document.getElementById('previewCommentModule').style.display = 'none';
}

// reply to another user's comment
function replyComment(commentId, commentMemberId, commentUsername) {
    document.replyform.reply_id.value = commentId; 
    document.replyform.reply_member_id.value = commentMemberId; 
    document.getElementById('postReplyTitle').innerHTML = 'Post a Reply to ' + commentUsername;
    document.getElementById('quoteReply').innerHTML = document.getElementById('commentText_'+commentId).innerHTML;
    document.getElementById('quoteReply').style.display = 'block';
    document.location.hash = 'reply';
}

// turn the editor form off; turn the preview module on and populate it
function previewComment() {
	//var theForm = document.getElementById('FCKeditor_form');

    var editorForm = FCKeditorAPI.GetInstance('comment');
    document.getElementById('comment_').innerHTML = editorForm.GetHTML();

	document.getElementById('postCommentForm').style.display = 'none';
	document.getElementById('previewCommentModule').style.display = 'block';
    document.getElementById('rating_comment').innerHTML = '0';
    // if editing another user's comment, show their photo and username on the preview
    var editId = document.getElementById('FCKeditor_form').edit_id.value;
    if (editId != '') {
        document.getElementById('commentUserPhoto_').innerHTML = document.getElementById('commentUserPhoto_'+editId).innerHTML;
        document.getElementById('commentUserName_').innerHTML = document.getElementById('commentUserName_'+editId).innerHTML;
    // otherwise, show my own photo and username
    } else {
        document.getElementById('commentUserPhoto_').innerHTML = myPhoto;
        document.getElementById('commentUserName_').innerHTML = myUsername;      
    }
}

// show a "poor quality" comment
function showPQComment(commentId) {
    document.getElementById('comment_'+commentId).style.display = 'block';
    document.getElementById('commentHide_'+commentId).style.display = 'none';
}

</script>

