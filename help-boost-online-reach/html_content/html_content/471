<script type="text/javascript">
function trackMPEvent(eventName, p1Name, p1Value, mpCallBack) {
    try {
        var properties = new Object();
        properties['pathname'] = window.location.pathname;
        if (p1Name && p1Value) {
            properties[p1Name] = p1Value;
        }
        if (mpCallBack) {
            mpq.push(["track", eventName, properties, mpCallBack]);
        } else {
            mpq.push(["track", eventName, properties]);
        }
    } catch (err) {
        mpq.push(["track", "trackMPEvent-error", {
            "pathname": window.location.pathname
        }]);
    }
}

function trackMPEventAndGo(url, eventName, p1Name, p1Value) {
    var mpCallBack = function() {
        window.location.hash = eventName;
        window.location.href = url;
    }
    trackMPEvent(eventName, p1Name, p1Value, mpCallBack);
}
</script>
