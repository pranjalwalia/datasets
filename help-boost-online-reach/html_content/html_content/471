	onClick="product_click('SHOP');"
	class="fltRight btnOrangeOnBlue18x54" style="margin-top: -2px;">SHOP</div>
		<div style="float: left;">
						olympics
						Gear
		</div>
		<div class="clear"></div>
	</div>

	<div class="moduleBody" style="padding: 0; margin: 0; padding-top: 3px; height: 155px;">
		<div class="product_body">
			<div id="productLoading31337" class="product_loading" style="display: none;">Loading...</div>

			<div id="productInfo31337" class="product_info">

				<form method="post" target="_new" name="productForm31337" action="/ajax/services.php" >
					<input type="hidden" name="action" value="tfs_productClick" />
					<input type="hidden" name="id" id="productId31337" />
					<input type="hidden" name="page" value="blog" />
				</form>

					<div class="product_box" 
