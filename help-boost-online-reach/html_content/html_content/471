        function registrationShowOverlay(title)
		{
 			if (typeof(team_comment) != "undefined") {
				document.getElementById('team_comment').value = team_comment;
				if (typeof(team_url) != "undefined") {
				document.getElementById('team_url').value = team_url;
				}
			}
			
            defaultTitleText = "Please sign in to complete your action";
            if( title )
                document.getElementById('registrationActionLink').innerHTML = title;
            else
                document.getElementById('registrationActionLink').innerHTML = defaultTitleText;
                
            document.getElementById('registrationErrorMsg').style.display = 'none';
            document.getElementById('popup_fav_sport1').style.borderColor = "transparent";
            document.getElementById('popup_fav_team1').style.borderColor = "transparent";
            document.getElementById('popupInputSection').style.height = 305 + 'px';
            
			var maxNumOfGroups = registrationGroupArray.length;
            var maxNumOfPositions = 3;
			var groupIndexToUse = lastGroupIndexUsed;
            
			while( groupIndexToUse == lastGroupIndexUsed )
			{
				groupIndexToUse = Math.floor(Math.random() * maxNumOfGroups);
			}
			
			var firstIndex = Math.floor(Math.random() * maxNumOfPositions);
			var secondIndex = Math.floor(Math.random() * maxNumOfPositions);
			while( secondIndex == firstIndex )
			{
				secondIndex = Math.floor(Math.random() * maxNumOfPositions);
			}
			
			var thirdIndex = Math.floor(Math.random() * maxNumOfPositions);
			while( thirdIndex == firstIndex || thirdIndex == secondIndex )
			{
				thirdIndex = Math.floor(Math.random() * maxNumOfPositions);
			}
			
			document.getElementById('registrationPopupName1').innerHTML = registrationGroupArray[groupIndexToUse][firstIndex][0];
			document.getElementById('registrationPopupTeamImg1').src = registrationGroupArray[groupIndexToUse][firstIndex][1];
			document.getElementById('registrationPopupUserImg1').src = registrationGroupArray[groupIndexToUse][firstIndex][2];
			
			document.getElementById('registrationPopupName2').innerHTML = registrationGroupArray[groupIndexToUse][secondIndex][0];
			document.getElementById('registrationPopupTeamImg2').src = registrationGroupArray[groupIndexToUse][secondIndex][1];
			document.getElementById('registrationPopupUserImg2').src = registrationGroupArray[groupIndexToUse][secondIndex][2];
            
            document.getElementById('registrationPopupName3').innerHTML = registrationGroupArray[groupIndexToUse][thirdIndex][0];
			document.getElementById('registrationPopupTeamImg3').src = registrationGroupArray[groupIndexToUse][thirdIndex][1];
			document.getElementById('registrationPopupUserImg3').src = registrationGroupArray[groupIndexToUse][thirdIndex][2];
            
            userMemberIdArray[0] = registrationGroupArray[groupIndexToUse][firstIndex][3];
            userMemberIdArray[1] = registrationGroupArray[groupIndexToUse][secondIndex][3];
            userMemberIdArray[2] = registrationGroupArray[groupIndexToUse][thirdIndex][3];
			
            lastGroupIndexUsed = groupIndexToUse;
            
            posY = Math.ceil(getScreenCenterY());  
            posX = Math.ceil(getScreenCenterX());
            popupBox = document.getElementById('popupRegistrationBox');
            popupBox.style.display = 'inline';
            document.getElementById('registrationPopupGreyBackground').style.width = document.body.clientWidth + 'px';
            document.getElementById('registrationPopupGreyBackground').style.height = document.body.clientHeight + 'px';
            //changeOpac(50, 'registrationPopupGreyBackground');
            changeOpac(0,'registrationPopupGreyBackground');
            changeOpac(0,'popupRegistrationBox');
            opacity('registrationPopupGreyBackground', 0, 60, 800);
            opacity('popupRegistrationBox', 0, 100, 400);
            document.getElementById('registrationPopupGreyBackground').style.display = 'block';
            posX = posX - 184;
            posY = posY - 172;
            popupBox.style.left = posX + 'px';
            popupBox.style.top = posY + 'px';
//            document.body.style.overflow = "hidden";
        }
        
        function registrationHideOverlay()
        {
            document.getElementById('registrationPopupGreyBackground').style.display = 'none';
            document.getElementById('popupRegistrationBox').style.display = 'none';
            document.getElementById('popup_fav_sport1').options[0].selected = true;
            document.getElementById('popup_fav_team1').style.display = 'none' ;
            document.getElementById('popup_fav_team1').options.length = 0;
//            document.body.style.overflow = "auto";
        }
        
        function opacity(id, opacStart, opacEnd, millisec) {
            //speed for each frame
            var speed = Math.round(millisec / 100);
            var timer = 0;
        
            //determine the direction for the blending, if start and end are the same nothing happens
            if(opacStart > opacEnd) {
                for(i = opacStart; i >= opacEnd; i--) {
                    setTimeout("changeOpac(" + i + ",'" + id + "')",(timer * speed));
                    timer++;
                }
            } else if(opacStart < opacEnd) {
                for(i = opacStart; i <= opacEnd; i++)
                    {
                    setTimeout("changeOpac(" + i + ",'" + id + "')",(timer * speed));
                    timer++;
                }
            }
        }

        //change the opacity for different browsers
        function changeOpac(opacity, id) {
            var object = document.getElementById(id).style;
            object.opacity = (opacity / 100);
            object.MozOpacity = (opacity / 100);
            object.KhtmlOpacity = (opacity / 100);
            object.filter = "alpha(opacity=" + opacity + ")";
        }
  
        function getScreenCenterY()
        {  
            var y = 0;  
            y = getScrollOffset()+(getInnerHeight()/2);  
            return(y);  
        }  
          
        function getScreenCenterX()
        {  
            return(document.body.clientWidth/2);  
        }  
          
        function getInnerHeight()
        {  
            var y;  
            if (self.innerHeight) // all except Explorer  
            {  
                y = self.innerHeight;  
            }  
            else if (document.documentElement && document.documentElement.clientHeight)  
            // Explorer 6 Strict Mode  
            {  
                y = document.documentElement.clientHeight;  
            }  
            else if (document.body) // other Explorers  
            {  
                y = document.body.clientHeight;  
            }  
            return(y);  
        }  
          
        function getScrollOffset()
        {  
            var y;  
            if (self.pageYOffset) // all except Explorer  
            {  
                y = self.pageYOffset;  
            }  
            else if (document.documentElement && document.documentElement.scrollTop)  
            // Explorer 6 Strict  
            {  
                y = document.documentElement.scrollTop;  
            }  
            else if (document.body) // all other Explorers  
            {  
                y = document.body.scrollTop;  
            }  
            return(y);  
        }
        function handleSelectSportEvent2(sportIndex, selectObj)
        {
            document.getElementById('popup_fav_team1').style.display = 'block';
            document.getElementById('registrationErrorMsg').style.display = 'none';
            selectObj.style.borderColor = "transparent";
            attach_file('/team_helper.php?sport_id=' + selectObj.options[selectObj.selectedIndex].value + '&menu_id=popup_fav_team1' + '&fav=1');
            document.getElementById('popupInputSection').style.height = 335 + 'px';
        } // end handleSelectSportEvent2
        
        function registrationPopupSubmitForm()
        {
            selectSportObj = document.getElementById('popup_fav_sport1');
            selectTeamObj = document.getElementById('popup_fav_team1');
            if( selectSportObj.selectedIndex == 0 )
            {
                document.getElementById('registrationErrorMsg').style.display = 'block';
                document.getElementById('registrationErrorMsg').innerHTML = 'Please select your sport.';
                selectSportObj.style.border = "solid 1px red";
            }
            else if( selectTeamObj.selectedIndex == 0 )
            {
                document.getElementById('registrationErrorMsg').style.display = 'block';
                document.getElementById('registrationErrorMsg').innerHTML = 'Please choose your favorite team.';
                selectTeamObj.style.border = "solid 1px red";
            }
            else
            {
                document.getElementById('registrationErrorMsg').style.display = 'none';
                selectTeamObj.style.borderColor = "transparent";
                var formElmt = document.getElementById('registrationPopupForm');
                formElmt.submit();
            }
        }
        function registrationPopupProfile(event, memberIdIndex)
        {
            member_id = userMemberIdArray[memberIdIndex];
            popProfile(event, member_id);
        }
    </script>
