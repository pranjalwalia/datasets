<!-- Global Scripts -->
<script type="text/javascript">
	jQuery(function() {
		/* Main Navigation */
		/*jQuery("#item-recipes, #item-home, #item-lifestyle, #item-health, #item-style").mouseout(function() {
			jQuery("#nav-main>li").each(function(i) {
				var re = jQuery(this).attr("class");
				if (re.match("default")) {
					jQuery("#nav-sub ul").show();
				}
			});
		});*/
		
		jQuery("#nav-sub ul").hide();
		jQuery("#nav-main .active a:first-child").css("background-position","0 100% !important");
		
		/* Activated */
		jQuery(".activated #item-recipes, .activated #item-home, .activated #item-lifestyle, .activated #item-health, .activated #item-style").hover(function() {
			jQuery("#nav-sub ul").hide();
			jQuery("#nav-main>li ul").hide();
			jQuery(this).children("ul").show();
			jQuery(this).find("a").css("background-position","0 100%")
		},
		function() {
			jQuery("#nav-main>li ul").hide();
			jQuery("#nav-main .active ul").show();
			jQuery(this).find("a").css("background-position","0 0")
		});
		
		jQuery(".activated #item-giveaways, .activated #item-blogs, .activated #item-video, .activated #item-horoscopes, .activated #item-games").hover(function() {
			jQuery("#nav-sub ul").show()
			jQuery("#nav-main .active ul").hide();
			jQuery("#item-recipes ul, #item-home ul, #item-lifestyle ul, #item-health ul, #item-style ul").hide();
		},
		function() {
			jQuery("#nav-sub ul").hide();
			jQuery("#nav-main .active ul").show()
		});
		
		/* Inactivated */
		jQuery("#nav-sub.inactivated-sub ul").show();
		
		jQuery(".inactivated #item-recipes, .inactivated #item-home, .inactivated #item-lifestyle, .inactivated #item-health, .inactivated #item-style").hover(function() {
			jQuery("#nav-sub ul").hide();
			jQuery("#nav-main>li ul").hide();
			jQuery(this).children("ul").show();
			jQuery(this).find("a").css("background-position","0 100%")
		},
		function() {
			jQuery("#nav-sub ul").show();
			jQuery("#nav-main>li ul").hide();
			jQuery(this).find("a").css("background-position","0 0")
		});
		
		jQuery(".inactivated #item-giveaways, .inactivated #item-blogs, .inactivated #item-video, .inactivated #item-horoscopes, .inactivated #item-games").hover(function() {
			jQuery("#nav-sub ul").show()
			jQuery("#item-recipes ul, #item-home ul, #item-lifestyle ul, #item-health ul, #item-style ul").hide();
		},
		function() {
		});
		
		/* Ticker */
		var newsitems;
		var curritem = 0; 

		newsitems = jQuery("#nav-sub li").hide().size();
		jQuery("#nav-sub li:eq("+curritem+")").show();
		setInterval(ticknews,4000);

		function ticknews() {
			jQuery("#nav-sub li:eq("+curritem+")").hide();
			curritem = ++curritem%newsitems;
			jQuery("#nav-sub li:eq("+curritem+")").show();
		} 
		
		/* Search Blur */
		jQuery('#search input[type="text"]').blur(function() {
			if (this.value == "") { 
				this.value = "Search Woman's Day"
			}
		});
		jQuery('#search input[type="text"]').focus(function() {
			if (this.value == "Search Woman's Day") {
				this.value = "";
			}
		});
		
		jQuery("#site-description").show(); 
	});
</script>
<script type="text/javascript" language="javascript">
function setHomepage() 
{
	 if (document.all) 
	 {
	  document.body.style.behavior='url(#default#homepage)';
	  document.body.setHomePage('http://www.womansday.com'); 
	 }
 else 
     {
			  alert("This is not an Internet Explorer. Please follow the instructions for your browser.");
	 }
	
 } 
</script>


