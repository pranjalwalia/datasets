        var fbApi = "8e32afe206687eb370d3602288e3fc35";
        $(document).ready(function()
        {
            FB.init({
                appId: "219649940739",
                cookie: true,
                status: true,
                xfbml: true,
                oauth:true
            });
        });
        $(function () {
            if (FB && FB.Event && FB.Event.subscribe) {
                FB.Event.subscribe('edge.create',
                    function(url) {
                        var itemId = url.substring(url.lastIndexOf('/') + 1, url.indexOf('?') > 0 ? url.indexOf('?') : url.length),
                            userId = Freq.user && Freq.user.id ? Freq.user.id : 0;
                        if (url.match('/video/') && !isNaN(itemId)) {
                            Freq.Services.Activity.videoSharedViaFacebook(userId, itemId);
                        }
                    }
                );
                if (_gaq) {
                    FB.Event.subscribe('edge.create', function(targetUrl) {
                      _gaq.push(['_trackSocial', 'facebook', 'like', targetUrl]);
                    });

                    FB.Event.subscribe('edge.remove', function(targetUrl) {
                      _gaq.push(['_trackSocial', 'facebook', 'unlike', targetUrl]);
                    });

                    FB.Event.subscribe('message.send', function(targetUrl) {
                      _gaq.push(['_trackSocial', 'facebook', 'send', targetUrl]);
                    });
                }
            }
        });
    </script>


