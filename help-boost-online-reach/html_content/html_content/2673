                    else if(arr[arr.length-1] == 'Articles.aspx')
                    {
                        showHideCarat('imgArticleSearch');
                    }
                    else
                    {
                        showHideCarat('imgRecipeSearch');
                    }
                    */
                    //-->
                </script>
        <div id="content" class="clearfix">
            <div id="stripe" style="">
                &nbsp;</div>
