<script type="text/javascript">

	jQ(document).ready(function(){
			var currentIndex;

			// set up jQuery address plugin handlers
			jQ('a.next-img, a.prev-img, a.img-jump').address();

			// this is triggered when one of the above handlers is clicked
			jQ.address.change(function(event) {
				if(event.value != '/') {
					currentIndex = jQ.address.parameter('index');
					var pic = event.value.replace(/\//g,'');
					loadImg('http://www.guardian.co.uk/fashion/gallery/2012/jul/08/gentlemen-compete-chap-olympiad-in-pictures' + pic, currentIndex);
					refreshAds();

				} else {
					// load current picture
					currentIndex = 0;
					var pic = '?picture=392787930';
					loadImg('http://www.guardian.co.uk/fashion/gallery/2012/jul/08/gentlemen-compete-chap-olympiad-in-pictures' + pic, currentIndex);
				}
			});

			function loadImg(url, ind) {
				jQ('.figure .placeholder').show();
				jQ('.figure a').hide();
				jQ('.carousel li').removeClass('current');
				jQ('.carousel li').eq(ind).addClass('current');
				jQ('.content').load(url + ' .article, .mini-nav', function(r, s, x) {
					jQ('.placeholder').hide();
					jQ('.content').show();
					insertExitGalleryLink();
				});
			}

			// get the referrer
			function insertExitGalleryLink() {
				if(document.referrer) {
					var guardian = document.referrer.indexOf("guardian.co.uk");
					var gucode = document.referrer.indexOf("gucode.co.uk");
					var guqa = document.referrer.indexOf("guqa.co.uk");
					if(guardian != -1 || gucode != -1 || guqa != -1) {
						jQ('.mini-nav').append('<li class="exit"><a href="' + document.referrer + '">Exit gallery and go to previous page</a></li>');
					}
				}
			}

			// do the ad refresh stuff
			function refreshAds() {
				// google adsense for content refresh
				if(window.doAdUpdate) {
					doAdUpdate(jQ('.caption').text());
				}

				// refresh advert iframes
				jQ('#Middle3 iframe').attr('src', 'http://resource.guim.co.uk/global/adcode/generatehtml?slot=Middle3&partner=http%3A%2F%2Fwww.guardian.co.uk%2Ffashion%2Fgallery%2F2012%2Fjul%2F08%2Fgentlemen-compete-chap-olympiad-in-pictures&k=lifeandstyle&k=culture&k=london&k=fashion&k=festivals&cf=fashion+and+beauty&pid=&ct=gallery&pt=gallery&');

			}

			// set up keyboard navigation
			var keys = {
				"LEFT" : 37,
				"RIGHT" : 39
			};

			jQ(document).keyup(function(e) {
				var code = e.keyCode ? e.keyCode : e.which;
				switch(code) {
					case keys.LEFT:
						jQ('a.prev-img').click();
						break;
					case keys.RIGHT:
						jQ('a.next-img').click();
						break;
				};
			});

	});

    function track_gallery_scroll(node, title) {
        s.linkTrackVars='eVar37,events';
        s.linkTrackEvents='event37';
        s.eVar37='Gallery scroll:Chap Olympiad 2012 - in pictures';
        s.events='event37';
        s.tl(node,'o',title);
    }

</script>

<div class="gallery">
	
	<div class="placeholder" style="line-height:480px; height:480px;"><img src="http://static.guim.co.uk/static/627efcf9ed45f4c7c9aa7d860545c6da7fb116c5/common/styles/images/ajax-loader.gif" alt="Loading" width="16" height="16" /></div>

	<div class="content initially-off">
		<ul class="mini-nav">
            <li><a href="?picture=392787944" onclick="if(true) { track_gallery_scroll(this, 'Previous Item'); } return false;" rel="address:/?picture=392787944&index=8" class="prev-img" title="Previous [keyboard: left arrow]"><img src="http://static.guim.co.uk/static/627efcf9ed45f4c7c9aa7d860545c6da7fb116c5/common/styles/images/gallery_left_arrow.gif" alt="Previous image" width="19" height="20" /></a></li>
            <li><span class="index">1</span> / 9</li>
