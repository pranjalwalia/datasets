<script language="javascript"> 
function getQuerystring(key, default_)
{
  if (default_==null) default_=""; 
  key = key.replace(/[\[]/,"\\\[").replace(/[\]]/,"\\\]");
  var regex = new RegExp("[\\?&]"+key+"=([^&#]*)");
  var qs = regex.exec(window.location.href);
  if(qs == null)
    return default_;
  else
    return qs[1];
}
</script>


<script language="javascript"> 
var url = window.location.pathname;
var recipeID;
if (url.indexOf('recipeallratings') != -1)
  {
  recipeID=getQuerystring('recipe_id') + "&min=1&max=3";
  }
else
  {
  var endDex = url.lastIndexOf(".aspx?");
  if(endDex == -1)
                {
                endDex = url.lastIndexOf(".");
                }
   var preUrl = url.substr(0,endDex);
   var preDex = preUrl.lastIndexOf("-");
   recipeID= preUrl.substr(preDex+1,preUrl.length) + "&min=1&max=3";
  }
document.write('<script type="text/javascript" lang="javascript" src="http://cnt.hsn.com/Feeds/KraftHRect/Control?args=@external_id|' + recipeID + '"><\/script>');
</script>



