			(function () {
				var widget = $('#tab_widget_197575'),
					tabs = widget.find('.labels a'),
					panes = widget.find('.content>div');
				tabs.click(function (e) {
					var tab = $(this);
					e.preventDefault();
					tabs.add(panes).removeClass('current');
					tab.add($(tab.attr('href'))).addClass('current');
				});
			}());
		
