<div id="shares_for_post_57177" class="shares"><div class="share-counts large-icons"><iframe src="http://finder.good.is/like_buttons/?layout=30px&amp;url=http%3A%2F%2Fwww.good.is%2Fpost%2Fthe-good-lunch-black-bean-soup%2F&amp;image_url=http%3A%2F%2Fcms3.good.is%2Fposts%2Fthumb_1300841746Soupbadge.jpg&amp;tags=Lifestyle%2Chealth%2Cfood%2Cshare%2Coffice%2Crecipes%2Cmexican%2Cportable%2Cvegetarian%2Cgadgets%2Cvegetable%2Clunch%2Cbeans%2Cpotluck%2Csoup%2Cthe+good+lunch%2Cseasonal%2Cspicy%2Cjalapenos%2Cblack+beans&amp;description=The+GOOD+Lunch%3A+Black+Bean+Soup" scrolling="no" frameborder="0" allowTransparency="true" class="its-good"></iframe><script type="text/javascript" src="http://finder.good.is/javascripts/share_button.js"></script><span class="fold"></span></div><script type='text/javascript'>
$(document).ready(function() {
  var shares = $('#shares_for_post_57177'),
      content = $('div.content'),
      sharesHeight = shares.outerHeight(true),
      sharesLeft = shares.offset().left,
      yMin = shares.offset().top - parseInt(shares.css('margin-top'), 10),
      yMax = content.offset().top + content.outerHeight() + parseInt(shares.css('margin-top'), 10),
      position = 'default',
      positionCSS = {
              'default': {},
              'fixed': {position: 'fixed', top: 0},
              'bottom': {position: 'absolute', top: yMax - sharesHeight}
      };
  
  function setSharesPosition() {
    var w = $(this), x = w.scrollLeft(), y = w.scrollTop();
    if (y > yMin) {
      if (y + sharesHeight < yMax) {position = 'fixed';}
      else {position = 'bottom';}
    }
    else {position = 'default';}
    // Clear all styles, then set appropriately.
    shares.removeAttr('style');
    shares.css(positionCSS[position]);
    // Adjust for horizontal scroll.
    if (position === 'fixed' && x > 0) {shares.css({left: sharesLeft - x});}
  }
  
  // Only slide the shares box on articles that are long enough.
  if (yMax - sharesHeight > yMin) {
          $(window).scroll(setSharesPosition);
          setSharesPosition();
  }
})
</script>
