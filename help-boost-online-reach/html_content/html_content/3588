<script type="text/javascript">
	$(function () {
		var $mainNav = $('#main_nav_160584'),
			$currentCategory = $mainNav.find('.current'),
			$currentSubItems = $currentCategory.find('.sub-items'),
			$lastNavCategory = $mainNav.find('.content-type').first().prev(),
			mainNavLeftEdge = $mainNav.offset().left,
			lastCategoryRightEdge = $lastNavCategory.offset().left + $lastNavCategory.width();

    $('.content-type', '.items' ).each( function() {
        $(this).hover(
						function () { $currentSubItems.hide(); },
						function () { $currentSubItems.show(); }
					);
      } );

		$('.sub-items', $mainNav)
			.each(function () {
				var $subItems = $(this),
					$thisCategory = $subItems.parent(),
					subItemsWidth = $subItems.outerWidth(),
					subItemsMarginLeft = -subItemsWidth / 2,
					subItemsLeftEdge,
					subItemsRightEdge;

				$subItems.css({
					left: '50%',
					marginLeft: subItemsMarginLeft
				});

				subItemsLeftEdge = $subItems.offset().left;
				subItemsRightEdge = subItemsLeftEdge + subItemsWidth;

				if (subItemsLeftEdge < mainNavLeftEdge) {
					subItemsMarginLeft += mainNavLeftEdge - subItemsLeftEdge;
				} else if (subItemsRightEdge > lastCategoryRightEdge) {
					subItemsMarginLeft += lastCategoryRightEdge - subItemsRightEdge;
				}

				$subItems.css('margin-left', subItemsMarginLeft);


				if ($thisCategory[0] == $currentCategory[0]) {
					$subItems.css('visibility', 'visible');
				} else {
					$thisCategory.hover(
						function () { $currentSubItems.hide(); },
						function () { $currentSubItems.show(); }
					);
				}

			});
	});
</script>







