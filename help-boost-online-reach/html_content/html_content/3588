		// Temporarily used for SOPA Blackout
		function isTimeForBlackout() {
			var now = new Date(),
				start = new Date(Date.UTC(2012, 0, 18, 13, 00, 00)),
				end = new Date(Date.UTC(2012, 0, 19, 01, 00, 00));
			return now > start && now < end;
		}
		if (isTimeForBlackout() && $.cookie('initial_blackout') && !$.cookie('remove_blackout')) {
			$('body').addClass('initial-blackout');
		}
</script>
