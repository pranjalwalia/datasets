<script type="text/javascript">
	$(function(){	
		var hostname = 'undefined';
		var inFrame = false;
		
		try {
			// check if we're in an iframe
			inFrame = (window.location != window.parent.location) ? true : false;
			// try to get the parent frame's hostname
			hostname = window.top.location.hostname;
		} catch ( error ) {
			// permission denied error
		}
		
		// check the hostname, display the button
		if ( hostname != 'undefined' ){
			switch( hostname ){
				case 'www.stumbleupon.com':
				case 'stumbleupon.com':
					$("#stumble-thru-good").show(0);	
				break;
			}
		} else if ( inFrame == true ) {
			// we're in a frame, show the stumble thru button
			$("#stumble-thru-good").show(0);
		}
	});
</script>

