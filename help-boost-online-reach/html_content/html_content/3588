      var loaded = {};
      function commentSubmitted( name, data )
      {
        $.post('/posts/notify_author_of_comment',
             {'post_id':55517, 'comment_text':data['js-CmtText']} );
        Mint.TT.behavior.record('add_comment','/post/how-pickle-juice-changed-the-world-of-sports-food-innovations-from-the-football-field');
      }

      function commentsLoaded( name, data )
      {
        var index = data['jcaIndex'];
        var userExists = !GOOD.user() == false;
        var count = 1;
        if( userExists )
          {
            count = 2;
          }
        if( !loaded[index] )
          {
            loaded[index] = 0;
          }

        if( loaded[index] >= count )
          {
            $('.jcaIndex' + data['jcaIndex'] + ' .js-PageMore').click();
          }
        loaded[index] = loaded[index] + 1;
      }

      var JSKitEvents = [ {subscribe: 'comments-data-loaded', callback: commentsLoaded}, {subscribe: 'comment-submit', callback: commentSubmitted} ];
    </script>
