<link href='http://www.tacticalintelligence.net/wp-content/plugins/yatcp/yatcp_style.css' rel='stylesheet' type='text/css' /><link rel='stylesheet' id='hybrid-print-css'  href='http://www.tacticalintelligence.net/wp-content/themes/TI/library/css/print.css?ver=0.2' type='text/css' media='print' />
<SCRIPT LANGUAGE="JavaScript">
function getAmounts (form) {
 	var thisPeriod = form.period.value;
 	var numAdults = form.adults.value;
 	var numChildren=form.children.value;


if (numAdults==''){
	numAdults=0;
}

if (numChildren==''){
	numChildren=0;
}

 	if (numAdults==0 && numChildren==0){
 		alert("Please re-enter the number of adults and children")
 		}
	else
	{
	 	form.grain.value=(Math.round(numAdults*(thisPeriod *(300/52)))+(Math.round(numChildren*thisPeriod * (150/52))));
	 	form.beans.value=(Math.round(numAdults*(thisPeriod *(60/52)))+(Math.round(numChildren*thisPeriod * (30/52))));
	 	form.dairy.value=(Math.round(numAdults*(thisPeriod *(73/52)))+(Math.round(numChildren*thisPeriod * (36/52))));
	 	form.sugar.value=(Math.round(numAdults*(thisPeriod *(60/52)))+(Math.round(numChildren*thisPeriod * (30/52))));
	 	form.leaven.value=(Math.round(numAdults*(thisPeriod *(7/52)))+(Math.round(numChildren*thisPeriod * (4/52))));
	 	form.salt.value=(Math.round(numAdults*(thisPeriod *(6/52)))+(Math.round(numChildren*thisPeriod * (3/52))));
	 	form.fat.value=(Math.round(numAdults*(thisPeriod *(30/52)))+(Math.round(numChildren*thisPeriod * (15/52))));
	 	form.water.value=((parseInt(numChildren)+parseInt(numAdults))*14);
	}

}

function getStorageReqs (form) {
     var numWheat= form.wheat.value;
     var numBeans= form.beans.value;
     var numRice=form.rice.value;
     var numMilk=form.milk.value;
     var numFlour=form.flour.value;
     var numSugar=form.sugar.value;
     var stack=form.stack.value;
     var numOats=form.oats.value;
     var numMacaroni=form.macaroni.value;
     var numBWide = 0;
     var numBHigh = 0;
     var ceilingMax = 0;
     var numBuckets=0;
     var numFootage=0;
     var useSI=false;

     if (form.useSI.checked == true){
          useSI=true;
     }

     if (stack==0){
          alert("Please enter how high your stack will be.");
     }
     else if (numWheat==0 && numBeans==0 && numRice==0 && numSugar==0 && numMilk==0 && numFlour==0 && numOats==0 && numMacaroni==0){
          alert("Please enter at least one storage item.");
     }
     else if (useSI == true)
     {

          numBuckets=(Math.ceil(numWheat/17)+Math.ceil(numBeans/15)+Math.ceil(numRice/16.4)+Math.ceil(numMilk/13)+Math.ceil(numFlour/15)+Math.ceil(numSugar/16)+Math.ceil(numOats/9)+Math.ceil(numMacaroni/9.5)   );

          form.numBuckets.value=numBuckets;
          numFootage=Math.floor(numBuckets/(.3048*stack));

          form.numFootage.value=numFootage;


     }
     else
     {

          numBuckets=(Math.ceil(numWheat/37)+Math.ceil(numBeans/33)+Math.ceil(numRice/36)+Math.ceil(numMilk/29)+Math.ceil(numFlour/33)+Math.ceil(numSugar/35)+Math.ceil(numOats/20)+Math.ceil(numMacaroni/21)   );

          form.numBuckets.value=numBuckets;
          numFootage=Math.floor(numBuckets/stack);
          if ( (numBuckets%stack)!=0){
               numFootage = numFootage+1;
          }

          form.numFootage.value=numFootage;


     }

}

</SCRIPT>

