GEL.thepage.initializer.addInitRoutine({
	name: 'nextSlider',
	namespace: 'widget.slider',
	callback: initNextSlider,
	priority: 60
});
function initNextSlider(){
	var _art,_right,wpElem;
	var _beenzagged=(function(){
		var _u = GEL.env.user;
		var isIE6 = (navigator.userAgent.toLowerCase().substr(25,6)=="msie 6") ? true : false;
		if(isIE6){
			$('#ody-nextstoryslider').css('display','none');
			return true;
		}
		return(_u.state || _u.yob || _u.zip )?true:false;
		})();
	var _laststate=(function(){
		var _t = new GEL.util.Cookie('GCIONSN');
		var _closed = _t.getValue('lastNextStoryState');
		return _closed;
		})();
	if(!_beenzagged){
		var _slidestate=_laststate=='false'?'handle':'';
		$('#conveyorbottom').css('z-index','-1');
		$('.footer-container').css('z-index','-1');
		_art=$('.ody-skin .content-container .ody-article');
		_right=$('.ody-skin .ody-aside');
		wpElem=_art.height()>_right.height()?_art:_right;
		var slider=GEL.widget.slider(wpElem,'ody-nextstoryslider',{
			close: '<img src="/odygci/p1/icnSlider_close.png" alt="close" width="12" height="12" />',
			closeCallback: function(){
				var _t = new GEL.util.Cookie('GCIONSN')
				_t.setValue('lastNextStoryState','false')
			},
			direction: 'right',
			handle: '<img src="/odygci/p1/icnSlider_open.png" alt="open" width="14" height="78" />',
			openCallback: function(){
				var _t = new GEL.util.Cookie('GCIONSN')
				_t.setValue('lastNextStoryState','true')
			},
			offset: function(){
				var offSet=$.waypoints('viewportHeight')-($('.ody-skin .content-container .ody-article .ody-article-bodytext').height()-$('.ody-skin .content-container .ody-article .ody-ob-taboola-wrapper').height());
				if(($('.ody-skin .content-container .ody-article .ody-article-bodytext').height()-$('.ody-skin .content-container .ody-article .ody-ob-taboola-wrapper').height())<=$('.ody-skin .ody-aside.aside').height()){
					offSet=$.waypoints('viewportHeight')-$('#ody-nextstoryslider').outerHeight(true)-$('.ody-skin .ody-aside.aside').height();
				}
				return offSet;
			},
			state: _slidestate
		});
	}
}
</script>
