<script type="text/javascript">
(function(){ 
GEL.thepage.initializer.addInitRoutine({ 
	name: "sharelinks",
	namespace: ['widget.ArticleTools','org.jQuery'],
	callback: initShareThis,
	priority: 31
});
GEL.thepage.initializer.addInitRoutine({ 
	name: "fontsize",
	namespace: "widget.ArticleTools",
	callback: initFontSize, 
	priority: 99
});
function initShareThis(){	// Facebook async initialization
	window.fbAsyncInit = function() {
		FB.init({ status: true, cookie: true, xfbml: true});
		FB.Event.subscribe('edge.create', function(href, widget) {
			var _vt = GEL.thepage.viewTracker;
			if (widget.dom.getAttribute("ref")) {
				_vt.providers.sitecat.trackLink("sharethis:facebook:like:" + widget.dom.getAttribute("ref"));
				}
		});
		FB.Event.subscribe('edge.remove', function(href, widget) {
			var _vt = GEL.thepage.viewTracker;
			if (widget.dom.getAttribute("ref")) {
				_vt.providers.sitecat.trackLink("sharethis:facebook:unlike:" + widget.dom.getAttribute("ref"));
				}
		});
	};
	(function() {
		var e = document.createElement('script');
		e.type = 'text/javascript';
		e.src = document.location.protocol + '//connect.facebook.net/en_US/all.js';
		e.async = true;
		document.getElementById('fb-root').appendChild(e);
	}());
	jQuery(document).ready(function($) {
		//Twitter share button
		jQuery.getScript('http://platform.twitter.com/widgets.js', function () {
			twttr.events.bind('tweet', function(event) {
				var _vt = GEL.thepage.viewTracker;
				var pos=event.target.parentNode.parentNode.parentNode.id;
				pos=(pos=='sharelinks')?'top':pos.replace('sharelinks-','');
				_vt.providers.sitecat.trackLink("sharethis:twitter:"+pos);
				});
		});
		// Google +1 Button
		var g = document.createElement('script');
		g.type="text/javascript";
		g.async = true;
		g.src = 'https://apis.google.com/js/plusone.js';
		document.getElementsByTagName('head')[0].appendChild(g);
	});
	var _w= 
		GEL.thepage.shareThis= 
			new GEL.widget.ShareThis("sharelinks"); 
	_w.init(); 
}
function initFontSize(){ 
	var _w= 
		GEL.thepage.fontSize= 
			new GEL.widget.FontSizeWidget("sharelinks"); 
	_w.init(); 
}
})();
function gplusone_share(r){
	var action=(r.state=='on')?"like":"unlike";	
	var _vt = GEL.thepage.viewTracker;
	_vt.providers.sitecat.trackLink("sharethis:google:"+action+":top");
}
</script>
