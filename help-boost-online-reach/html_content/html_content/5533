(function(){
	var _event= GEL.ement("ody-pgwidget");
	_event.on("galleryready",function(){
		GEL.register("widget.GalleryWidget", { 
			type: "script",
			depends: ["util.Selector","widget.ArticleTools","social.pluck"],
			requires: ["widget"],
			path: "widgets/photogallery/odypgcontrol.js"
		});
		GEL.register("legacy.Photogallery", { 
			path: "legacy/odypgcore.js", 
			depends: ["anim.YUIAnimator","widget.GELTabs","widget.GalleryWidget","widget.modal"],
			type: "script"
		}); 
		GEL.thepage.initializer.addInitRoutine({ 
			name: "photogallery", 
			priority: 10, 
			callback: function(){
				GEL.slide=function(){
					var 
						_t = GEL.ement('ody-pgtools').element.offsetTop ? GEL.ement('ody-pgtools').element.offsetTop : GEL.ement('ody-pgtools').element.getBoundingClientRect().top,
						_c = GEL.ement('ody-capcontrol'),
						_h = GEL.ement('ody-pgcapstxt').element.offsetHeight
					;
					GEL.log("offset top: "+_t+", offset height: "+_h);
					if(GEL.ement('ody-caps').element.style.height=='0px' || !GEL.ement('ody-caps').element.style.height){
						var anim = new YAHOO.util.Anim('ody-caps',{top:{to:parseInt(_t-_h)},height:{to:_h}},.75,YAHOO.util.Easing.easeIn);
						_c.element.innerHTML = '<span class="ody-hideArw">Hide Caption</span>';
					}else{
						var anim = new YAHOO.util.Anim('ody-caps',{top:{to:parseInt(_t+0)},height:{to:0}},.75,YAHOO.util.Easing.easeIn);
						_c.element.innerHTML = '<span class="ody-showArw">Show Caption</span>';
					}
					anim.animate();
				};
				if(GEL.thepage.pageinfo.galleryobject)
					OPG.initialSetup(); 
			}, 
			namespace: "legacy.Photogallery"
		}); 
		GEL.thepage.initializer.addInitRoutine({
			name: "pgcontrol",
			namespace: "widget.GalleryWidget",
			callback: initGalleryWidget,
			priority: 21
		});
		function initGalleryWidget() {
			var 
				_wid = GEL.thepage.GalleryWidget = new GEL.widget.GalleryWidget("ody-pgwidget")
				_gid = new GEL.widget.GalleryWidget("ody-pgmodaldata");
			;
			if(GEL.thepage.pageinfo.galleryobject){
				_wid.init();
				_gid.init();
			}
			GEL.ement('ody-pgtools').element.style.display='block';
			GEL.ement('ody-caps').element.style.display='block';
		}
	});
})();
