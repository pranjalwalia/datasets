GEL.thepage.initializer.addInitRoutine({ 
	name: "userauthpostload",
	namespace: ["legacy.GDNpostload"],
	priority: 120,
	callback: function(){ 
		/* 
		The wonders of UA never cease. 
		    Initializing these widgets will fail 
		    if done immediately after the above. 
		    Once the LoggedIn/Out widget is loaded 
		    it will run the callbacks for these 
		    widgets which, since they have not loaded,
		    will fail.  
		    Initializing UR before UA is complete will result 
		    in UR overwriting the GCIONID cookie 
		    This 1 second timeout should give 
		    UA enough time to complete 
		*/
		GDN.Api.Register(function(){ 
			GEL.env.user.onLogin(); 	
		}, null, "Login"); 
		GDN.Api.Register(function(){ 
			GEL.env.user.onLogout(); 
		}, null, "Logout");
		GDN.Autologin();
	GEL.thepage.on("ready", function(){ 
			_timer= setTimeout(init, 1000);
		}); 
		function init(){ 
			var _noneedzag=  (function(){ 
				var _u = GEL.env.user;
				return (_u.state || _u.yob || _u.zip || GEL.env.cookies.gcionid.values.status =="16") ? true : false;
				})();
			if(!_noneedzag) {
			// turned off by localdata
			}
			/*GDN.UA.Page.PluckLogin();
			GDN.UA.Page.PluckReg();
			GDN.UA.Page.PluckLogout();
			GDN.UA.Page.SaxotechLogin(); 
			GDN.UA.Page.Newsletters();*/
		}
	}
}); 
GEL.thepage.initializer.addInitRoutine({ 
	name: "userauthlateload",
	priority: 300, 
	callback: function(){
	if ( GEL.config.userauth.enable_premium == false &&
		 GEL.thepage.pageinfo.categoryid.toUpperCase()!="PLUCK_REGISTER"  &&
		 GEL.thepage.pageinfo.categoryid.toUpperCase()!="PLUCK_SIGNIN"  &&
		 GEL.thepage.pageinfo.categoryid.toUpperCase()!="NLETTERREGISTER" &&
		 GEL.thepage.pageinfo.categoryid.toUpperCase()!="GETPUBLISHED01" &&
		 GEL.thepage.pageinfo.categoryid.toUpperCase()!="NLETTERSIGNIN") {
			GEL.util.uautil.MakeRemoteCallsilently("signup","socialprovider","signup.html");
			GEL.util.uautil.MakeRemoteCallsilently("userauthlogin","socialprovider","login.html");
		/*	Adding another div for Newsletter sample page */
		if(GEL.util.uautil.isnlettersubscribe()){
			var el = document.createElement("div");
			el.setAttribute('id', 'NLSampleModalcontainer');
			var temp_elem = document.getElementById("UAModalcontainerwrapper");
			temp_elem.appendChild(el);
			}
		}
	}
});
