	;
	_xhr.open('get', '/apps/pbcs.dll/gallery?'+_uri+'&template=galleryobject');
	_xhr.onreadystatechange = function () {
		if (_xhr.readyState === 4) {
			GEL.thepage.pageinfo.galleryobject=JSON.parse(_xhr.responseText);
			var 
				_d = document,
