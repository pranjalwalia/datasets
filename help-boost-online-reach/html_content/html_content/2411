<script>
	if (typeof doGoal === 'function') {
		$('.gallery-list li a').click(function () {
			doGoal();
		});
	}
	jQuery(function ($) {
		var originalList = $('#gallery_list_related_original'), selectedList;
		// Always show the original set; it's the one with the ads.
		originalList.parent('.gallery-list').show();
		if (galleryListAB !== 'original') {
			selectedList = $('#gallery_list_related_' + galleryListAB);
			// If our A/B selection is not the original, then re-populate the 
			// non-ad gallery posts with the appropriate other posts.
			originalList.find('li.content').each(function () {
				var nextItem = selectedList.find('li.content').first();
				// Make sure the right margin is correct.
				if ($(this).hasClass('last')) {
					nextItem.addClass('last');
				}
				else {
					nextItem.removeClass('last');
				}
				$(this).replaceWith(nextItem);
			});
			// Make sure we have updated comment counts; they might have got 
			// lost in the item shuffle above.
			load_comment_counts();
		}
	});
</script>
