    } else {
        jQuery(function() {
            Epi.text.truncatedVersion("recipeIntroText", "\"As a student at Bloomsburg University, I spent many Sunday afternoons at Russell&#8217;s restaurant in downtown Bloomsburg, Pennsylvania,\" writes Marganne...");
        });
    }
