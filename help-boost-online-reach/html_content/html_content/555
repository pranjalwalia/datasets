<script type="text/javascript">
jQuery(document).ready(function($){
                var shareLinks = $("div.hrecipe li.Share div.sharePanelTitle a");
                
                shareLinks.filter(".delicious, .stumbleupon, .digg, .reddit").each(function() {
                                var link = $(this);
                                link.attr("href", link.attr("href").replace("url=window.location.href&", "url=' + encodeURIComponent(window.location.href) + '&"));
                });
                
              try {
                var twitLinks = shareLinks.filter(".twitter");
                twitLinks.attr("href", twitLinks.attr("href").replace("+window.location.href'", "+'+ encodeURIComponent(window.location.href)"));
              } catch (err) {}
}); 

function HoverLinkItem(sUrl, linkName) {   
        if (linkName == "facebook") {       
            shareFacebook(sUrl);
        }
        else {
            shareSite(sUrl, linkName);
        }
    }    
</script>
