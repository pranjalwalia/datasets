<script type="text/javascript">
    function SendSecureLogin(validationGroup) {
        var validationResult;
        document.forms[0].action = modalLoginUrl;
        jQuery(".left_container input").removeClass("invalid_text");
        if (typeof (Page_ClientValidate) == 'function')
                  validationResult = Page_ClientValidate(validationGroup);            
        if (validationResult)
        {
           //ApplyUnicaEventTag('ev=member_login_bt');
           document.forms[0].submit();
        }
        else
        {            
           showValidationErrors(validationGroup);
        }  
    }
     
    function openLogin(registrationSource, message, destinationUrl, modalAction)
    {
        $signupPanel = jQuery("#ModalSignupPanel").clone(true);
        if (typeof (registrationSource) != "undefined") {
            jQuery("#" + registrationSourceId).val(registrationSource);         
         
            if (registrationSource != null) {
                var hdnField = jQuery('.modal_login input[id$="hdnModalRegistrationUrl"]');
                var modalRegistrationUrl = hdnField.val();
                modalRegistrationUrl += (modalRegistrationUrl.indexOf("?") < 0 ? "?" : "&");
                modalRegistrationUrl += "esrc=";
                modalRegistrationUrl += registrationSource;
                hdnField.val(modalRegistrationUrl);
            }
        }
        if (typeof (message) != "undefined") {
            var arr = message.split('|');
            if(arr.length != 0){
                if(arr.length > 1)
                    jQuery("#" + headLineId).text(arr[1]); 
                jQuery("#" + instructionsId).text(arr[0]);                    
            }
            else{
                jQuery("#" + instructionsId).text(message);
            }
        }
        if (typeof (destinationUrl) != "undefined") {
            jQuery("#" + destinationUrlId).val(destinationUrl);         
        }
        if (typeof (modalAction) != "undefined") {
            jQuery("#" + loginActionHiddenId).val(modalAction);        
        }
        OpenVariableDialog('Login_LoginDialog');
    }

      function closeLogin()
    {
        CloseDialog('Login_LoginDialog', false);       
    }
   
           
    function trackModalRegistrationStep(stepNumber) {        
    switch (stepNumber) {
        case 1:
            ApplyUnicaEventTag('ev=BCReg_ModalPopups_bt');
            break;
        case 2:
            ApplyUnicaEventTag('ev=BCReg_enter_ZipDOB_bt');
            break;
        case 3:
            ApplyUnicaEventTag('ev=BCReg_enter_email_bt');
            break;
        case 4:
            ApplyUnicaEventTag('ev=BCReg_create_password_bt');
            break;
        case 5:
            ApplyUnicaEventTag('ev=BCReg_select_NWletter_bt');
            break;
        }
        
    }   
    var modalLoginUrl = 'https\x3a\x2f\x2fwww.bettycrocker.com\x2f\x7e\x2fauth\x2fUserLoginProcessor.ashx\x3fWT.dcsvid\x3dNjUzOTE3MTMzOQS2\x26rvrin\x3dCCFA2F64-475A-41D8-BA08-B8E0439180B3\x26WT.mc_id\x3dNewsletter_BC_06_09_2011_Control';
    var modalRegistrationUrl = 'https\x3a\x2f\x2fwww.bettycrocker.com\x2f\x7e\x2freg\x2fRegistrationProcessor.ashx\x3fWT.dcsvid\x3dNjUzOTE3MTMzOQS2\x26rvrin\x3dCCFA2F64-475A-41D8-BA08-B8E0439180B3\x26WT.mc_id\x3dNewsletter_BC_06_09_2011_Control';
    var modalSubscriptionUrl = 'https\x3a\x2f\x2fwww.bettycrocker.com\x2f\x7e\x2fsub\x2fSubscriptionProcessor.ashx\x3fWT.dcsvid\x3dNjUzOTE3MTMzOQS2\x26rvrin\x3dCCFA2F64-475A-41D8-BA08-B8E0439180B3\x26WT.mc_id\x3dNewsletter_BC_06_09_2011_Control';
    var loginActionHiddenId = 'Login_LoginActionHidden';
     var instructionsId = 'Login_lblInstructions';
