			<iframe scrolling="no" 
				frameborder="0" 
				allowtransparency="true" 
				style="width: 95px; height: 21px;" 
				src="http://www.facebook.com/plugins/like.php?href=http://findarticles.com/p/articles/mi_m0820/is_n228/ai_18523878/&amp;layout=button_count&amp;show_faces=true&amp;width=95&amp;action=like&amp;colorscheme=light&amp;height=21" 
				class="fb-like">
