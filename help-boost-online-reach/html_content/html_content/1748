//kdmaininit
function kdmaininit() {

AOL.getJS('http://o.aolcdn.com/os_merge/?file=/living-food/js/gjcarousel.min.js',function(){

$("#kitchendaily-tv-carousel").jcarousel({
	scroll: 1,
	buttonNextHTML: "<div>&#9658;</div>",
	buttonPrevHTML: "<div>&#9668;</div>"
});
var cnt = $("#kitchendaily-tv-carousel .jcarousel-item").length;

var cur = $("#current-page").html();

$("#total-pages").html("of " + cnt);

$(".jcarousel-prev").click(function(){

	if(cur > 1) {
		cur--;

		$("#current-page").html(cur);

		$('#kitchendaily-tv .jcarousel-item-' + cur + ' img').each(function(i){

			$(this).attr('src', $(this).attr('data-img'));
		});
	}
});

$(".jcarousel-next").click(function(){

	if(cur < cnt) {
		cur++;

		$("#current-page").html(cur);

		$('#kitchendaily-tv .jcarousel-item-' + cur + ' img').each(function(i){

			$(this).attr('src', $(this).attr('data-img'));
		});
	}
});

$('#kitchendaily-tv .jcarousel-item-1 img').each(function(i){

	$(this).attr('src', $(this).attr('data-img'));

});

var h = $('#kitchendaily-tv .pages').html();

$('#kitchendaily-tv .pages').remove();

$('#kitchendaily-tv .jcarousel-prev').after('<div class="pages">' + h + '</div>');

$('#kitchendaily-tv .jcarousel-prev').before('<a href="http://www.kitchendaily.com/videos/" title="SEE VIDEOS"class="see-videos">SEE VIDEOS </a>');




});
}
