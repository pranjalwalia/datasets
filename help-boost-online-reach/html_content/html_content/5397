var IBS_action = "/search/form.html";

var YP_action = "";

var OV_action = "http://www.wxii12.com/websearch/index.html?qt=";

/**
* Changes form action attribute value based on which searches
* are present for the current site.
*/
function doSubmit(form, action) {
form.action = action;
if (!form) {}

/* monkey with the query values if YP search */
else if (action == YP_action) {
if (YP_action.substr(YP_action.length-1) == "=") { /* append query terms if YP_action is setup for it */
YP_action += escape(form.elements.qt.value);
}

document.location.href = YP_action;

}

else {
form.submit();
}
