<script type="text/javascript">
IBSYS.using('WebTrends');
if (typeof ib_WTCustomTags == "function") ib_WTCustomTags();
if (typeof IBSYS.WebTrends.collectUserData == 'function') IBSYS.WebTrends.collectUserData();
if (typeof dcsVar == "function") dcsVar();

if (IBSYS.application.Application.getProperty('tracking.remapReferrer') && document.referrer && IBSYS.propPath('siteinfo.distribution') == 'ext') {
  (function() {
    var ref = document.referrer.match(/(?:[^:]+:\/\/)?([^\/]+)([^\?]*)\??(.*)/);
    window.DCS.dcssip = ref[1];
    window.DCS.dcsuri = ref[2];
    window.DCS.dcsqry = ref[3];
  })();
}

if (IBSYS.propPath('IBSYS.widgets.LinksWeLike')) {
  IBSYS.widgets.LinksWeLike.prepLegacyWT();
}

if (typeof dcsMeta == "function")  dcsMeta();
if (typeof dcsFunc == "function") dcsFunc("dcsAdv");
if (IBSYS && IBSYS.event) {
  (function() {
    var e = new IBSYS.event.GlobalEvent('beforeDcsTag');
    try {
      IBSYS.application.Application.announceEvent(e);
    } catch (ex) {}
  })();
}
if (typeof dcsTag == "function") {
  if (window.location.hostname.indexOf('channel4000.com') < 0) {
    if (IBSYS.qs('noWTPageView') != 'true' && !IBSYS.noLegacyTracking) {
      dcsTag();
    }
  } else {
    setTimeout(function() {
      dcsTag();
    }, 2000);
  }
}
