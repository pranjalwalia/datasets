<script type="text/javascript">
//<![CDATA[
  
  function populateTwitterForm(user) {
    $('#twitter_name').val(user.data('screen_name'));
    $('#twitter_id').val(user.data('id'));
    $('#twitter_image').val(user.data('profile_image_url'));
  }
  
  var handleTwitterLogin;
  $(document).bind("twitter:loaded", function() {
    twttr.anywhere(function (T) {
      if (T.isConnected()) {
        handleTwitterLogin = function (val) {
          $.post("/users/twitter_connect_login", { 'twitter_id': T.currentUser.data('id'), 'post_auth':  val });
          populateTwitterForm(T.currentUser);
        } 
      } else { 
        if ($('#twitter_login').length) {
          $('#twitter_login').html("<span id='twitter_login'></span>");  
          T("#twitter_login").connectButton({
            authComplete: function(user) {
              populateTwitterForm(user);
              $.post("/users/twitter_connect_login", { 'twitter_id': T.currentUser.data('id'), 'post_auth': '' });
            }
          });
        } 
        if ($('#twitter_login_comments').length) {
          $('#twitter_login_comments').html("<span id='twitter_login_comments'></span>");  
          T("#twitter_login_comments").connectButton({
            authComplete: function(user) {
              populateTwitterForm(user);
              $.post("/users/twitter_connect_login", { 'twitter_id': T.currentUser.data('id'), 'post_auth': '' });
            }
          });
        }
      }
    });
  });
  
  $(function() {
    $('twitter_login_form').submit(function() {
      $('#twitter_login_form').hide();
      $('#twitter_login_spinner').show();
    });
  });

//]]>
</script>

    
