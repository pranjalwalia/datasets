</div><script language="javascript">
function validateForm(obj){
	var toValidate = obj.comments.value;
	var re = /([^=]+)http:\/\/www./i;
	var r = toValidate.replace(re,"$1");
	var re = /^http:\/\/www./i;
	var r2 = r.replace(re,"");
	var re = /([^=]+)www./i;
	var r3 = r2.replace(re,"$1");
	var re = /^www./i;
	var r4 = r3.replace(re,"");
	var re = /([^=]+)http:\/\//i;
	var r5 = r4.replace(re,"$1");
	var re = /^http:\/\//i;
	var r6 = r5.replace(re,"");
	obj.comments.value = r6;
	obj.submit();
}
</script>
