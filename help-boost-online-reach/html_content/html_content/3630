    <script type="text/javascript">
        (function() {
            var jnxtac = jQuery.noConflict(true);
            jnxtac("#txtsearch").autocomplete({ cid: '51da71b4dd3ae4e8cc2b70f0e93e179d' });
            jnxtac("#txtsearch").autocomplete("option", "num_products", 8);
            jnxtac("#txtsearch").autocomplete("option", "num_terms", 5);
            jnxtac.ui.autocomplete.prototype._handleItemSelect = function(item) {
                location.href = "http://www.prankplace.com/search/Search.aspx?keywords=" + escape(item);
                this.element.val(item);

            };
            jnxtac.ui.autocomplete.prototype._drawProduct = function(item) {
                var image = item.Imageurl;
                image = image.replace("midsize", "thumbs");
                image = image.replace(".jpg", "_th.jpg");
                image = image.replace(".gif", "_th.gif");
                image = image.replace(".png", "_th.png");
                var price = (item.Price && item.Price !== '(null)') ? '<span class="nxt-ac-item-price">$' + item.Price + '</span>' : '';
                return '<img src="' + image + '"><span class="nxt-ac-item-text"><p>' + item.Name + '</p>' + price + '</span>';
            };
            jnxtac.ui.autocomplete.prototype._positionMenu = function(ul) {
                ul.position({
                    my: "left top", 	// position the top left of my autocomplete dropdown (valid values: top, center, bottom, left, right)
                    at: "left bottom", // to the bottom left of 
                    of: this.element, // my search box the autocomplete is being attached to
                    offset: "-95 0", // adding 50 to the left positioning and 50 to the top positioning of the dropdown
                    collision: "none"	// with no collision detection
                });
            };

            jnxtac.ui.autocomplete.prototype._handleProductSelect = function(item) {
                var url = item.Producturl;
                url = "http://www.prankplace.com" + item.Producturl;
                location.href = url;
            };
        })();
        </script>
        <script type="text/javascript">
            adroll_adv_id = "ZPZWV6ANENHXLAF42ZYURS";
            adroll_pix_id = "W3J6TOGFZ5DWPOXZSZ3ZY3";
            (function () {
            var oldonload = window.onload;
            window.onload = function(){
               __adroll_loaded=true;
               var scr = document.createElement("script");
               var host = (("https:" == document.location.protocol) ? "https://s.adroll.com" : "http://a.adroll.com");
               scr.setAttribute('async', 'true');
               scr.type = "text/javascript";
               scr.src = host + "/j/roundtrip.js";
               ((document.getElementsByTagName('head') || [null])[0] ||
                document.getElementsByTagName('script')[0].parentNode).appendChild(scr);
               if(oldonload){oldonload()}};
            }());
        </script>
    </body>
