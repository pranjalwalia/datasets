        $(document).ready(function () {
            $('#print-cart').bind('click', function () {

                var pdpPrint2 = pdpPrint.replace('xxxxxx', '378');
                var w = 815,
                                h = 795,
						        winl = (screen.width - w) / 2,
						        wint = (screen.height - h) / 2,
						        settings;

                settings = 'height=' + h + ','
                settings += 'width=' + w + ',';
                settings += 'top=' + wint + ',';
                settings += 'left=' + winl + ',';
                settings += 'toolbar=0, scrollbars=yes, location=0, statusbar=0 ,menubar=0 ,resizable=0';

                window.open(pdpPrint2, 'PDPPrintablePage', settings);
                return false;
            });

        });	
			                    
