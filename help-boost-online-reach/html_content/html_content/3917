<script type="text/javascript">
    function SendSecureLogin(validationGroup) {
        var validationResult;
        document.forms[0].action = modalLoginUrl;
        jQuery(".left_container input").removeClass("invalid_text");
        if (typeof (Page_ClientValidate) == 'function')
                  validationResult = Page_ClientValidate(validationGroup);            
        if (validationResult)
        {
           //ApplyUnicaEventTag('ev=member_login_bt');
           document.forms[0].submit();
        }
        else
        {            
           showValidationErrors(validationGroup);
        }  
    }
     
    function openLogin(registrationSource, message, destinationUrl, modalAction)
    {
        $signupPanel = jQuery("#ModalSignupPanel").clone(true);
        if (typeof (registrationSource) != "undefined") {
         jQuery("#" + registrationSourceId).val(registrationSource);         
        }
        if (typeof (message) != "undefined") {
         jQuery("#" + instructionsId).text(message);         
        }
         if (typeof (destinationUrl) != "undefined") {
         jQuery("#" + destinationUrlId).val(destinationUrl);         
        }
        if (typeof (modalAction) != "undefined") {
         jQuery("#" + loginActionHiddenId).val(modalAction);        
        }

        OpenVariableDialog('Login_LoginDialog');
    }
      function closeLogin()
    {
        CloseDialog('Login_LoginDialog', false);       
    }
   
           
    function trackModalRegistrationStep(stepNumber) {        
    switch (stepNumber) {
        case 1:
            ApplyUnicaEventTag('ev=BCReg_ModalPopups_bt');
            break;
        case 2:
            ApplyUnicaEventTag('ev=BCReg_enter_ZipDOB_bt');
            break;
        case 3:
            ApplyUnicaEventTag('ev=BCReg_enter_email_bt');
            break;
        case 4:
            ApplyUnicaEventTag('ev=BCReg_create_password_bt');
            break;
        case 5:
            ApplyUnicaEventTag('ev=BCReg_select_NWletter_bt');
            break;
        }
        
    }   
