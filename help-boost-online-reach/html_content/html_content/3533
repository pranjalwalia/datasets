<script>
function fnsearch(){

	day1 = document.search.SelDay.value;
	month1 = document.search.SelMonth.value;
	year1 = document.search.SelYear.value;
	tmpdate=month1+"/"+day1+"/"+year1	 
	fromdate=new Date(year1,month1-1,day1)	
	if (!((fromdate.getDate()==day1)&&(fromdate.getMonth()+1==month1)&&(fromdate.getFullYear()==year1))){
		alert("Please Select a valid Date")	
		document.search.SelDay.focus();
		return false;	
	}	
	tmpcatid=document.search.ncategoryid.value;
	tmpkeyword=document.search.keyword.value;
	document.search.action="http://www.medindia.net/news/newsday_list.asp?ddate="+tmpdate+"&ncategoryid=" +tmpcatid +"&keyword=" +tmpkeyword	
	return true;
}	
</script>
<form name="search" onsubmit="return fnsearch()" method="post" style="display:inline">
<input type="hidden" value="id" />
