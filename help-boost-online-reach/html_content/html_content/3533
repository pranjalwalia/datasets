<script type="text/javascript">
    showWt('wt','wtab', '3',"2") 
   var grouppage
   var memberpage
   var discussionpage
   var contenttype
    grouppage=1
    memberpage=1
    discussionpage=1
    contenttype=""
  
 
function fnnextmember()
{
    
    memberpage=memberpage+1
    fnmember()
}
function fnpreviousmember()
{
    memberpage=memberpage-1
    fnmember()
}  

function fnmember()
{    
    document.getElementById("loadingmembercontent").style.display="inline";
    xmlHttp=GetXmlHttpObject();
    if (xmlHttp==null)
    {
        alert ("Your browser does not support AJAX!");
        return;
    } 
     
    var url='http://www.medindia.net/webservices/News_Ajaxgetmembers.asp';
    
    
	 
    params="";
    params=params+"sg_CategoryID=0";
    params=params+"&sg_PatientinfoIDs=";
    params=params+"&sg_SearchAlphabet=";
    params=params+"&sg_supportgroupid=383";
    params=params+"&sg_SearchText=";
    params=params+"&sg_SearchType=5";
    params=params+"&sg_PageNo=" +memberpage;
    params=params+"&sg_Pagesize=18";
    params=params+"&sg_SortMemberColumn=userid";
    params=params+"&sid="+Math.random();
    xmlHttp.open("POST",url,true);
    params=encodeURI(params)
    xmlHttp.setRequestHeader("Content-type", "application/x-www-form-urlencoded;charset=UTF-8");
    xmlHttp.setRequestHeader("Content-length", params.length);
    xmlHttp.setRequestHeader("Connection", "close");
    contenttype="membercontent"
    xmlHttp.onreadystatechange=stateViewReply;
    xmlHttp.send(params);

} 
 
function fnnextdiscussion()
{
    discussionpage=discussionpage+1
    fndiscussion()
}
function fnpreviousdiscussion()
{
    discussionpage=discussionpage-1
    fndiscussion()
}    

function fndiscussion()
{    
    document.getElementById("loadingdiscussioncontent").style.display="inline";
    xmlHttp=GetXmlHttpObject();
    if (xmlHttp==null)
    {
        alert ("Your browser does not support AJAX!");
        return;
    } 
    
    var url='http://www.medindia.net/webservices/News_Ajaxgetdiscussions.asp';
    
  
	
    params="";
    params=params+"sg_CategoryID=0";
    params=params+"&sg_PatientinfoIDs=";
    params=params+"&sg_supportgroupid=383";
    params=params+"&sg_SearchAlphabet=";
    params=params+"&sg_SearchText=";
    params=params+"&sg_SearchType=5";
    params=params+"&sg_PageNo=" +discussionpage;
    params=params+"&sg_Pagesize=3";
    params=params+"&sg_SortDiscussionColumn=posteddate";
    params=params+"&sid="+Math.random();
    xmlHttp.open("POST",url,true);
    params=encodeURI(params)
    xmlHttp.setRequestHeader("Content-type", "application/x-www-form-urlencoded;charset=UTF-8");
    xmlHttp.setRequestHeader("Content-length", params.length);
    xmlHttp.setRequestHeader("Connection", "close");
    contenttype="discussioncontent"
    xmlHttp.onreadystatechange=stateViewReply;
    xmlHttp.send(params);

}

function stateViewReply() 
{ 
	if (xmlHttp.readyState==4){
	    if (contenttype!=""){
	   	    document.getElementById(contenttype).innerHTML =xmlHttp.responseText;
	        eval("document.getElementById('loading"+contenttype+"')").style.display='none';	 
	   	}
	    contenttype="";
	}
}
function GetXmlHttpObject()
{
	var xmlHttp=null;
	try {xmlHttp=new XMLHttpRequest();}
	catch (e)
  	{
		try { xmlHttp=new ActiveXObject("Msxml2.XMLHTTP"); }
	  	catch (e)  	{ xmlHttp=new ActiveXObject("Microsoft.XMLHTTP");}
  	}
	return xmlHttp;
}


</script>
</div>
