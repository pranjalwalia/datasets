<script type="text/javascript">
jQuery(document).ready(function(){
	jQuery("textarea.cmtboxinput").focusin(function(){
		this.value = this.defaultValue==this.value?"":this.value;
		jQuery(this).addClass("cmtboxinputfocus");
	})
	.focusout(function(){
		this.value=this.value==""?this.defaultValue:this.value;
		jQuery(this).removeClass("cmtboxinputfocus");
	});
});
</script>
