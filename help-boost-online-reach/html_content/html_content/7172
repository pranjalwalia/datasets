  <script language="JavaScript" type="text/javascript">
function goOtherSite(txtSelectName) {
	var objSelect = document.getElementById(txtSelectName);
	if (objSelect) {
		var vUrl=getSelectedValue(objSelect);
		if (vUrl=="") {
			alert ("Choose a site from the dropdown before clicking Go");
		} else {
			window.location=vUrl;
		}
	}
}
function getSelectedValue(objSelect) {
	for (i=0; i<objSelect.options.length; i++) {
		if (objSelect.options[i].selected) {
			return objSelect.options[i].value;
			break;
		}
	}
	return "";
}
  </script>
