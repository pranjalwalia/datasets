    function registerGetBlogUrls() {
      gadgets.rpc.register('getBlogUrls', function() {
        var holder = {};
        
          
            
            
              holder.currentPost = "http://www.blogger.com/feeds/876663859573125537/posts/default/2074131145743250608";
            
            
            
              holder.currentComments = "http://www.blogger.com/feeds/876663859573125537/2074131145743250608/comments/default";
            
            holder.currentPostUrl = "";
            holder.currentPostId = 2074131145743250608
          
          
          
            holder.postFeed = "http://www.blogger.com/feeds/876663859573125537/posts/default";
          
          
          
            holder.commentFeed = "http://www.blogger.com/feeds/876663859573125537/comments/default";
          
          holder.currentBlogUrl = "http://icookstuff.blogspot.com/";
          holder.currentBlogId = "876663859573125537";
        
        return holder;
      });
    }
  </script>
<script type="text/javascript">
  if (!window.registeredCommonBloggerCallbacks) {
    window.registeredCommonBloggerCallbacks = true;

    gadgets.rpc.register('resize_iframe', function(height) {
      var el = document.getElementById(this['f']);
      if (el) {
        el.style.height = height + 'px';
      }
    });

    
    gadgets.rpc.register('set_pref', function() {});

    registerGetBlogUrls();
  }
  </script>
