	(function(){
		GEL.thepage.initializer.addInitRoutine({
			name: 'shareTools',
			namespace: 'org.jQuery.waypoint',
			callback: function(){
				$('#article-bodytext').waypoint(function(e,d){
					// Check to see if #sharelinks is in view.
					var docViewTop = $(window).scrollTop();
					var docViewBottom = docViewTop + $(window).height();
					var elemTop = $('#sharelinks').offset().top;
					var elemBottom = elemTop + $('#sharelinks').height();
					if(!((elemBottom >= docViewTop) && (elemTop <= docViewBottom))){
						$('#sharelinks-bottom').removeClass('gel-hidden');
					}
				},{
					offset: 'bottom-in-view',
					triggerOnce: true
				});
			},
			priority: 101
		});
	})();
