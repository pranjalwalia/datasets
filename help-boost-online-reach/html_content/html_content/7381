	_c.hours = _t.getHours();
	_c.minutes = _t.getMinutes();
	_c.dayminutes = parseInt(parseInt(_t.getHours()*60)+_t.getMinutes());
	_a.newDate = (function(){
		var _date = new Date('February 26, 2012 01:11 AM' + " GMT" + _OFFSET + "00");
		return _date.toLocaleString();
	})();
