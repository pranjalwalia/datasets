  $('#adRightMiddle').attr('onclick', adClick.replace('%ad%', 'ad2'));

  var providerUrls = [  ];
  $('a[target=_blank]').not($('#adRightTop a, #sponsor a, #adRightMiddle a')).each(function (i) {
	if (providerUrls.length > 0) {
      for (var j=0; j < providerUrls.length; j++) {
        if ( ($(this).attr('onclick') == undefined) && ($(this).attr('href').toLowerCase().indexOf(providerUrls[j].toLowerCase()) != -1) ) {
          $(this).attr('onclick', providerClick);
        }
        else if ($(this).attr('onclick') == undefined) {
          $(this).attr('onclick', standardClick);
        }
      }
	}
	else {
      $(this).attr('onclick', standardClick);
	}
  });
