/* Add onclick elements to all links with target="_blank" set.  If we're on a page where the
 * provider property (prop17) is set and the link looks like a provider URL, set the provider id.
 */
 $(function() {
  var standardClick = "var s=s_gi(s_account); s.events='event15'; s.linkTrackVars='events'; s.linkTrackEvents='event15'; "+
    "s.tl(this,'o','Outbound Click');";
  var adClick = "var s=s_gi(s_account); s.events='event5'; s.eVar14='%ad%'; "+
    "s.linkTrackVars='prop17,prop25,eVar14,eVar17,eVar25,events'; s.linkTrackEvents='event5';"+
