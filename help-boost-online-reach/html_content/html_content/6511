<script type="text/javascript" language="javascript">function RemoveScriptTag(textField) { if (textField.val() != undefined) { var fieldData = textField.val(); fieldData = fieldData.replace(/<script[^>]*?>/gi, ''); fieldData = fieldData.replace(/<\/script>/gi, ''); textField.val(fieldData); return fieldData; }} </script>
<script type="text/javascript" language="javascript"> function RemoveScriptTagFromAll() {RemoveScriptTag($('#captcha')); RemoveScriptTag($('#author')); RemoveScriptTag($('#email')); RemoveScriptTag($('#url')); RemoveScriptTag($('#comments')); return true; }</script>
