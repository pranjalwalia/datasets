//<![CDATA[

			$(function () {
				$('#shares_for_post_28221').load('/thin/index.php?action=shares&post_id=28221', function () {
					var shares = $(this),
						content = $('div.content'),
						sharesHeight = shares.outerHeight(true),
						sharesLeft = shares.offset().left,
						yMin = shares.offset().top - parseInt(shares.css('margin-top'), 10),
						yMax = content.offset().top + content.outerHeight(),
						position = 'default',
						positionCSS = {
							'default': {},
							'fixed': {position: 'fixed', top: 0},
							'bottom': {position: 'absolute', top: yMax - sharesHeight}
						};
					function setSharesPosition() {
						var w = $(this), x = w.scrollLeft(), y = w.scrollTop();
						if (y > yMin) {
							if (y + sharesHeight < yMax) {
								position = 'fixed';
							}
							else {
								position = 'bottom';
							}
						}
						else {
							position = 'default';
						}
						// Clear all styles, then set appropriately.
						shares.removeAttr('style');
						shares.css(positionCSS[position]);
						// Adjust for horizontal scroll.
						if (position === 'fixed' && x > 0) {
							shares.css({left: sharesLeft - x});
						}
					}
					// Only slide the shares box on articles that are long enough.
					if (yMax - sharesHeight > yMin) {
						$(window).scroll(setSharesPosition);
						setSharesPosition();
					}
				});
			});
//]]>
</script>
