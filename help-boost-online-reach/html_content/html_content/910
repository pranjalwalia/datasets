    jQuery(function ($) {
		var ua = $.browser;
	 var userAgent = navigator.userAgent.toLowerCase(); 
	$.browser.chrome = /chrome/.test(navigator.userAgent.toLowerCase()); 

		
		$('.basic').click(function (e) {
			
			$('#basic-modal-content').modal();
			//alert();
			if ( ua.mozilla){
			$('#simplemodal-container a.modalCloseImg').css('background-image','url(/website/images/fav_firefox.jpg)');	
			}
			
			else if ( $.browser.chrome){
				$('#simplemodal-container a.modalCloseImg').css('background-image','url(/website/images/fav_chrome.jpg)');	
				
			}
			else if( ua.safari){
				$('#simplemodal-container a.modalCloseImg').css('background-image','url(/website/images/fav_safari.jpg)');	
			}
			else if ( ua.msie){
				$('#simplemodal-container a.modalCloseImg').css('background-image','url(/website/images/fav_msie.jpg)');	
			}
			else{
				$('#simplemodal-container a.modalCloseImg').css('background-image','url(/website/images/fav_unknown.jpg)');	
			}
			
			
			return false;
		});
	});
</script>
<script type="text/javascript">



/* added by cwelch. 
If Header Banner Ad is available, prepareNav() needs to execute immediately to accommodate banner ad height by pushing nav position down, otherwise prepareNav() will execute when page finishes loading.
*/

	$(function() {
		prepareNav();
	});





function prepareNav(){
	var to = $('#floatNav').offset();
	if ($(window).scrollTop() > to.top){ $('#floatNav').css({'position':'fixed','top':'0px'});}
	else{$('#floatNav').css({'position':'relative','top':'0px'});}
	$(window).scroll(function(){
		if ($(window).scrollTop() > to.top){ $('#floatNav').css({'position':'fixed','top':'0px','border-top':'6px solid #fff'});}
		else{$('#floatNav').css({'position':'relative','top':'0px','border-top':'0'});}
	});
	
	
	

	$('li.nvv').mouseover(function(){
		if ($(this).find('a.nav8').text() == 'FASHION'){
			
			$(this).find('a.nav8').css('background-position','-77px -182px');
		}
		if ($(this).find('a.nav2').text() != ''){
			$(this).find('a.nav2').css('background-position','-238px -26px');
		}
		if ($(this).find('a.nav5').text() == 'SHOP'){
			$(this).find('a.nav5').css('background-position','-58px -104px');
		}
	}).mouseout(function(){
		if ($(this).find('a.nav8').text() == 'FASHION'){
			$(this).find('a.nav8').css('background-position','0 -182px');
		}
		if ($(this).find('a.nav2').text() != ''){
			$(this).find('a.nav2').css('background-position','0 -26px');
		}
		if ($(this).find('a.nav5').text() == 'SHOP'){
			$(this).find('a.nav5').css('background-position','0 -104px');
		}
	});
};


</script>



