slideshow.slideUrls = ["\/our-dirty-mouths\/4-b-188311","\/chips-pretzels-and-popcorn\/4-b-188312","\/diet-sodas\/4-b-188356","\/coffee\/4-b-188355","\/alcohol\/4-b-188313","\/citrus-fruits\/4-b-188318","\/non-citrus-fruit\/4-b-188357","\/salad-dressings\/4-b-188354","\/tomatoes-tomato-sauce-ketchup\/4-b-188352","\/breads-and-pasta\/4-b-188353","\/end\/4-b-188305"]
slideshow.slideTitles = ["Our Dirty Mouths","Chips, Pretzels and Popcorn","Diet Sodas","Coffee","Alcohol","Citrus fruits","Non-Citrus Fruit","Salad Dressings ","Tomatoes, Tomato Sauce, Ketchup","Breads and Pasta","9 Foods That Trash Your Teeth"]
