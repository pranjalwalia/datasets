<script type="text/javascript">
    var show_pins = function() {
      var dh = document.height;
      $('pin_bg').setStyle({ height: dh + "px" });
      $('pin_bd').setStyle({ height: dh + "px" });
      document.body.appendChild($('pin_bg').show());
      document.body.appendChild($('pin_bd').show());
      var images = $$('.buzz_superlist_item_image');
      if (!images.size()) images.push({width: 355, height: 236});
      if (!$('pin_img_container').innerHTML && images){
        images.each(function(e){
          var sub_buzz_id;
          var id = e.id;
          if (id) {
            sub_buzz_id = id.split('superlist_')[1];
            e = e.select('img')[0];
          }
          var pin_image = document.createElement('SPAN');
          pin_image.addClassName('pin_image');
          var width = e.width;
          var height = e.height;
          var new_width  = 200;
          var new_height = 200;
          var image_margin = '';
          if (width > height) {
            new_width = e.width / (e.height / 200);
            image_margin = 'margin-left: ' + (0 - (new_width - 200) / 2) + 'px';
          } else {
            new_height = e.height / (e.width / 200);
            image_margin = 'margin-top: ' + (0 - (new_height - 200) / 2) + 'px';
          }
          var pin_url = encodeURIComponent('http://www.buzzfeed.com/stives/11-healthier-versions-of-your-favorite-dishes-8e3w' + (sub_buzz_id ? '?sub=' + sub_buzz_id : ''));
          var image_src = id ? e.readAttribute('rel:bf_image_src') : BF_STATIC.big_image_root + '/static/campaign_images/webdr02/2012/12/21/10/11-healthier-versions-of-your-favorite-dishes-1-13929-1356104246-3_big.jpg';
          var description = '11 Healthier Versions Of Your Favorite Dishes';
          if (e.alt) {
            description = e.alt;
          } else if (e.id && e.select('h1').length) {
            $A(e.select('h1')[0].childNodes).each(function(el){
                if (el.nodeName == '#text')
                    description = el.textContent.trim();
            })
          }
          pin_image.innerHTML = '<span class="pin_img"><img style="' + image_margin + '" src="' + image_src + '" width="' + new_width + 'px" height="' + new_height + 'px"></span><cite><span class="pin_mask"></span><span>' + height + 'x' + width + '</span></cite><a onclick="var sTop = window.screen.height/2-(168); var sLeft = window.screen.width/2-(313); window.open(this.href, \'sharer\',\'toolbar=0,status=0,width=626,height=336,top=\'+sTop+\',left=\'+sLeft);return false;" href="http://pinterest.com/pin/create/bookmarklet/?url='+pin_url+'&media='+ image_src + '&description=' + description + '" target="_blank"></a>';
          pin_image.select('a')[0].observe('click', function() {
            if($("pin_bg")) $("pin_bg").hide();
            if($("pin_bd")) $("pin_bd").hide();
          });
          $('pin_img_container').appendChild(pin_image);
          $('pin_bd').scrollTo(0);
        });
      }
    }
</script>
