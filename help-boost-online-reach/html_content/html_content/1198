<script type="text/javascript">
	function replaceSmallLike(el) {
		var element = $(el);
		if( ! element.hasClassName('real_btn') ) {
			element.addClassName('real_btn');
   	 	var like_xfbml = new Element('fb:like', { 'class': 'like_xfbml', 'send': 'false', 'action': 'like', 'rel:page_type': 'Buzz', 'rel:buzz_id': '1925461', 'href': 'http://www.buzzfeed.com/stives/11-healthier-versions-of-your-favorite-dishes-8e3w', 'layout': 'button_count', 'show_faces': 'false', 'width' : '110' });
			element.update('');
			element.appendChild(like_xfbml);
			FB.XFBML.parse(element);			//make sure this is last
		}
  }
</script>

<div class="facebook_like_send_small share_button" id="fb_like_small"  style="width:110px !important;">

