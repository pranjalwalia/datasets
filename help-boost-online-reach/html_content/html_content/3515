<script type="text/javascript">
function format_link()
{
	alert('test');
}
function resize_images(image)
{
	if ( image.width > 300 ) {
		image.width = 300;
		image.onclick = function() {
			window.open( image.src );
		}
		image.style.cursor = 'pointer';
		image.title = "Click to enlarge";
	}
}
//
function check_user_form()
{
	if ( document.getElementById("id_comment_body").value == "" )
	{
		alert('Please enter comment body.');
		return false;
	}
	bbstyle(-1);
	return true;
}
</script>

