function check_dublicate_username()
{
	var url = 'index.php?_ws=Users&_ms=check_user&tip_id=1174&&user_name=' + encodeURIComponent(document.getElementById("id_user_name").value);
	// notice the use of a proxy to circumvent the Same Origin Policy. 
	new Ajax.Request(url, {
		method: 'get',
		onSuccess: function(transport) {
			if (transport.responseText != '0')
			{
				alert('This user name already exists. Please choose another one.');
			}
		}
		});
}

function check_dublicate_email()
{
	var url = 'index.php?_ws=Users&_ms=check_email&tip_id=1174&&user_email=' + encodeURIComponent(document.getElementById("id_user_email").value);
	// notice the use of a proxy to circumvent the Same Origin Policy.
	new Ajax.Request(url, {
		method: 'get',
		onSuccess: function(transport) {
			if (transport.responseText != '0')
			{
				alert('This email already exists. Please choose another one.');
			}
		}
		});
}

function ma_sleep(naptime)
{
	naptime = naptime * 1000;
	var sleeping = true;
	var now = new Date();
	var alarm;
	var startingMSeconds = now.getTime();
	//alert("starting nap at timestamp: " + startingMSeconds + "\nWill sleep for: " + naptime + " ms");
	while(sleeping)
	{
		alarm = new Date();
		alarmMSeconds = alarm.getTime();
		if(alarmMSeconds - startingMSeconds > naptime){ sleeping = false; }
	}
}
//
function alreadyvotted_rate_the_tip(tip_id)
{
	document.getElementById('alreadyvotted_rate_the_tip_' + tip_id).style.display = 'inline';
	document.getElementById('rate_the_tip_' + tip_id).style.display = 'none';
}
//
function notloggedin_rate_the_tip(tip_id)
{
	document.getElementById('notloggedin_rate_the_tip_' + tip_id).style.display = 'inline';
}
//
function rate_the_tip(tip_id, up)
{
	var url = 'index.php?_ws=Tips&_ms=rate&&tip_id=' + encodeURIComponent(tip_id) + '&up=' + encodeURIComponent(up);
	// notice the use of a proxy to circumvent the Same Origin Policy. 
	new Ajax.Request(url, {
		method: 'get',
		onSuccess: function(transport) {
			if (transport.responseText == '0') {
				alert('You have to be logged in to do that.');
			} else if (transport.responseText == '2') {
				//alert('You cannot vote twice for the same tip.');
				alreadyvotted_rate_the_tip(tip_id)
			} else if (transport.responseText == '1') {
				//alert('Your vote has been saved. Thank you.');
				document.getElementById('alreadyvotted_rate_the_tip_' + tip_id).style.display = 'none';
				document.getElementById('rate_the_tip_' + tip_id).style.display = 'inline';
				document.getElementById('rating_text_tip_' + tip_id).innerHTML = 'Thank you for your vote.';
				reload_ratings(tip_id, up);
			}
		}
		});
}
//
function reload_ratings(tip_id, up)
{
	var url = 'index.php?_ws=Tips&_ms=get_rating&&tip_id=' + encodeURIComponent(tip_id) + '&up=' + encodeURIComponent(up);
	// notice the use of a proxy to circumvent the Same Origin Policy. 
	new Ajax.Request(url, {
		method: 'get',
		onSuccess: function(transport) {
			if ( up == '1')
			{
				document.getElementById('rating_up_tip_'+tip_id).innerHTML = transport.responseText;
			} else {
				document.getElementById('rating_down_tip_'+tip_id).innerHTML = transport.responseText;
			}
		}
		});	
}
//
function onClickSortBy(e)
{
	window.location = "/?_ws=Tips&_ms=set_sort_by&tip_id=1174&&sort_by=" + e.value;
}

function popUp(URL,width,height) {
day = new Date();
id = day.getTime();
eval("page" + id + " = window.open(URL, '" + id + "', 'toolbar=0,scrollbars=0,location=0,statusbar=0,menubar=0,resizable=0,width="+width+",height="+height+",left = 262,top = 184');");
}

