var timeObj        = new Date;
var unixTimeMs   = timeObj.getTime();
var unixTime      = parseInt(unixTimeMs / 1000);
var orderID        = pageTracker._visitCode() + '-' + unixTime;

pageTracker._addTrans(
      orderID, // required
      "",
      "0.01",
      "",
      "",
      "",
      "",
      ""
); 

