<script type="text/javascript">//<![CDATA[
var HeaderEdition = Class.create();
HeaderEdition.prototype = {
initialize: function(name, hdrPrompt, hdrEditions, hdrNav, omnitureEdition, subNav) {
this.name = name;
this.hdrPrompt = (hdrPrompt == '') ? $('cnn_hdr-prompt').innerHTML : hdrPrompt;
this.hdrEditions = (hdrEditions == '') ? $('hdr-editions').innerHTML : hdrEditions;
this.hdrNav = (hdrNav == '') ? $('cnn-hdr-nav').innerHTML : hdrNav;
this.ftrNav = this.ftrBottom = '';
this.bnBanner = '';
this.omnitureEdition = omnitureEdition;
if($('ir_subnav_cont')) {
this.subNav = (subNav == '') ? $('ir_subnav_cont').innerHTML : subNav;
}
},
setSubNav:function(subNav){
},
setFooter: function(ftrNav, ftrBottom, ftrLinkBoxItems) {
this.ftrNav = (ftrNav == '') ? $('ftr-nav').innerHTML : ftrNav;
this.ftrBottom = (ftrBottom == '') ? $('ftr-bottom').innerHTML : ftrBottom;
this.ftrLinkBoxItems = (ftrLinkBoxItems=='')?$('ftr-ir-links').innerHTML:ftrLinkBoxItems;
},
setBreakingNews: function(bnBanner) {
this.bnBanner = bnBanner;
if (currentHeader == this)
this.showOrHideBNBanner();
},
show: function() {
if (currentHeader.name == 'cnnMx') return; // locked in: don't switch away
if (this.name != currentHeader.name) {
$('cnn_hdr-prompt').innerHTML = this.hdrPrompt;
$('hdr-editions').innerHTML = this.hdrEditions;
$('cnn-hdr-nav').innerHTML = this.hdrNav;
$('ftr-nav').innerHTML = this.ftrNav;
$('ftr-bottom').innerHTML = this.ftrBottom;
if($('ir_subnav_cont'))$('ir_subnav_cont').innerHTML = this.subNav;
$('ftr-ir-links').innerHTML = this.ftrLinkBoxItems;
this.showOrHideBNBanner();
currentHeader = this;
cnnIrptEd = currentHeader.getOmnitureEdition();
}
},
reRender:function(){
$('cnn_hdr-prompt').innerHTML = this.hdrPrompt;
$('hdr-editions').innerHTML = this.hdrEditions;
$('cnn-hdr-nav').innerHTML = this.hdrNav;
$('ftr-nav').innerHTML = this.ftrNav;
$('ftr-bottom').innerHTML = this.ftrBottom;
if($('ir_subnav_cont'))$('ir_subnav_cont').innerHTML = this.subNav;
$('ftr-ir-links').innerHTML = this.ftrLinkBoxItems;
this.showOrHideBNBanner();
},
showOrHideBNBanner: function() {
var div = $('cnnBannerContainer');
if (this.bnBanner == '' || this.bnBanner.substring(0,4) == ("<" + "!--"))
div.hide();
else
div.show();
div.innerHTML = this.bnBanner;
},
cookieName: "SelectedEdition",
cookieDomain: /cnn\.com/.test(ir_loginToUrlBase) ? 'cnn.com' : null,
getCookieValue: function() {
return Cookies.getCookie(this.cookieName);
},
setCookieValue: function(newValue) {
var oldValue = this.getCookieValue();
if (newValue != oldValue)
Cookies.setCookie(this.cookieName, newValue, 365*100, '/', this.cookieDomain);
},
deleteCookieValue: function() {
var oldValue = this.getCookieValue();
if (oldValue != null)
Cookies.setCookie(this.cookieName, "Deleted", -1, '/', this.cookieDomain);
},
switchTo: function() {
this.setCookieValue(this.name);
$('cnn_hdr-prompt').hide();
this.show();
},
switchToAndSetCNNSelectedEdition: function() {
this.switchTo();
},
switchToIfCookieSet: function() {
var cookieValue = this.getCookieValue();
if (cookieValue == this.name)
this.switchTo();
else if (cookieValue == null)
$('cnn_hdr-prompt').show();
},
promptIfNoCookie: function() {
if (this.getCookieValue() == null)
$('cnn_hdr-prompt').show();
else
$('cnn_hdr-prompt').hide();
},
switchToAndClearCookie: function() {
this.deleteCookieValue();
$('cnn_hdr-prompt').show();
this.show();
},
syncHeaderToCNNSelectedEdition: function(editionChoice) {
var cookieValue = this.getCookieValue();
if (editionChoice == 'www') {
if (cookieValue != 'www')
usHeader.switchTo();
} else if (editionChoice == 'edition') {
if (cookieValue != 'edition')
intlHeader.switchTo();
}
else if (editionChoice == 'mexico' || editionChoice == 'cnnMx') {
if (cookieValue != 'mexico' || cookieValue != 'cnnMx')
cnnMxHeader.switchTo();
}
else {
if (cookieValue != null)
usHeader.switchToAndClearCookie();
}
},
syncCookieToCNNSelectedEdition: function(editionChoice) {
var cookieValue = this.getCookieValue();
if (editionChoice == 'www' || editionChoice == 'edition')
this.setCookieValue(editionChoice);
else
this.deleteCookieValue();
this.promptIfNoCookie();
},
goTo: function() {
this.setCookieValue(this.name);
return true;
},
getUrlToSetCNNSelectedEdition: function() {
return 'http://us.cnn.com/ireport/setEdition.html?SelectedEdition=' + encodeURIComponent(this.name);
},
getOmnitureEdition: function() {
return this.omnitureEdition;
}
};
var usHeader = new HeaderEdition('www', '', '', '', 'cnn2global', '');
