(function() {
var referrer = document.referrer.toLowerCase();
var followingReferrer = true;
if (referrer.substr(0,19) == 'http://www.cnn.com/' ||
referrer.substr(0,18) == 'http://us.cnn.com/')
usHeader.show();
else if (referrer.substr(0,23) == 'http://edition.cnn.com/')
intlHeader.show();
else {
followingReferrer = false;
intlHeader.switchToIfCookieSet();
}
currentHeader.promptIfNoCookie();
