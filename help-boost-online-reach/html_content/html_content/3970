    <script type="text/javascript">
        var ir_ssoUiUrl = "http://audience.cnn.com/services/cnn/"
        var ir_loginToUrlBase = "http://ireport.cnn.com";
        
        var ir_overlaysShowing = $H();
        function irShowOverlay(id,show) {
            (show?Element.show:Element.hide)($(id));
            if (show) {
                centerDivOfPage(id);
                Event.observe(window,'resize',irRecenterOverlaysOnResize);
                Event.observe(window,'orientationchange',irRecenterOverlaysOnResize);
            }
            
            if (show)
                ir_overlaysShowing.set(id,$(id));
            else
                ir_overlaysShowing.unset(id,$(id));
            var anyShowing = ir_overlaysShowing.keys().length > 0;
            
            var useHTML5Player = /\b(iPad|iPod|iPhone)\b/.test(navigator.userAgent);
            if (useHTML5Player)
                $$('.htmlplayer, video, audio').each(anyShowing?Element.hide:Element.show);
            
            $$('#ir-sidebar-ad div, .ad-336-280 div, .ad-970-66 div, .ir-entitlement-ad-content div').each(anyShowing?Element.hide:Element.show);
        }
        
        var ir_overlaysRecenterTimeoutId;
        function irRecenterOverlaysOnResize() {
            window.clearTimeout(ir_overlaysRecenterTimeoutId);
            ir_overlaysRecenterTimeoutId = window.setTimeout(irRecenterOverlays,100);
        }
        function irRecenterOverlays() {
            ir_overlaysShowing.each(function(pair) { centerDivOfPage(pair.value); });
        }
        
        // Moved from element-js-2.0-connect-connect-queueConfig.js.
        function centerDivOfPage(divId) {
            var div = $(divId);
            
            // ----------------------- skin ad fix --------------------------------
            // IREPORT-2265  Fix for skin ad display side impact
            // When grey background width is adjusted to 1000px in order to display skin ad tag correctly,
            // upload overlay is no longer centered
            
            //var winVisWidth  = window.innerWidth  || document.documentElement.clientWidth;
            var winVisWidth  = jQuery(div).parent().width();
            
            // depend on upload window's parent width in stead of browser's width.
            // this way upload window will be centered as per the width of grey background
            // ----------------------- skin ad fix --------------------------------

            var winVisHeight = window.innerHeight || document.documentElement.clientHeight;
            var winX = window.pageXOffset || document.documentElement.scrollLeft;
            var winY = window.pageYOffset || document.documentElement.scrollTop;
            var divLeft = Math.floor(((winVisWidth  - div.offsetWidth)  / 2) + winX);
            var divTop  = Math.floor(((winVisHeight - div.offsetHeight) / 2) + winY);
            divTop = (divTop <= 110) ? 110 : divTop;
            divLeft = (divLeft <= 9) ?  10 : divLeft;
            div.style.left = divLeft + 'px';
            div.style.top  = divTop  + 'px';
        }
    </script>
