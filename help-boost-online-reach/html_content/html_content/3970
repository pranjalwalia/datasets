function irRegisterShowThanks() {
jQuery("#irRegisterShowThanks").load("/services/register-thanks.jspa",
function(){
irShowOverlay("irRegisterShowThanks",true);
});
}
if (Cookies.getCookie("irRegisterShowThanks") != null) {
Cookies.setCookie("irRegisterShowThanks","",-1,"/");
irRegisterShowThanks();
}
</script>
