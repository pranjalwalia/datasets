var irGetBanners_Called = false;
function irGetBanners(event) {
if (irGetBanners_Called) return;
irGetBanners_Called = true;
var url = "/services/banner-list.jspa";
new Ajax.Request(url, { method: 'get' });
}
Event.observe(document, "dom:loaded", irGetBanners);
Event.observe(window, "load", irGetBanners); // for IE6
function ir_clsbnbnnr(bannerTimestamp)
{
currentHeader.setBreakingNews("<!--Breaking news closed-->");
// Insert session cookie with timestamp to ensure that Banner isn't shown again until next updated
var cookieDomain = /cnn\.com/.test(ir_loginToUrlBase) ? 'cnn.com' : 'ireport.com';
Cookies.setCookie("bannerLastClosed", bannerTimestamp, null, '/', cookieDomain);
}
