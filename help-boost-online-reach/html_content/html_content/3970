    <script type="text/javascript">//<![CDATA[
        var irCeKey = "DOC-929213";
        var irCommentPonder = 1900895;
        var irCommentPonderStep = 24;
        var irCommentLast = 0;
        
        function irUpdateComments(start,msg) {
            //$("ir_inline_thread").innerHTML = "Loading comments&hellip;";
            var isInternetExplorer = navigator.appName.indexOf("Microsoft") != -1;
            var rh = '';
            if(isInternetExplorer)
                var rh = { 'If-Modified-Since': 'Thu, 1 Jan 1970 00:00:00 GMT' };
            irCommentLast = msg;
            var params = {
                irCeKey: irCeKey,
                start: start
                };
            if (msg > 0) {
                params.message = msg;
            }
            new Ajax.Updater("ir_inline_thread","/services/ir-inline-thread.jspa",{
                method: "get",
                parameters: params,
                requestHeaders: rh,
                //TODO: Use onSuccess when upgrading to Prototype 1.6.
                onComplete:
                    function() {
                        var elem = $("ir_src_comment_count");
                        if (elem != null) {
                            $("ir_comment_count").innerHTML = "(" + elem.innerHTML + ")";
                            new Effect.Highlight("ir_c_m" + irCommentLast,{ duration: 1.5 });
                        }
                    },
                onFailure: function() { $("ir_inline_thread").innerHTML = "Failed to load comments."; }
                });
        }
        
        function irResetCommentForm() {
            $("post_comment_status").innerHTML = "";
            $("ir_comment_error").innerHTML = "";

           ($("post_comment_facebook")||[]).disabled=false;
            $("post_comment_btn").style.display="inline"
        }
        
        function irCommentPost() {         
            var postComment = $("post_comment").value;
            if (postComment == null || postComment == "") {
                $('ir_comment_error').innerHTML = "Please enter some text for a comment.";
                return;
            }
            
            // [IREPORT-2506] Warn user and prevent submissions when comments contain links with strings related to hyperlinks, URLs
            var lowerCaseComment = postComment.toLowerCase();
            if(lowerCaseComment.indexOf('href') !== -1 || lowerCaseComment.indexOf('www.') !== -1 || lowerCaseComment.indexOf('http') !== -1 || lowerCaseComment.indexOf('ftp') !== -1) {
            $('ir_comment_error').innerHTML = "To prevent spam, comments may not contain URLs or hyperlinks.";
                return;
            }
            
            $("post_comment_btn").style.display="none";
           ($("post_comment_facebook")||[]).disabled=true;
            
            $("post_comment_status").innerHTML =
                "<img src=\"/images/jive-icon-working-16x16.gif\" alt=\"\"/> <span style=\"vertical-align: top;\">Posting comment...</span>";
            
            // [IREPORT-2566] Publish comment to Facebook Open Graph
            // Since posting to Facebook can be disabled by a system property, set the default value to false if it doesn't exist.
            var postToFacebook = "false";
            var ajaxParams = {
                key: irCeKey,
                rawFeed: "true",
                post: "true",
                comment: postComment
            };
            
            if ( $('post_comment_facebook') !== null ) {
                // An empty checkbox will return null, so if it's not set, keep it false.
                postToFacebook = $('post_comment_facebook').getValue() || "false";
                ajaxParams['postToFacebook'] = postToFacebook;
            }

            new Ajax.Request("/services/ir-inline-thread-post.jspa",{
                parameters: ajaxParams,
                requestHeaders: { Accept: "text/javascript" },
                onSuccess:
                    function(transport) {
                        var obj = transport.responseText.evalJSON(true);
                        if (obj != null && obj.messageId != null) {
                            irUpdateComments(0,obj.messageId);
                            Element.scrollTo("cf");
                            $("post_comment").value = "";
                        } else {
                            window.alert("Unable to post comment (server error).  Please try again later.");
                        }
                        irResetCommentForm();
                    },
                onFailure:
                    function() {
                        window.alert("Unable to post comment (server error).  Please try again later.");
                        irResetCommentForm();
                    }
                });
        }
               
        function commentAbuseSubmitted(messageid) {
            new Ajax.Request("/services/message-abuse-post-execute.jspa",{
                method: "get",
                parameters: { objectID: messageid, objectType: "2" },
                //TODO: Use onSuccess when upgrading to Prototype 1.6.
                onComplete:
                    function(data) {
                        var obj = eval( '(' + data.responseText + ')' );
                        var value1 = obj.displayMessage;
                        document.getElementById('ircommentabusearea'+messageid).innerHTML = "<span>"+value1+"<\/span>";  
                    },
                onFailure: function() {
                        document.getElementById('ircommentabusearea'+messageid).innerHTML = "Failed to report violation on comment.";  
                    }
                });
                
                //SO THAT FORM wont submit
                return false;
        }

        function hideComment(commentID) {
            $('hide-' + commentID).hide();
            $('hide-working-' + commentID).show();
            new Ajax.Request("/mod/people-comments-updates!hideComment.jspa",{
              method: 'post',
              parameters: { commentID: commentID },
                onSuccess:
                  function(data) {
                    var obj = data.responseText.evalJSON(true);
                    var message = obj.message;
                    if (message == 'success') {
                      $('hide-working-' + commentID).hide();
                      $('show-' + commentID).show();
                      $('line-' + commentID).addClassName('ir-mod-comment-hide');
                    } else {
                      alert(message);
                    }
                  },
                onFailure:
                  function(data) {
                      alert('Error, responseText: ' + data.responseText);
                  }
            });
        }
        function showComment(commentID) {
            $('show-' + commentID).hide();
            $('show-working-' + commentID).show();
            new Ajax.Request("/mod/people-comments-updates!showComment.jspa",{
              method: 'post',
              parameters: { commentID: commentID },
                onSuccess:
                  function(data) {
                    var obj = data.responseText.evalJSON(true);
                    var message = obj.message;
                    if (message == 'success') {
                      $('show-working-' + commentID).hide();
                      $('hide-' + commentID).show();
                      $('line-' + commentID).removeClassName('ir-mod-comment-hide');
                    } else {
                      alert(message);
                    }
                  },
                onFailure:
                  function(data) {
                      alert('Error, responseText: ' + data.responseText);
                  }
            });
        }
    //]]></script>

    <!--<script type="text/javascript" src="http://i.cdn.turner.com/cnn/.element/js/3.0/jquery-1.4.2.min.js"></script> -->

    <script type="text/javascript">//<![CDATA[
        function getDefaultOverlayImageURL(size, type) {
            size = size || "640";
            type=type||"default";
            if(size=='896') {
                if(type!='audio')return "/themes/custom/resources/redesign/overlay_noaudio_896x504.png"; 
                else return "/themes/custom/resources/redesign/overlay_audio_896x504.png" 
            }
            else {
                if(type!='audio')return "/themes/custom/resources/redesign/overlay_noaudio_640x360.png";
                else return "/themes/custom/resources/redesign/overlay_audio_640x360.png"; 
            }
        }
    //]]></script>
        
    <script type="text/javascript">//<![CDATA[
    
        var videoId = null;
        var cvpplayer = null;
        var isNavigatedVideo = false;
        var ifPlay = true;
        var contentPlaying = false;
        var isPrevMediaVideo = false;        
                
        var mediaImageArray = new Array();
        var mediaTypeArray = new Array();
        var currentDisplay = 0;

        var overlay;
        var overlayBox;
        var userID = 1169251;
        
        function sendFeedback(state) {
            if (state == "INAPPROPRIATE") {
                jsmd.trackMetrics("ireport-flag-story");
            }
            new Ajax.Request("/services/feedback.jspa",{
                method: "post",
                parameters: {
                    id: "929213",
                    state: state,
                    "jive.token.name": "ir.feedback.doc929213",
                    "ir.feedback.doc929213": "OY51KAWLIV78Y4Z6TCZ08YRIBYJV97LE",
                    rawFeed: "true"
                },
                onSuccess: function(data) {
                    var obj = data.responseText.evalJSON(true);
                    if (obj.status == "error") {
                        $('story-feedback-status').innerHTML = obj.message;
                        $('story-feedback-status').show();
                    } else {
                        $('story-feedback-status').innerHTML =
                            "Feedback submitted successfully!";
                        $('story-feedback-status').show();
                    }
                },
                onFailure: function(data) {
                    var obj = data.responseText.evalJSON(true);
                    $('story-feedback-status').innerHTML = "Failed to send feedback: " + obj.message;
                    //$('story-feedback-status').show();
                    jquery('#story-feedback-status').show(100, function() { 
                        jquery('#story-feedback-status').hide('slow');
                    });
                }
            });
        }
        
        function startWatching() {
            Watches.watchUser(userID, {
                callback:function() {
                    jQuery("#follow1").hide();
                    jQuery("#follow2").show();
                    Jive.AlertMessage('thread.watch.notify', {
                        beforeStart:function() {
                            $('thread.watch.notify').innerHTML = '<div>You are receiving e-mail notifications for this user. To unsubscribe, go to your profile and click "Following".</div>';
                        }
                    });
                }
            });
        }
        function hideItem(item){
            if(item&&$(item))$(item).hide();
        }
        function showItem(item){
            if(item&&$(item))$(item).show();
        }
        function stopVideoPlayer(){
                if(cvpplayer) { 
                    try {
                        cvpplayer.pause();
                        cvpplayer.stop();
                    }catch(err){}
                }     
        
        }
        var addLoad = true;
        function cloneImageAttributes(orig, newItem){
            newItem = jQuery(newItem);
            orig = jQuery(orig);
            var attributes = ["id", "title", "class", "alt"];
            jQuery.each(attributes, function(index, value){
                newItem.attr(value, orig.attr(value));
            });
            return newItem;
        }
        function setStoryImage(url){
            jQuery("#story-image").addClass("offscreen");
            
            //jQuery("#story-image").attr("src","");
            var newImg = jQuery("<img>");
            
            var currImg = jQuery("#story-image");
            newImg = cloneImageAttributes(currImg, newImg);
            var parentContainer = currImg.parent();
            
            
            if(addLoad) {
                newImg.load(function() {
                    var aspect = (this.width/this.height);   
                    if(aspect>1.77) {
                        var newWidth = 896;
                        var hewHeight = 896/aspect;
                        jQuery(this).attr('style', "width:" + newWidth + "px !important; height:" + hewHeight + 'px !important;');
                        jQuery(this).css('margin-top', (504/2 - hewHeight/2) + 'px');
                     }
                     else {
                        jQuery(this).attr('style', '');
                        var hScale = 504/this.height;
                        if(hScale!=1) {
                            this.width = this.width*hScale;
                            this.height = this.height*hScale;
                        }
                        jQuery(this).css('margin-left', (896/2 - this.width/2) + 'px');
                     
                     }
                    jQuery(this).removeClass("offscreen");
                    if(this.width<896 || this.height<504) {
                        addBackgroundImage(this.src.replace(currentImageExt,"_896_ghst") );
                        showItem('background-image');
                    } else { 
                       hideItem('background-image');
                    }
                    newImg.css("opacity", ".1");
                        if(currImg)currImg.remove();
                        parentContainer.append(newImg);
                        newImg.animate({opacity:1},250);

                });
                newImg.error(function() { 
                    if(this.src.indexOf("_lg")<0) {
                        currentImageExt = "_lg";
                        setStoryImage(this.src.replace("_p9","_lg"));
                        
                        addBackgroundImage("http://ireport.cnn.com" + getDefaultOverlayImageURL('896') );
                        showItem('background-image');
                    }
                    
                });
            }
            newImg.attr("src", url);
            jQuery("#story-image").attr("src",url);            
        
        }
        var addBackgroundLoad = true;
        function addBackgroundImage(expImage){
            var imageToDisplay; 
            //jQuery("#background-image").attr("src","");
            if (mediaTypeArray.size()>0 && mediaTypeArray[currentDisplay] != 'image' && mediaTypeArray[currentDisplay] != 'video') {
                 return;
            }
            if(expImage==null||expImage=='undefined') {
                imageToDisplay = mediaImageArray[currentDisplay];
            }
            else {
                imageToDisplay = expImage;
            }
            if(addBackgroundLoad) {
                jQuery("#background-image").error(function() {
                    //var newVal = this.src.replace("_896_ghst","_p1");
                    //http://ireport.cnn.com
                    var newVal = "http://ireport.cnn.com" + getDefaultOverlayImageURL('896');
                    if(newVal != this.src) {
                        addBackgroundImage(newVal);
                    }
                    
              })
                addBackgroundLoad = false;
            }            
            jQuery("#background-image").attr("src",imageToDisplay);
            
        }
        function showNext(currentSelect, id, type, imageUrl) {
            currentDisplay = currentSelect;
            trackingMediaType = type;
            $('current-select').innerHTML = "<b>" + (currentSelect+1) + "</b>";
            
            if (isPrevMediaVideo) {
                //showItem('story-image'); 
                showItem("story-media-container");
                setStoryImage(imageUrl);
                
                stopVideoPlayer();
                
                hideItem('video-cont');
                hideItem('play-button');
            }
            ifPlay = false;
            
            if (type == "video" || type == "audio") {
                setStoryImage(imageUrl);
                isNavigatedVideo = true;
                videoId = id;
//                hideItem('vett');               
                showItem('vett');               
                showItem('story-video');                
                showItem('play-button');                

            } else if (type == "image") {
                isPrevMediaVideo = false;
                //showItem('story-image');
                showItem("story-media-container");
                setStoryImage(imageUrl);                
                stopVideoPlayer();

                hideItem('video-cont');
                hideItem('play-button');
                showItem('vett');

            }
            updateNavButton(currentSelect);
        }

        function goLeft() {
            if (mediaImageArray != null && mediaImageArray.size() > 0 && currentDisplay != 0) {

                jQuery("#nav-btn-right").addClass('right-nav-btn-active');
                if (isPrevMediaVideo) {
                    //showItem('story-image');                
                    showItem("story-media-container");
                    stopVideoPlayer();
                    hideItem('video-cont');
                    hideItem('play-button');
                }

                ifPlay = false;
                setStoryImage(mediaImageArray[currentDisplay-1]);
                showItem('play-button');
                
                //hideItem('vett');
                showItem('vett');
                
                trackingMediaType = mediaTypeArray[currentDisplay-1];
                if (mediaTypeArray[currentDisplay-1] == "image") {
                    isPrevMediaVideo = false;
                    //showItem('story-image');
                    showItem("story-media-container");
                    hideItem('play-button');                    

                    stopVideoPlayer();
                    hideItem('video-cont');
                    showItem('vett');

                } 
                currentDisplay = currentDisplay - 1;
                isNavigatedVideo = true;
                videoId = "929213/" + currentDisplay;
                $('current-select').innerHTML = "<b>" + (currentDisplay+1) + "</b>";
                updateNavButton(currentDisplay);
            }
        }

        function goRight() {
            if (mediaImageArray != null && mediaImageArray.size() > 0 && currentDisplay < (mediaImageArray.size() - 1)) {

                jQuery("#nav-btn-left").addClass('left-nav-btn-active');
                if (isPrevMediaVideo) {
                stopVideoPlayer();
                hideItem('video-cont');
                hideItem('play-button');
                //showItem('story-image');
                showItem("story-media-container");                
            }

                ifPlay = false;
                setStoryImage(mediaImageArray[currentDisplay+1]);
                showItem('play-button');
                //hideItem('vett');
                showItem('vett');

                trackingMediaType = mediaTypeArray[currentDisplay+1];
                if (mediaTypeArray[currentDisplay+1] == "image") {
                    isPrevMediaVideo = false;
                    
                    //showItem('story-image');
                    showItem("story-media-container");
                    hideItem('play-button');                    

                    stopVideoPlayer();
                    hideItem('video-cont');
                    showItem('vett');
                }
                currentDisplay = currentDisplay + 1;
                isNavigatedVideo = true;
                videoId = "929213/" + currentDisplay;
                $('current-select').innerHTML = "<b>" + (currentDisplay+1) + "</b>";
                updateNavButton(currentDisplay);
            }
        }
        
        function playVideo(id) {
            hideItem('vett');
            hideItem("background-image");
            hideItem('play-button');        
            //hideItem('story-image');        
            hideItem("story-media-container");
            showItem('video-cont');
            if (!isNavigatedVideo) { 
                videoId = id;
            }
            isPrevMediaVideo = true;
            ifPlay = true;
            if (cvpplayer == null) {
                cvpplayer = playThis(videoId);
            } else {
                cvpplayer.play(videoId);
            }

        }
        
        function playThis(id)  {
        
           var fiftySent = false;
           var cvp = new CVP( {
                id : 'cvp_video',
                width : '896px',
                height : '504px',
                flashVars : {
                    context : 'fullsize', 
                    autostart : 'true',
                    site : 'ireport', 
                    profile : 'ireport-site'
                },
                embed : {
                
                    containerSwf: '/themes/custom/resources/cvplayer2.0/ireport_video_player.swf',
                    //containerSwf : '/themes/custom/resources/cvplayer2.0/IReportMainContainer.swf',
                    //containerSwf : '/themes/custom/resources/cvplayer2.0/cvp_main_container.swf',
                    expressInstallSwf : 'http://i.cdn.turner.com/xslo/cvp/assets/flash/expressInstall.swf',
                    options : {
                        quality : 'high',
                        bgcolor : '#000000',
                        allowFullScreen : 'true', 
                        allowScriptAccess : 'always',    
                        wmode:'opaque'                
                    }
                },
                onCVPReady : function(contentId) {
                    if (ifPlay) { this.play(videoId); }
                },
                onContentBegin: function() {
                    fiftySent = false;     
                },
                onTrackingContentPlay : function(playerId, dataObj) { 
                    // In the case of audio, use trackMetrics() to pass the title
                    if (trackingMediaType == "audio") {
                        trackMetrics( "audio-start","Mike Dillard Featured in Aspire Magazine" );
                    } else {
                        sendVideoEvent(this.getContentEntry(dataObj.contentId), trackingMediaType + "-start");
                    } 
                },  
                onTrackingContentComplete : function(playerId, dataObj) {
                    contentPlaying = false;
                    sendVideoEvent(this.getContentEntry(dataObj.contentId), trackingMediaType + "-complete"); 
                }, 
                onTrackingContentReplay : function(playerId, dataObj) {
                    sendVideoEvent(this.getContentEntry(dataObj.contentId), trackingMediaType + "-restart"); 
                },
                onTrackingContentProgress : function(playerId, dataObj) {
                    var percent = dataObj.percent?dataObj.percent:dataObj.precent;

                    // not required as of now 11/1/2011
                    //if (percent >= 25 && percent < 50) {
                    //    sendVideoEvent(this.getContentEntry(dataObj.contentId), trackingMediaType + "-twentyfive"); 
                    //}
                    
                    if (percent >= 50 && percent < 65 && !fiftySent) {
                        sendVideoEvent(this.getContentEntry(dataObj.contentId), trackingMediaType + "-fifty"); 
                        fiftySent = true;
                    }
                    
                    // not required as of now 11/1/2011
                    //if (percent >= 75 && percent < 100) {
                    //    sendVideoEvent(this.getContentEntry(dataObj.contentId), trackingMediaType + "-seventyfive");
                    //} 
                },
                onTrackingContentPaused : function(playerId, dataObj) {
                    sendVideoEvent(this.getContentEntry(dataObj.contentId), trackingMediaType + "-pause"); 
                },
                onTrackingContentSeek : function(playerId, dataObj) {
                    sendVideoEvent(this.getContentEntry(dataObj.contentId), trackingMediaType + "-seek"); 
                },
                onTrackingContentFullScreen : function(playerId, dataObj) {
                    sendVideoEvent(this.getContentEntry(dataObj.contentId), trackingMediaType + "-fullscreen"); 
                }
            });
            cvp.embedSWF("story-video");
            return cvp;        
        }

        function updateNavButton(idx) {

            for (var i=0; i < mediaTypeArray.size(); i++) {
                if (i == idx ) {
                    $('nav-btn-'+i).src = "http://ireport.cnn.com/themes/custom/resources/redesign/11x11_dot_nav_over.png";
                } else {
                    $('nav-btn-'+i).src = "http://ireport.cnn.com/themes/custom/resources/redesign/11x11_dot_nav_up.png";
                }
            }

            if (idx == 0) {
                jQuery("#nav-btn-left").removeClass('left-nav-btn-active');
                jQuery("#nav-btn-left").addClass('left-nav-btn-inactive');
            } else {
                jQuery("#nav-btn-left").addClass('left-nav-btn-active');
            }                

            if (idx == (mediaImageArray.size() - 1)) {
                jQuery("#nav-btn-right").removeClass('right-nav-btn-active');
                jQuery("#nav-btn-right").addClass('right-nav-btn-inactive');
            } else {
                jQuery("#nav-btn-right").addClass('right-nav-btn-active');
            }                
        }
        
        function displayDiscretionOverlay() {
        
            overlayShowCommon();
            overlayBox.append("<br/><br/><img src='http://ireport.cnn.com/themes/custom/resources/redesign/overlays_3_03.jpg'/><br/><br/>");
            overlayBox.append("<img src='http://ireport.cnn.com/themes/custom/resources/redesign/overlays_3_07.jpg'/><br/><br/>");
            overlayBox.append("<img src='http://ireport.cnn.com/themes/custom/resources/redesign/overlays_1_13.jpg'/>&nbsp;&nbsp;&nbsp;");
            overlayBox.append("<span id='story-overlay-yes'><a href='#' onclick='confirmDiscretionOverlay(); return false;'><img width='1' height='1' title='Yes, I want to.' alt='Yes' src='http://i.cdn.turner.com/cnn/.element/img/3.0/1px.gif'/></a></span>&nbsp;&nbsp;");
            overlayBox.append("<span id='story-overlay-no'><a href='#' onclick='exitOverlay(); return false;'><img width='1' height='1' title='No, I do not.' alt='No' src='http://i.cdn.turner.com/cnn/.element/img/3.0/1px.gif'/></a></span><br/>");
        } 


