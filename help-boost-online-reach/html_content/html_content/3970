        } 

        function overlayShowCommon() {

            overlay = jQuery("#story-overlay");
            overlay.show();
            overlayBox = jQuery("#story-overlay-box");
            overlayBox.show();
            overlayBox.css("padding", "10px");
            overlayBox.html("");
        }
        
        function overlayHideCommon() {
            overlayBox.hide();
            overlay.hide();
        }
                        
        function confirmDiscretionOverlay() {
            jQuery("#discretion").addClass('offscreen');
            overlayHideCommon();
        }
        
        function confirmReviewOverlay() {
            jQuery("#underReview").addClass('offscreen');
            overlayHideCommon();
        }

        function confirmDiscretionReviewOverlay() {
            jQuery("#discretion").addClass('offscreen');
            jQuery("#underReview").addClass('offscreen');
            overlayHideCommon();
        }
        
        function exitOverlay() {
            window.history.back();
        }

        /* Google API call to generate asynchronous +1 button */
        (function() {
            var po = document.createElement('script'); po.type = 'text/javascript'; po.async = true;
            po.src = 'https://apis.google.com/js/plusone.js';
            var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(po, s);
        })();

